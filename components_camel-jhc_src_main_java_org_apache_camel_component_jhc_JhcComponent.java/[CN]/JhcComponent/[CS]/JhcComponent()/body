{
  setHeaderFilterStrategy(new JhcHeaderFilterStrategy());
  params=new BasicHttpParams().setIntParameter(HttpConnectionParams.SO_TIMEOUT,5000).setIntParameter(HttpConnectionParams.CONNECTION_TIMEOUT,10000).setIntParameter(HttpConnectionParams.SOCKET_BUFFER_SIZE,8 * 1024).setBooleanParameter(HttpConnectionParams.STALE_CONNECTION_CHECK,false).setBooleanParameter(HttpConnectionParams.TCP_NODELAY,true).setParameter(HttpProtocolParams.USER_AGENT,"Camel-JhcComponent/1.1");
}
