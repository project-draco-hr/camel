{
synchronized (routers) {
    MethodBasedRouter result=routers.get(uriPattern);
    if (result == null) {
      result=new MethodBasedRouter(uriPattern);
      if (LOG.isDebugEnabled()) {
        LOG.debug("Added method based router: " + result);
      }
      routers.put(uriPattern,result);
    }
    return result;
  }
}
