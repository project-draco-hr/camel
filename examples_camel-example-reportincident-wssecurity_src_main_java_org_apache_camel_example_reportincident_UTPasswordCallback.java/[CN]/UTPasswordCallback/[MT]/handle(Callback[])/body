{
  String user="";
  for (  Callback callback : callbacks) {
    WSPasswordCallback pc=(WSPasswordCallback)callback;
    user=pc.getIdentifier();
    String pass=passwords.get(user);
    if (pass != null) {
      pc.setPassword(pass);
      return;
    }
  }
  throw new IOException("Password does not exist for the user : " + user);
}
