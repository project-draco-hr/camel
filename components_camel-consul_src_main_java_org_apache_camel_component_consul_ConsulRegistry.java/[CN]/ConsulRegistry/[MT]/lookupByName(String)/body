{
  key=key.replaceAll("\\$","/");
  kvClient=consul.keyValueClient();
  Optional<String> result=kvClient.getValueAsString(key);
  if (result.isPresent()) {
    byte[] postDecodedValue=Base64.decodeBase64(result.get());
    return SerializationUtils.deserialize(postDecodedValue);
  }
  return null;
}
