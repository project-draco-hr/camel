{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("atom:http://localhost:" + JettyTestServer.getInstance().port + "/?splitEntries=false&username=camel&password=camelPass").to("mock:result");
      from("atom:?feedUri=http://localhost:" + JettyTestServer.getInstance().port + "/&splitEntries=false&username=camel&password=camelPass").to("mock:result2");
    }
  }
;
}
