{
  RouteBuilder[] rbs=new RouteBuilder[2];
  rbs[0]=new RouteBuilder(){
    public void configure(){
      from("direct:input").to("ws://localhost:" + PORT);
    }
  }
;
  rbs[1]=new RouteBuilder(){
    public void configure(){
      from("ws://localhost:" + PORT).to("mock:result");
    }
  }
;
  return rbs;
}
