{
  String dsl="from(\"direct:start\").doTry().to(\"mock:result\")" + ".doCatch(IOException.class).handled(false).to(\"mock:io\")" + ".doCatch(Exception.class).to(\"mock:error\")"+ ".doFinally().to(\"mock:finally\").end()"+ ".to(\"mock:last\").end()";
  String expected="from(\"direct:start\").doTry().to(\"mock:result\")" + ".doCatch(IOException.class).handled(false).to(\"mock:io\")" + ".doCatch(Exception.class).to(\"mock:error\")"+ ".doFinally().to(\"mock:finally\").end()"+ ".to(\"mock:last\")";
  assertEquals(expected,render(dsl));
}
