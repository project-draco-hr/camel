{
  String dsl="from(\"direct:start\").doTry().to(\"mock:result\")" + ".doCatch(IOException.class).handled(false).to(\"mock:io\")" + ".doCatch(Exception.class).to(\"mock:error\").end()";
  String expected="from(\"direct:start\").doTry().to(\"mock:result\")" + ".doCatch(IOException.class).handled(false).to(\"mock:io\")" + ".doCatch(Exception.class).to(\"mock:error\")";
  assertEquals(expected,render(dsl));
}
