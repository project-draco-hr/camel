{
  if (predicate != null) {
    return new Processor<E>(){
      public void onExchange(      E exchange){
        if (predicate.evaluate(exchange)) {
          destination.send(exchange);
        }
      }
    }
;
  }
 else {
    return new Processor<E>(){
      public void onExchange(      E exchange){
        destination.send(exchange);
      }
    }
;
  }
}
