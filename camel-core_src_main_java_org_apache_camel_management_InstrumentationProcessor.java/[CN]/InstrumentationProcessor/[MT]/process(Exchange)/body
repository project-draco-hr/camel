{
  if (processor != null) {
    long startTime=-1;
    if (counter != null && counter.isStatisticsEnabled()) {
      startTime=System.nanoTime();
    }
    try {
      processor.process(exchange);
    }
 catch (    Exception e) {
      exchange.setException(e);
    }
    if (startTime != -1) {
      long diff=(System.nanoTime() - startTime) / 1000000;
      recordTime(exchange,diff);
    }
  }
}
