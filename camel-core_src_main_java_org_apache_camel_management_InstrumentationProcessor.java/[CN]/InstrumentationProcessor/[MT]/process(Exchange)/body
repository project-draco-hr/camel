{
  if (processor != null) {
    long startTime=0;
    if (counter != null) {
      startTime=System.nanoTime();
    }
    try {
      processor.process(exchange);
    }
 catch (    Exception e) {
      exchange.setException(e);
    }
    if (counter != null) {
      recordTime(exchange,(System.nanoTime() - startTime) / 1000000.0);
    }
  }
}
