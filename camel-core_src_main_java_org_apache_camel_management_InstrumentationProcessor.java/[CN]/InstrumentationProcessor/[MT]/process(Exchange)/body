{
  if (processor != null) {
    long startTime=-1;
    if (counter != null && counter.isStatisticsEnabled()) {
      startTime=System.currentTimeMillis();
    }
    try {
      processor.process(exchange);
    }
 catch (    Exception e) {
      exchange.setException(e);
    }
    if (startTime != -1) {
      recordTime(exchange,System.currentTimeMillis() - startTime);
    }
  }
}
