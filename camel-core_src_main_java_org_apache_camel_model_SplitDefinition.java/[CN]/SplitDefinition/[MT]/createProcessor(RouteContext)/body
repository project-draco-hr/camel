{
  Processor childProcessor=routeContext.createProcessor(this);
  aggregationStrategy=createAggregationStrategy(routeContext);
  executorService=createExecutorService(routeContext);
  Expression exp=getExpression().createExpression(routeContext);
  return new Splitter(routeContext.getCamelContext(),exp,childProcessor,aggregationStrategy,isParallelProcessing(),executorService,isStreaming(),isStopOnException());
}
