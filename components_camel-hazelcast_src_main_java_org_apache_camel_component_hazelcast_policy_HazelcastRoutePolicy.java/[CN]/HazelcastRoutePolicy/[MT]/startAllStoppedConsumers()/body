{
synchronized (lock) {
    try {
      for (      Route route : suspendedRoutes) {
        LOGGER.debug("Starting consumer for {} ({})",route.getId(),route.getConsumer());
        startConsumer(route.getConsumer());
      }
      suspendedRoutes.clear();
    }
 catch (    Exception e) {
      handleException(e);
    }
  }
}
