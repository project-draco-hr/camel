{
  final String payload=ExchangeHelper.convertToMandatoryType(exchange,String.class,graph);
  final MultiFormatWriter writer=new MultiFormatWriter();
  final String type=this.params.getType().toString();
  final String encoding=this.params.getEncoding();
  final BitMatrix matrix=writer.encode(new String(payload.getBytes(encoding),encoding),this.params.getFormat(),this.params.getWidth(),this.params.getHeight(),writerHintMap);
  MatrixToImageWriter.writeToStream(matrix,type,stream);
}
