{
  try {
    resolveMandatoryEndpoint("jms:topic:Foo.Bar?username=James");
    fail("Expect the exception here");
  }
 catch (  ResolveEndpointFailedException exception) {
  }
  try {
    resolveMandatoryEndpoint("jms:topic:Foo.Bar?password=ABC");
    fail("Expect the exception here");
  }
 catch (  ResolveEndpointFailedException exception) {
  }
}
