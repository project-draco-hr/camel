{
  HystrixCommandGroupKey key=HystrixCommandGroupKey.Factory.asKey(id);
  HystrixProcessorCommand command=new HystrixProcessorCommand(key,exchange,callback,processor,fallback);
  try {
    command.execute();
  }
 catch (  Throwable e) {
    exchange.setException(e);
  }
  callback.done(true);
  return true;
}
