{
  JmsBroker broker=new JmsBroker();
  try {
    broker.start();
    ApplicationContext context=new ClassPathXmlApplicationContext("META-INF/spring/camel-server.xml");
    Thread.sleep(5 * 60 * 1000);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    try {
      broker.stop();
    }
 catch (    Exception e) {
    }
    System.exit(0);
  }
}
