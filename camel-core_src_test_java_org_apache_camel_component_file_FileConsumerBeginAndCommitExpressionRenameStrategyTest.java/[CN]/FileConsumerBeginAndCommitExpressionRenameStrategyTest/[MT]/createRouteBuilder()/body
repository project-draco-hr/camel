{
  return new RouteBuilder(){
    public void configure() throws Exception {
      from("newfile://target/reports?preMoveExpression=../inprogress/${file:name.noext}.bak&moveExpression=../done/${file:name}&consumer.delay=5000").process(new Processor(){
        public void process(        Exchange exchange) throws Exception {
          GenericFileExchange<File> fe=(GenericFileExchange<File>)exchange;
          assertTrue(fe.getGenericFile().getRelativeFileName().indexOf("inprogress") > -1);
        }
      }
).to("mock:report");
    }
  }
;
}
