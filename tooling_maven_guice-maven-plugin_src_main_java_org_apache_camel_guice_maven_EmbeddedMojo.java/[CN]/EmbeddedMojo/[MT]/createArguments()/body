{
  List<String> args=new ArrayList<String>(5);
  if (isDotEnabled()) {
    args.add("-outdir");
    args.add(getOutputDirectory());
  }
  if (isDotAggregationEnabled()) {
    args.add("-aggregate-dot");
    args.add("true");
  }
  args.add("-duration");
  args.add(getDuration());
  return args.toArray(new String[0]);
}
