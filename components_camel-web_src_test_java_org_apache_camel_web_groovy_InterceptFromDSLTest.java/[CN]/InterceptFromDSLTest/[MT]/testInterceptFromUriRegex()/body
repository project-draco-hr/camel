{
  String dsl="interceptFrom(\"seda:(bar|foo)\").to(\"mock:intercept\");" + "from(\"direct:start\").to(\"mock:result\");from(\"seda:bar\").to(\"mock:result\");" + "from(\"seda:foo\").to(\"mock:result\");from(\"seda:cheese\").to(\"mock:result\")";
  String expected="from(\"direct:start\").to(\"mock:result\");\n" + "interceptFrom(\"seda:(bar|foo)\").to(\"mock:intercept\");from(\"seda:bar\").to(\"mock:result\");\n" + "interceptFrom(\"seda:(bar|foo)\").to(\"mock:intercept\");from(\"seda:foo\").to(\"mock:result\");\n"+ "from(\"seda:cheese\").to(\"mock:result\")";
  assertEquals(expected,renderRoutes(dsl));
}
