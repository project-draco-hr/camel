{
  String dsl="interceptFrom(\"seda:bar\").to(\"mock:bar\");" + "from(\"direct:start\").to(\"mock:first\").to(\"seda:bar\");" + "from(\"seda:bar\").to(\"mock:result\");from(\"seda:foo\").to(\"mock:result\")";
  String expected="from(\"direct:start\").to(\"mock:first\").to(\"seda:bar\");\n" + "interceptFrom(\"seda:bar\").to(\"mock:bar\");from(\"seda:bar\").to(\"mock:result\");\n" + "from(\"seda:foo\").to(\"mock:result\")";
  assertEquals(expected,renderRoutes(dsl));
}
