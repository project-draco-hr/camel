{
  XMLReader xmlReader=null;
  WeakReference<XMLReader> ref;
  while ((ref=pool.poll()) != null) {
    if ((xmlReader=ref.get()) != null) {
      break;
    }
  }
  if (xmlReader == null) {
    xmlReader=saxParserFactory.newSAXParser().getXMLReader();
  }
  return new OneTimeXMLReader(xmlReader);
}
