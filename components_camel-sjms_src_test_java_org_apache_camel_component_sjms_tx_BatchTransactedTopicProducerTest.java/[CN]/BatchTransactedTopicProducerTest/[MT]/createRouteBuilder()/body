{
  return new RouteBuilder(){
    @Override public void configure(){
      from("direct:start").to("log:test-before?showAll=true").to("sjms:topic:batch.topic?transacted=true").to("mock:test.prebatch");
      from("sjms:topic:batch.topic").to("log:test-after?showAll=true").to("mock:test.postbatch");
    }
  }
;
}
