{
  CachedOutputStream cos=null;
  InputStream is=null;
  try {
    cos=new CachedOutputStream(exchange.getContext().getProperties());
    is=GZIPHelper.getInputStream(method);
    if (is == null) {
      return null;
    }
    IOUtils.copy(is,cos);
    cos.flush();
    return cos.getInputStream();
  }
  finally {
    ObjectHelper.close(is,"Extracting response body",LOG);
  }
}
