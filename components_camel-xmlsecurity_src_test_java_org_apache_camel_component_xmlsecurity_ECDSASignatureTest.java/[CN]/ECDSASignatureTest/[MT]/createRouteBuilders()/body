{
  if (!canTest) {
    return new RouteBuilder[]{};
  }
  return new RouteBuilder[]{new RouteBuilder(){
    public void configure() throws Exception {
      from("direct:ecdsa").to("xmlsecurity:sign://ecdsa?keyAccessor=#accessor" + "&signatureAlgorithm=http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha1").to("xmlsecurity:verify://ecdsa?keySelector=#selector").to("mock:result");
    }
  }
};
}
