{
  if (!isRunAllowed()) {
    return null;
  }
  Object answer;
  try {
    attemptCounter.incrementAndGet();
    answer=doConvertTo(type,exchange,value,true);
  }
 catch (  Exception e) {
    failedCounter.incrementAndGet();
    return null;
  }
  if (answer == Void.TYPE) {
    missCounter.incrementAndGet();
    return null;
  }
 else {
    hitCounter.incrementAndGet();
    return (T)answer;
  }
}
