{
  super(endpoint);
  this.jdbcTemplate=jdbcTemplate;
  this.sqlPrepareStatementStrategy=sqlPrepareStatementStrategy;
  this.query=query;
  this.batch=batch;
  this.alwaysPopulateStatement=alwaysPopulateStatement;
