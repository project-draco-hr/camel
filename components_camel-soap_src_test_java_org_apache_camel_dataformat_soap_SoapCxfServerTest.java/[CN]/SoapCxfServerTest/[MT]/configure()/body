{
  String jaxbPackage=GetCustomersByName.class.getPackage().getName();
  ElementNameStrategy elNameStrat=new ServiceInterfaceStrategy(CustomerService.class,true);
  SoapJaxbDataFormat soapDataFormat=new SoapJaxbDataFormat(jaxbPackage,elNameStrat);
  from("direct:camelClient").onException(NoSuchCustomerException.class).handled(true).unmarshal(soapDataFormat).end().marshal(soapDataFormat).to("direct:cxfEndpoint").unmarshal(soapDataFormat);
}
