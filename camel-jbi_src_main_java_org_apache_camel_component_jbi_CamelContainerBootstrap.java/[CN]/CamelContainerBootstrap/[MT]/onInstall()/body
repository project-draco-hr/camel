{
  try {
    if (logger.isDebugEnabled()) {
      logger.debug("Bootstrap onInstall");
    }
    doOnInstall();
    if (logger.isDebugEnabled()) {
      logger.debug("Bootstrap onInstall done");
    }
  }
 catch (  JBIException e) {
    throw e;
  }
catch (  Exception e) {
    throw new JBIException("Error calling onInstall",e);
  }
}
