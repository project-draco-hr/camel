{
  String answer=camelMessage.getHeader(CxfBeanHeaderNames.CONTENT_TYPE,String.class);
  if (answer != null) {
    return answer;
  }
  if (answer == null) {
    answer=camelMessage.getHeader("org.apache.camel.component.http.contentType",String.class);
  }
  if (answer != null) {
    return answer;
  }
  return answer="*/*";
}
