{
  URL url=null;
  BundleEntry entry=null;
  BundleContext bundleContext=Activator.getBundle().getBundleContext();
  for (  Bundle bundle : bundleContext.getBundles()) {
    url=bundle.getEntry(path);
    if (url != null) {
      entry=new BundleEntry();
      entry.url=url;
      entry.bundle=bundle;
      break;
    }
  }
  return entry;
}
