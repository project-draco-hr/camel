{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("seda:foo").to("direct:aggregate");
      from("seda:bar").to("direct:aggregate");
      from("seda:baz").to("direct:aggregate");
      from("direct:aggregate").aggregate(header("type"),new MyAggregationStrategy()).batchSize(25).batchTimeout(5000).to("mock:result").end();
    }
  }
;
}
