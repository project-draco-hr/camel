{
  super.doStart();
  String path=getEndpoint().getPath();
  String verb=getEndpoint().getVerb();
  String accept=getEndpoint().getAccept();
  if ("get".equals(verb)) {
    log.info("get(/{})",verb);
    if (accept != null) {
      Spark.get(path,accept,route);
    }
 else {
      Spark.get(path,route);
    }
  }
}
