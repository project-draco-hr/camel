{
  super.doStart();
  String path=getEndpoint().getPath();
  String verb=getEndpoint().getVerb();
  String acceptType=getEndpoint().getAcceptType();
  if ("get".equals(verb)) {
    log.info("get(/{})",verb);
    if (acceptType != null) {
      Spark.get(path,acceptType,route);
    }
 else {
      Spark.get(path,route);
    }
  }
}
