{
  String[] keys=new String[3];
  for (int i=1; i < 4; i++) {
    keys[i - 1]="Confirm" + i;
  }
  addExchanges(keys);
  for (  String key : keys) {
    assertTrue(exists(key));
  }
  aggregationRepository.confirm(camelContext,"Exchange-Confirm5");
  for (  String key : keys) {
    assertTrue(exists(key));
  }
}
