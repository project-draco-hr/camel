{
  Option[] options=options(logProfile(),systemProperty("org.ops4j.pax.logging.DefaultServiceLog.level").value("DEBUG"),mavenBundle().groupId("org.apache.camel").artifactId("camel-core").versionAsInProject(),knopflerfish(),felix(),equinox());
  if (OSGiIntegrationTest.class.getClassLoader().getResource("META-INF/maven/paxexam-config.args") != null) {
    options=OptionUtils.combine(options,mavenConfiguration());
  }
  return options;
}
