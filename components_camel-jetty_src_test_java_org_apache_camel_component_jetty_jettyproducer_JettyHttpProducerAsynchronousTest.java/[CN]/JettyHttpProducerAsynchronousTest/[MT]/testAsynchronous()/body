{
  thread1="";
  thread2="";
  MockEndpoint mock=getMockEndpoint("mock:result");
  mock.expectedMessageCount(1);
  mock.message(0).outBody().isEqualTo("Bye World");
  Object body=null;
  template.sendBody("direct:start",body);
