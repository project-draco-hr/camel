{
  if (sender instanceof NonBlockingNagiosPassiveCheckSender) {
    NonBlockingNagiosPassiveCheckSender nonBlocking=(NonBlockingNagiosPassiveCheckSender)sender;
    ExecutorService executor=getEndpoint().getCamelContext().getExecutorServiceManager().getExecutorService(ThreadPoolBuilder.singleThreadExecutor(getEndpoint().getEndpointUri()),this);
    nonBlocking.setExecutor(executor);
  }
  super.doStart();
}
