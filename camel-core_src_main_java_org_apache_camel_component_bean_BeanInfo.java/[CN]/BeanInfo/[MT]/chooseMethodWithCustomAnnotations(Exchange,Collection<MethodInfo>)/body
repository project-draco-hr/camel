{
  MethodInfo chosen=null;
  for (  MethodInfo possible : possibles) {
    if (possible.hasCustomAnnotation()) {
      if (chosen != null) {
        chosen=null;
        break;
      }
 else {
        chosen=possible;
      }
    }
  }
  if (chosen != null) {
    return chosen;
  }
  throw new AmbiguousMethodCallException(exchange,possibles);
}
