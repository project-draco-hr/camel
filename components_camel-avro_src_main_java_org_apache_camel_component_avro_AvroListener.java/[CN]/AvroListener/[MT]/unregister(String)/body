{
  if (!StringUtils.isEmpty(messageName)) {
    if (consumerRegistry.remove(messageName) == null)     Log.warn("Consumer with message name " + messageName + " was already unregistered.");
  }
 else   defaultConsumer=null;
  if ((defaultConsumer == null) && (consumerRegistry.isEmpty())) {
    if (server != null) {
      server.close();
    }
    return true;
  }
  return false;
}
