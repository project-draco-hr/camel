{
  for (int i=0; i < batchSize; ++i) {
    Exchange e=queue.poll();
    if (e != null) {
      try {
        collection.add(e);
      }
 catch (      Exception t) {
        e.setException(t);
      }
catch (      Throwable t) {
        getExceptionHandler().handleException(t);
      }
    }
 else {
      break;
    }
  }
}
