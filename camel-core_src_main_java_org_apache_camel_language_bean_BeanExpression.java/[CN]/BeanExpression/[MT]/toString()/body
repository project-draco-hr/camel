{
  StringBuilder sb=new StringBuilder("BeanExpression[");
  if (bean != null) {
    sb.append(bean.toString());
  }
 else   if (beanName != null) {
    sb.append(beanName);
  }
 else   if (type != null) {
    sb.append(ObjectHelper.className(type));
  }
  if (method != null) {
    sb.append(" method: ").append(method);
  }
  sb.append("]");
  return sb.toString();
}
