{
  Duration duration=Duration.parse("P60000000000000D");
  try {
    context.getTypeConverter().convertTo(long.class,duration);
  }
 catch (  TypeConversionException e) {
    assertIsInstanceOf(ArithmeticException.class,e.getCause().getCause());
    assertThat(e.getMessage(),is("Error during type conversion from type: java.time.Duration to the required type: " + "long with value PT1440000000000000H due java.lang.ArithmeticException: long overflow"));
  }
}
