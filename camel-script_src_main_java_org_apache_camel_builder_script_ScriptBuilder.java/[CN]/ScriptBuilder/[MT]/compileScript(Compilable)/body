{
  try {
    if (scriptText != null) {
      compiledScript=compilable.compile(scriptText);
    }
 else     if (scriptResource != null) {
      compiledScript=compilable.compile(createScriptReader());
    }
  }
 catch (  ScriptException e) {
    throw createScriptCompileException(e);
  }
catch (  IOException e) {
    throw createScriptCompileException(e);
  }
}
