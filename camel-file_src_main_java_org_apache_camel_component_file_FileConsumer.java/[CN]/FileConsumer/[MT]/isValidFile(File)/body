{
  boolean result=false;
  if (file != null && file.exists()) {
    if (isMatched(file)) {
      if (isAttemptFileLock()) {
        FileChannel fc=null;
        try {
          fc=new RandomAccessFile(file,"rw").getChannel();
          fc.lock();
          result=true;
        }
 catch (        Throwable e) {
        }
 finally {
          if (fc != null) {
            try {
              fc.close();
            }
 catch (            IOException e) {
            }
          }
        }
      }
    }
  }
  return result;
}
