{
  ObjectInstance instance=null;
  try {
    instance=server.registerMBean(obj,name);
  }
 catch (  InstanceAlreadyExistsException e) {
    if (forceRegistration) {
      server.unregisterMBean(name);
      instance=server.registerMBean(obj,name);
    }
 else {
      throw e;
    }
  }
  if (instance != null) {
    mbeans.add(name);
  }
}
