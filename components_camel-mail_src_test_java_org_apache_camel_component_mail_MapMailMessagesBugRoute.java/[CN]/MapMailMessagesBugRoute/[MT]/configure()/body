{
  from("imaps://outlook.office365.com?peek=true&unseen=true&debugMode=true&username=<username>&password=<password>").process(new Processor(){
    @Override public void process(    Exchange exchange) throws Exception {
      if (true) {
        throw new IOException("This will cause messages to be marked SEEN even when peek=true.");
      }
    }
  }
).to("log:mail");
}
