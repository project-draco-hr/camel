{
  context.addRoutes(new RouteBuilder(){
    public void configure(){
      from("direct:start").marshal().secureXML("//cheesesites/france/cheese",false,"Just another 24 Byte key").unmarshal().secureXML("//cheesesites/france",false,"Just another 24 Byte key").process(new DecryptedXMLMessageProcessor());
    }
  }
);
  context.start();
  sendText();
}
