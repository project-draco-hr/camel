{
  try {
    for (long i=startIndex; i < endIndex; i++) {
      Exchange exchange=endpoint.createExchange(i);
      getProcessor().process(exchange);
      try {
        long delay=endpoint.getProduceDelay();
        if (delay < 3) {
          delay=3;
        }
        Thread.sleep(delay);
      }
 catch (      InterruptedException e) {
        LOG.debug(e);
      }
      if (reporter != null) {
        reporter.process(exchange);
      }
    }
  }
 catch (  Exception e) {
    handleException(e);
  }
}
