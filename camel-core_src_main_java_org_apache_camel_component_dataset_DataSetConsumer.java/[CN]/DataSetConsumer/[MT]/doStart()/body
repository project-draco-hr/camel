{
  super.doStart();
  DataSet dataSet=endpoint.getDataSet();
  for (long i=0; i < dataSet.getSize(); i++) {
    Exchange exchange=endpoint.createExchange(i);
    getProcessor().process(exchange);
    long delay=endpoint.getProduceDelay();
    if (delay > 0) {
      Thread.sleep(delay);
    }
  }
}
