{
  if (outputDir.exists()) {
    if (!outputDir.isDirectory()) {
      getLog().error("File with same name already exists: " + outputDir.getAbsolutePath());
      return false;
    }
  }
 else {
    if (!outputDir.mkdirs()) {
      getLog().error("Cannot make output directory at: " + outputDir.getAbsolutePath());
      return false;
    }
  }
  return true;
}
