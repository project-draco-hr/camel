{
  for (  Map.Entry<String,Set<String>> entry : packages.entrySet()) {
    sink.section2();
    sink.sectionTitle2();
    sink.text(entry.getKey());
    sink.sectionTitle2_();
    sink.list();
    for (    String clazz : entry.getValue()) {
      sink.listItem();
      sink.anchor(clazz);
      sink.text(clazz);
      sink.anchor_();
      sink.listItem_();
    }
    sink.list_();
    sink.section2_();
  }
}
