{
  Option[] options=combine(getDefaultCamelKarafOptions(),new KarafDistributionConfigurationFileExtendOption("etc/jre.properties","jre-1.6",",sun.misc"),new KarafDistributionConfigurationFileExtendOption("etc/jre.properties","jre-1.7",",sun.misc"),new KarafDistributionConfigurationFileExtendOption("etc/jre.properties","jre-1.8",",sun.misc"),loadCamelFeatures("camel-disruptor"));
  return options;
}
