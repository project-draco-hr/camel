{
  LOG.error("Exception on receiving message from address: " + address + " using connector: "+ connector,cause);
  this.message=null;
  this.messageReceived=false;
  this.cause=cause;
  if (ioSession != null) {
    try {
      closeSessionIfNeededAndAwaitCloseInHandler(ioSession);
    }
 catch (    InterruptedException e) {
      Thread.currentThread().interrupt();
    }
  }
}
