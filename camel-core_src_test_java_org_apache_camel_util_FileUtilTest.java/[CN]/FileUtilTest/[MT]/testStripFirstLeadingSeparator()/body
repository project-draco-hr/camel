{
  assertEquals(null,FileUtil.stripFirstLeadingSeparator(null));
  assertEquals("foo",FileUtil.stripFirstLeadingSeparator("foo"));
  assertEquals("foo/bar",FileUtil.stripFirstLeadingSeparator("foo/bar"));
  assertEquals("foo/",FileUtil.stripFirstLeadingSeparator("foo/"));
  assertEquals("foo/bar",FileUtil.stripFirstLeadingSeparator("/foo/bar"));
  assertEquals("/foo/bar",FileUtil.stripFirstLeadingSeparator("//foo/bar"));
  assertEquals("//foo/bar",FileUtil.stripFirstLeadingSeparator("///foo/bar"));
}
