{
  assertEquals(null,FileUtil.stripTrailingSeparator(null));
  assertEquals("foo",FileUtil.stripTrailingSeparator("foo"));
  assertEquals("foo/bar",FileUtil.stripTrailingSeparator("foo/bar"));
  assertEquals("foo",FileUtil.stripTrailingSeparator("foo/"));
  assertEquals("foo/bar",FileUtil.stripTrailingSeparator("foo/bar/"));
  assertEquals("/foo/bar",FileUtil.stripTrailingSeparator("/foo/bar/"));
  assertEquals("/foo/bar",FileUtil.stripTrailingSeparator("/foo/bar"));
}
