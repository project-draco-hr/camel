{
  if (FileUtil.isWindows()) {
    assertEquals("foo\\bar",FileUtil.normalizePath("foo/bar"));
    assertEquals("foo\\bar\\baz",FileUtil.normalizePath("foo/bar\\baz"));
  }
 else {
    assertEquals("foo/bar",FileUtil.normalizePath("foo/bar"));
    assertEquals("foo/bar/baz",FileUtil.normalizePath("foo/bar\\baz"));
  }
}
