{
  Map<String,List<RouteType>> map=new HashMap<String,List<RouteType>>();
  for (  RouteType route : routes) {
    String group=route.getGroup();
    if (group == null) {
      group="Camel Routes";
    }
    List<RouteType> list=map.get(group);
    if (list == null) {
      list=new ArrayList<RouteType>();
      map.put(group,list);
    }
    list.add(route);
  }
  return map;
}
