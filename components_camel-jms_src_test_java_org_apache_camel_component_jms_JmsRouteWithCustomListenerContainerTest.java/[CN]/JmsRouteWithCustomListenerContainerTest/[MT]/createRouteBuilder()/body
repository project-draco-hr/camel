{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("activemq:queue:inbox?customMessageListenerContainerRef=myListenerContainer").to("mock:inbox").inOnly("activemq:topic:order").beanRef("orderService","handleOrder");
      from("activemq:topic:order").to("mock:topic");
    }
  }
;
}
