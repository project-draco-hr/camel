{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("seda:start").onException(Exception.class).retryUntil(bean("myRetryBean")).end().recipientList(header("recipientListHeader")).parallelProcessing().to("mock:result");
      from("direct:foo").to("log:foo").to("mock:foo");
    }
  }
;
}
