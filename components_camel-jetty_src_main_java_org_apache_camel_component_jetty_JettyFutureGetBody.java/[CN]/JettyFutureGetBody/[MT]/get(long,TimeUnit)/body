{
  if (!isDone()) {
    boolean done=httpExchange.waitForBodyToComplete(timeout,timeUnit);
    if (done) {
      return doGetBody();
    }
 else {
      ExchangeTimedOutException cause=new ExchangeTimedOutException(exchange,timeout);
      throw new ExecutionException(cause);
    }
  }
  return doGetBody();
}
