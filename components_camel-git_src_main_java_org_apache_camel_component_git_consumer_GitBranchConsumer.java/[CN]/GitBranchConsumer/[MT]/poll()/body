{
  int count=0;
  List<Ref> call=getGit().branchList().call();
  for (  Ref ref : call) {
    if (!used.contains(ref.getName())) {
      Exchange e=getEndpoint().createExchange();
      e.getOut().setBody(ref);
      getProcessor().process(e);
      used.add(ref.getName());
      count++;
    }
  }
  return count;
}
