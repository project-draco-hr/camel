{
  try {
    Object newHeader=expression.evaluate(exchange,Object.class);
    boolean out=exchange.hasOut();
    Message old=out ? exchange.getOut() : exchange.getIn();
    old.setHeader(headerName,newHeader);
  }
 catch (  Exception e) {
    exchange.setException(e);
  }
  callback.done(true);
  return true;
}
