{
  if (!headerName.startsWith("JMS")) {
    return false;
  }
  if (headerName.startsWith("JMSX")) {
    return false;
  }
  if (headerName.startsWith("JMS_")) {
    return false;
  }
  if (headerName.length() > 3) {
    Character fourth=headerName.charAt(3);
    if (Character.isLetter(fourth)) {
      return true;
    }
  }
  return false;
}
