{
  if (isLockFile()) {
    Channel channel=exchange.getProperty("org.apache.camel.fileChannel",Channel.class);
    if (channel != null) {
      if (log.isDebugEnabled()) {
        log.debug("Unlocking file: " + file);
      }
      channel.close();
    }
 else {
      throw new Exception("No Channel available to close on exchange: " + exchange);
    }
  }
}
