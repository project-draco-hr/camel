{
  bodies.add(body);
  if (previous != null) {
    previouses.add(previous);
  }
  int invoked=0;
  Object current=headers.get("invoked");
  if (current != null) {
    invoked=Integer.valueOf(current.toString());
  }
  invoked++;
  headers.put("invoked",invoked);
  if (invoked == 1) {
    return "mock:a";
  }
 else   if (invoked == 2) {
    return "mock:b,mock:c";
  }
 else   if (invoked == 3) {
    return "direct:foo";
  }
 else   if (invoked == 4) {
    return "mock:result";
  }
  return null;
}
