{
  int total=exchanges.size();
  if (maxMessagesPerPoll > 0 && total > maxMessagesPerPoll) {
    log.debug("Limiting to maximum messages to poll {} as there was {} messages in this poll.",maxMessagesPerPoll,total);
    total=maxMessagesPerPoll;
  }
  for (int index=0; index < total && isBatchAllowed(); index++) {
    Exchange exchange=(Exchange)exchanges.poll();
    exchange.setProperty(Exchange.BATCH_INDEX,index);
    exchange.setProperty(Exchange.BATCH_SIZE,total);
    exchange.setProperty(Exchange.BATCH_COMPLETE,index == total - 1);
    pendingExchanges=total - index - 1;
    if (customProcessor != null) {
      customProcessExchange(exchange,customProcessor);
    }
 else {
      processExchange(exchange);
    }
  }
  removeExcessiveInProgressFiles((Deque)exchanges,0);
  return total;
}
