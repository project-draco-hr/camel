{
  TinyBundle bundle=TinyBundles.newBundle();
  for (  URL url : getBlueprintDescriptors()) {
    bundle.add("OSGI-INF/blueprint/blueprint-" + url.getFile().replace("/","-"),url);
  }
  bundle.set("Manifest-Version","2").set("Bundle-ManifestVersion","2").set("Bundle-SymbolicName","test-bundle").set("Bundle-Version","0.0.0");
  return bundle;
}
