{
  log.trace("existsFile({})",name);
  try {
    String[] names=client.listNames(name);
    if (names == null) {
      return false;
    }
    return (names.length >= 1);
  }
 catch (  IOException e) {
    throw new GenericFileOperationFailedException(client.getReplyCode(),client.getReplyString(),e.getMessage(),e);
  }
}
