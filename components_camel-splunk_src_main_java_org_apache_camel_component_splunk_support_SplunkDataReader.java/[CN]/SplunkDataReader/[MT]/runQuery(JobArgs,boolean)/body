{
  Service service=endpoint.getService();
  Job job=service.getJobs().create(getSearch(),queryArgs);
  if (realtime) {
    while (!job.isReady()) {
      Thread.sleep(500);
    }
    Thread.sleep(1000);
  }
 else {
    while (!job.isDone()) {
      Thread.sleep(500);
    }
  }
  return extractData(job,realtime);
}
