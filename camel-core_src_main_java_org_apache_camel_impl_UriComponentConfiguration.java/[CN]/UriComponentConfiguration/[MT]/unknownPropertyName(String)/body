{
  if (isStrictOnParameterNames()) {
    throw new InvalidPropertyException(this,name,endpointClass);
  }
 else {
    LOG.warn("Using parameter " + name + " on endpoint "+ getEndpointClass().getName()+ " which does not have a @UriParam annotation! "+ "Please add the @UriParam annotation to the "+ name+ "field");
  }
}
