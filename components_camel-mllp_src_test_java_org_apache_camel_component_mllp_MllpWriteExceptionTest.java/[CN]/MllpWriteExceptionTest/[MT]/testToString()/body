{
  final String expectedString="org.apache.camel.component.mllp.MllpWriteException: " + "{mllpPayload=" + "MSH|^~\\&|APP_A|FAC_A|^org^sys||||ADT^A04^ADT_A04|||2.6<CR>"+ "PID|1||1100832^^^^PI||TEST^FIG||98765432|U||R|435 MAIN STREET^^LONGMONT^CO^80503||123-456-7890|||S<CR><CR><LF>"+ "}";
  assertEquals(expectedString,exception.toString());
}
