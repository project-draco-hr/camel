{
  from("jms:queue:loan").process(new CreditAgencyProcessor()).multicast(new BankResponseAggregationStrategy()).parallelProcessing().to("jms:queue:bank1","jms:queue:bank2","jms:queue:bank3").end().process(new ReplyProcessor());
  from("jms:queue:bank1").process(new BankProcessor("bank1"));
  from("jms:queue:bank2").process(new BankProcessor("bank2"));
  from("jms:queue:bank3").process(new BankProcessor("bank3"));
}
