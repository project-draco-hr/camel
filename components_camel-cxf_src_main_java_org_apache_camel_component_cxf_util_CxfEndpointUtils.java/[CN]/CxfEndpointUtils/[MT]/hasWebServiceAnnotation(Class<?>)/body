{
  if (cls == null) {
    return false;
  }
  if (null != cls.getAnnotation(WebService.class)) {
    return true;
  }
  for (  Class<?> inf : cls.getInterfaces()) {
    if (null != inf.getAnnotation(WebService.class)) {
      return true;
    }
  }
  return hasWebServiceAnnotation(cls.getSuperclass());
}
