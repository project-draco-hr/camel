{
  String retval=exchange.getIn().getHeader(Exchange.DESTINATION_OVERRIDE_URL,String.class);
  if (retval == null) {
    retval=defaultAddress;
  }
 else {
    if (LOG.isTraceEnabled()) {
      LOG.trace("Client address is overridden by header '" + Exchange.DESTINATION_OVERRIDE_URL + "' to value '"+ retval+ "'");
    }
  }
  return retval;
}
