{
  if (version == null) {
    return null;
  }
  try {
    URL found=null;
    Enumeration<URL> urls=classLoader.getResources(name);
    while (urls.hasMoreElements()) {
      URL url=urls.nextElement();
      if (url.getPath().contains(version)) {
        found=url;
        break;
      }
    }
    if (found != null) {
      return found.openStream();
    }
  }
 catch (  IOException e) {
  }
  return null;
}
