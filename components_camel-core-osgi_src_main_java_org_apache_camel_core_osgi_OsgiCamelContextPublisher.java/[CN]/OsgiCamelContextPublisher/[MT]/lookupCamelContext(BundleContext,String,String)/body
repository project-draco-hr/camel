{
  Version bundleVersion=getBundleVersion(bundleContext.getBundle());
  ServiceReference[] refs=bundleContext.getServiceReferences(CamelContext.class.getName(),"(&(" + CONTEXT_SYMBOLIC_NAME_PROPERTY + "="+ symbolicName+ ")"+ "("+ CONTEXT_NAME_PROPERTY+ "="+ contextName+ ")"+ "("+ CONTEXT_VERSION_PROPERTY+ "="+ bundleVersion+ "))");
  return refs != null && refs.length > 0;
}
