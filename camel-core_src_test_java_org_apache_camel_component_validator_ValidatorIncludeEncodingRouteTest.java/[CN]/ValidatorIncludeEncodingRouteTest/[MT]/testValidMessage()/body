{
  validEndpoint.expectedMessageCount(1);
  finallyEndpoint.expectedMessageCount(1);
  String body="<t:text xmlns:t=\"org.text\">\n" + "  <t:sentence>J'aime les cam??lid??s</t:sentence>\n" + "</t:text>";
