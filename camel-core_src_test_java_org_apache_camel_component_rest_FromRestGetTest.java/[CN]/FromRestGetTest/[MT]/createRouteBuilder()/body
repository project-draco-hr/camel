{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      restConfiguration().host("localhost");
      rest("/say/hello").get().to("direct:hello");
      rest("/say/bye").get().consumes("application/json").param().type(RestParamType.header).description("header param description1").dataType("integer").allowableValues("1","2","3","4").defaultValue("1").name("header_count").required(true).endParam().param().type(RestParamType.query).description("header param description2").dataType("string").allowableValues("a","b","c","d").defaultValue("b").collectionFormat(CollectionFormat.multi).name("header_letter").required(false).endParam().responseMessage().code(300).message("test msg").responseModel(Integer.class).header("rate").description("Rate limit").dataType("integer").endHeader().endResponseMessage().responseMessage().code("error").message("does not work").endResponseMessage().to("direct:bye").post().to("mock:update");
      from("direct:hello").transform().constant("Hello World");
      from("direct:bye").transform().constant("Bye World");
    }
  }
;
}
