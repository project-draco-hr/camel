{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:start").to("direct:multicast").to("mock:result");
      from("direct:multicast").multicast(new MyAggregationStrategy()).to("direct:a").to("direct:b");
      Predicate goodWord=body().contains("World");
      from("direct:a","direct:b").filter(goodWord).to("mock:filtered");
    }
  }
;
}
