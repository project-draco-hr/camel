{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("jetty:http://localhost:8080/myapp?matchOnUriPrefix=true").to("jetty:http://localhost:8081/myapp2?bridgeEndpoint=true&throwExceptionOnFailure=false&urlRewrite=#myRewrite");
      from("jetty:http://localhost:8081/myapp2?matchOnUriPrefix=true").transform().simple("${header.CamelHttpUrl}?${header.CamelHttpQuery}");
    }
  }
;
}
