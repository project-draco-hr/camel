{
  return new RouteBuilder(){
    public void configure(){
      from("websocket://localhost:9292/bar").log(">>> Message received from BAR WebSocket Client : ${body}").transform().simple("The bar has ${body}").to("websocket://localhost:9292/bar");
      from("websocket://localhost:9292/pub").log(">>> Message received from PUB WebSocket Client : ${body}").transform().simple("The pub has ${body}").to("websocket://localhost:9292/pub");
    }
  }
;
}
