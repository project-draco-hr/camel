{
  return new RouteBuilder(){
    public void configure(){
      from("websocket://localhost:9292/echo").log(">>> Message received from WebSocket Client : ${body}").transform().simple("${body}${body}").to("websocket://localhost:9292/echo");
      from("websocket://localhost:9393/echo").log(">>> Message received from WebSocket Client : ${body}").transform().simple("${body}${body}").to("websocket://localhost:9393/echo");
    }
  }
;
}
