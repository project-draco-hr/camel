{
  return new RouteBuilder(){
    public void configure() throws Exception {
      String fileUrl="file:target/ftptest/?append=false&noop=true";
      from(getFtpUrl()).setHeader(NewFileComponent.HEADER_FILE_NAME,constant("deleteme.txt")).convertBodyTo(String.class).to(fileUrl).to("mock:result");
    }
  }
;
}
