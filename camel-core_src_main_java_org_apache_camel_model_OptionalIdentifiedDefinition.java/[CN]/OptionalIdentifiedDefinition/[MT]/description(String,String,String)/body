{
  if (id != null) {
    setId(id);
  }
  if (text != null) {
    if (description == null) {
      description=new DescriptionDefinition();
    }
    description.setText(text);
  }
  if (lang != null) {
    if (description == null) {
      description=new DescriptionDefinition();
    }
    description.setLang(lang);
  }
  return (T)this;
}
