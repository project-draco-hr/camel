{
  if (ID_ATTRIBUTE.equals(propertyName)) {
    return;
  }
  if (org.springframework.util.StringUtils.hasText(val)) {
    if (val.startsWith("#")) {
      Map<String,Object> map=getPropertyMap(bean);
      map.put(propertyName,val);
    }
 else {
      bean.addPropertyValue(propertyName,val);
    }
  }
}
