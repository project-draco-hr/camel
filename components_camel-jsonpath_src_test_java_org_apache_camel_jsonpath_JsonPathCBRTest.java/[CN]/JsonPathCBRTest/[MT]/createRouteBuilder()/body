{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:start").choice().when().language("jsonpath","$.store.book[?(@.price < 10)]").to("mock:cheap").when().language("jsonpath","$.store.book[?(@.price < 30)]").to("mock:average").otherwise().to("mock:expensive");
    }
  }
;
}
