{
  InputStream stream=ExchangeHelper.getMandatoryInBody(exchange,InputStream.class);
  try {
    Message out=exchange.getOut(true);
    out.copyFrom(exchange.getIn());
    Object result=dataFormat.unmarshal(exchange,stream);
    out.setBody(result);
  }
  finally {
    if (null != stream) {
      stream.close();
    }
  }
}
