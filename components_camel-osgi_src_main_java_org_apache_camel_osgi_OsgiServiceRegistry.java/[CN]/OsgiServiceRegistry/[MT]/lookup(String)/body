{
  Object service=serviceCacheMap.get(name);
  if (service == null) {
    ServiceReference sr=bundleContext.getServiceReference(name);
    if (sr != null) {
      service=bundleContext.getService(sr);
      if (service != null) {
        serviceCacheMap.put(name,service);
      }
    }
  }
  return service;
}
