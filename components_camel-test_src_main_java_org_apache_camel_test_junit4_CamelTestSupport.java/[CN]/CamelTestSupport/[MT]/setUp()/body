{
  log.info("********************************************************************************");
  log.info("Testing: " + getTestMethodName() + "("+ getClass().getName()+ ")");
  log.info("********************************************************************************");
  boolean first=INIT.compareAndSet(false,true);
  if (isCreateCamelContextPerClass()) {
    if (first) {
      doPreSetup();
      doSetUp();
      doPostSetup();
    }
 else {
      postProcessTest();
      resetMocks();
    }
  }
 else {
    doPreSetup();
    doSetUp();
    doPostSetup();
  }
  watch.restart();
}
