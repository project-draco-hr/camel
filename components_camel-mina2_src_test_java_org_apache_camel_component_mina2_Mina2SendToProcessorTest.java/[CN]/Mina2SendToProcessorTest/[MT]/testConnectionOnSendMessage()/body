{
  context.addRoutes(new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:start").to(String.format("mina2:tcp://localhost:%1$s?sync=false",getPort()));
    }
  }
);
  try {
    context.start();
  }
 catch (  Exception e) {
    fail("Should not have thrown an exception");
  }
}
