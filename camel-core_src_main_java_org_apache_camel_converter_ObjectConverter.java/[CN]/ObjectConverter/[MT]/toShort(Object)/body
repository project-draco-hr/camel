{
  if (value instanceof Short) {
    return (Short)value;
  }
 else   if (value instanceof Number) {
    if (value.equals(Double.NaN)) {
      return null;
    }
    Number number=(Number)value;
    return number.shortValue();
  }
 else   if (value instanceof String) {
    return Short.valueOf((String)value);
  }
 else {
    return null;
  }
}
