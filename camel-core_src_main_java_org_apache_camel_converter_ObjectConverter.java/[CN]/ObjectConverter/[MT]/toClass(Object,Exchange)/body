{
  if (value instanceof Class) {
    return (Class)value;
  }
 else   if (value instanceof String) {
    if (exchange != null) {
      return exchange.getContext().getClassResolver().resolveClass((String)value);
    }
 else {
      return ObjectHelper.loadClass((String)value);
    }
  }
 else {
    return null;
  }
}
