{
  if (value instanceof Long) {
    return (Long)value;
  }
 else   if (value instanceof Number) {
    if (value.equals(Double.NaN)) {
      return null;
    }
    Number number=(Number)value;
    return number.longValue();
  }
 else   if (value instanceof String) {
    return Long.valueOf((String)value);
  }
 else {
    return null;
  }
}
