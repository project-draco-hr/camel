{
  if (value instanceof Byte) {
    return (Byte)value;
  }
 else   if (value instanceof Number) {
    if (isNaN(value)) {
      return Byte.valueOf((byte)0);
    }
    Number number=(Number)value;
    return number.byteValue();
  }
 else   if (value instanceof String) {
    return Byte.valueOf((String)value);
  }
 else {
    return null;
  }
}
