{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  ExceptionPolicyKey that=(ExceptionPolicyKey)o;
  if (!exceptionClass.equals(that.exceptionClass)) {
    return false;
  }
  if (when != null ? !when.equals(that.when) : that.when != null) {
    return false;
  }
  return true;
}
