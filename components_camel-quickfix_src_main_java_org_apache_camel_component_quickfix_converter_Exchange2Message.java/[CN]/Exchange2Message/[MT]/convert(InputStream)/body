{
  byte buffer[]=new byte[BUFFER_SIZE];
  ByteArrayOutputStream baos=new ByteArrayOutputStream();
  int count;
  count=in.read(buffer);
  while (count != -1) {
    baos.write(buffer,0,count);
    count=in.read(buffer);
  }
  String str=baos.toString("ISO-8859-1");
  in.close();
  return new Message(str);
}
