{
  if (!(msg instanceof ByteBuf)) {
    out.add(msg);
  }
 else {
    ByteBuf cb=(ByteBuf)msg;
    if (cb.isReadable()) {
      byte[] bytes=new byte[cb.readableBytes()];
      int readerIndex=cb.readerIndex();
      cb.getBytes(readerIndex,bytes);
      out.add(bytes);
    }
 else {
      out.add((Object)null);
    }
  }
}
