{
  MockEndpoint mock=getMockEndpoint("mock:result");
  mock.expectedBodiesReceived("Hello World");
  template.sendBody("seda:foo","Hello World");
  assertMockEndpointsSatisfied();
  context.startRoute("third");
  try {
    context.startRoute("first");
    fail("Should have thrown exception");
  }
 catch (  FailedToStartRouteException e) {
    assertEquals("Failed to start route first because of Multiple consumers for the same endpoint is not allowed:" + " Endpoint[seda://foo?concurrentConsumers=5]",e.getMessage());
  }
}
