{
  StringBuilder servers=new StringBuilder();
  String prefix="nats://";
  String[] pieces=getServers().split(",");
  for (int i=0; i < pieces.length; i++) {
    if (i < pieces.length - 1) {
      servers.append(prefix + pieces[i] + ",");
    }
 else {
      servers.append(prefix + pieces[i]);
    }
  }
  return servers.toString();
}
