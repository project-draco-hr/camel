{
  DefaultErrorHandler answer=new DefaultErrorHandler(routeContext.getCamelContext(),processor,getLogger(),getOnRedelivery(),getRedeliveryPolicy(),getExceptionPolicyStrategy(),getRetryWhilePolicy(routeContext.getCamelContext()),getExecutorService(routeContext.getCamelContext()),getOnPrepareFailure());
  configure(routeContext,answer);
  return answer;
}
