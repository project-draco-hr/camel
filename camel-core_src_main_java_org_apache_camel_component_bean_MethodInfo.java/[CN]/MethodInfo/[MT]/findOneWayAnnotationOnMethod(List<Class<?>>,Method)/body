{
  for (  Class<?> type : classes) {
    try {
      Method definedMethod=type.getMethod(method.getName(),method.getParameterTypes());
      Pattern answer=getPatternAnnotation(definedMethod);
      if (answer != null) {
        return answer;
      }
    }
 catch (    NoSuchMethodException e) {
    }
  }
  return null;
}
