{
  currentStream.close();
  if (!closedOnCompletion) {
    if (fileInputStreamCache != null) {
      fileInputStreamCache.close();
    }
    try {
      cleanUpTempFile();
    }
 catch (    Exception e) {
      LOG.warn("Error deleting temporary cache file: " + tempFile + ". This exception will be ignored.",e);
    }
  }
}
