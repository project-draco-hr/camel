{
  this();
  String value=properties.get(THRESHOLD);
  if (value != null) {
    int i=Integer.parseInt(value);
    if (i > 0) {
      threshold=i;
    }
  }
  value=properties.get(TEMP_DIR);
  if (value != null) {
    File f=new File(value);
    if (f.exists() && f.isDirectory()) {
      outputDir=f;
    }
 else {
      outputDir=null;
    }
  }
 else {
    outputDir=null;
  }
}
