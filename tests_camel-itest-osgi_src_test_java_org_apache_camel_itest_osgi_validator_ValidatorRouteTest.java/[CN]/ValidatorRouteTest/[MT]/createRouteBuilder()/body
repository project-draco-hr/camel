{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:start").doTry().to("validator:org/apache/camel/itest/osgi/validator/schema.xsd").to("mock:valid").doCatch(ValidationException.class).to("mock:invalid").doFinally().to("mock:finally").end();
    }
  }
;
}
