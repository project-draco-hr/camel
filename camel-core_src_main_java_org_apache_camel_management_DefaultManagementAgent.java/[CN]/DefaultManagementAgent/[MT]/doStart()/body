{
  ObjectHelper.notNull(camelContext,"CamelContext");
  camelContext.getLifecycleStrategies().add(0,new DefaultManagementLifecycleStrategy(camelContext));
  if (server == null) {
    finalizeSettings();
    createMBeanServer();
  }
  LOG.debug("Starting JMX agent on server: {}",getMBeanServer());
}
