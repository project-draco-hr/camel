{
  if ("json-jackson".equals(modelName) && "library".equals(optionName)) {
    return "Jackson";
  }
 else   if ("json-xstream".equals(modelName) && "library".equals(optionName)) {
    return "XStream";
  }
 else   if ("json-gson".equals(modelName) && "library".equals(optionName)) {
    return "Gson";
  }
  return defaultValue;
}
