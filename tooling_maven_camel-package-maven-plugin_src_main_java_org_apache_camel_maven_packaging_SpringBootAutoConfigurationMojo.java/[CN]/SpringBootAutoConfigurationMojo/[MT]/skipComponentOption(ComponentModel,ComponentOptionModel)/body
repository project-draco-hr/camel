{
  if ("netty4-http".equals(model.getScheme()) || "netty-http".equals(model.getScheme())) {
    String name=option.getName();
    if (name.equals("textline") || name.equals("delimiter") || name.equals("autoAppendDelimiter")|| name.equals("decoderMaxLineLength")|| name.equals("encoding")|| name.equals("allowDefaultCodec")|| name.equals("udpConnectionlessSending")|| name.equals("networkInterface")|| name.equals("clientMode")|| name.equals("reconnect")|| name.equals("reconnectInterval")|| name.equals("useByteBuf")|| name.equals("udpByteArrayCodec")|| name.equals("broadcast")) {
      return true;
    }
  }
  return false;
}
