{
  return new RouteBuilder(){
    public void configure(){
      port1=getPort();
      port2=getNextPort();
      errorHandler(noErrorHandler());
      from("jetty:http://localhost:" + port2 + "/test/hello?matchOnUriPrefix=true&mapHttpMessageHeaders=false&mapHttpMessageBody=false").log("I was here").to("jetty:http://localhost:" + port1 + "?bridgeEndpoint=true");
      from("jetty://http://localhost:" + port1 + "?matchOnUriPrefix=true").log("Me too").to("mock:input");
    }
  }
;
}
