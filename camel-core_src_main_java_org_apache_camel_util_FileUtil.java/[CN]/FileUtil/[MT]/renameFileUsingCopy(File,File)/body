{
  if (!from.exists()) {
    return false;
  }
  boolean renamed=false;
  LOG.debug("Rename file '{}' to '{}' using copy/delete strategy.",from,to);
  copyFile(from,to);
  if (!deleteFile(from)) {
    throw new IOException("Renaming file from '" + from + "' to '"+ to+ "' failed: Cannot delete file '"+ from+ "' after copy succeeded");
  }
 else {
    renamed=true;
  }
  return renamed;
}
