{
  if (!file.exists()) {
    return new ArrayList<String>();
  }
  BufferedReader reader=new BufferedReader(new FileReader(file));
  List<String> results=new ArrayList<String>();
  String line=reader.readLine();
  while (line != null) {
    results.add(line);
    line=reader.readLine();
  }
  return results;
}
