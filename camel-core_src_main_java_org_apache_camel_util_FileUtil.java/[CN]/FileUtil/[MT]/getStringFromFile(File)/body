{
  InputStream is=null;
  String result=null;
  try {
    is=new FileInputStream(location);
    result=normalizeCRLF(is);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    if (is != null) {
      try {
        is.close();
      }
 catch (      Exception e) {
      }
    }
  }
  return result;
}
