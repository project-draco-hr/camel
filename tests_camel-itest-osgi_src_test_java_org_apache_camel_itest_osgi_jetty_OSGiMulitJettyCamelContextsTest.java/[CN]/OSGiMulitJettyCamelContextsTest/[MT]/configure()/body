{
  Option[] options=combine(getDefaultCamelKarafOptions(),loadCamelFeatures("camel-jetty"),provision(TinyBundles.bundle().add("META-INF/spring/CamelContext1.xml",OSGiMulitJettyCamelContextsTest.class.getResource("CamelContext1.xml")).add(JettyProcessor.class).set(Constants.BUNDLE_SYMBOLICNAME,"org.apache.camel.itest.osgi.CamelContextBundle1").set(Constants.BUNDLE_NAME,"CamelContext1").set(Constants.DYNAMICIMPORT_PACKAGE,"*").build()),provision(TinyBundles.bundle().add("META-INF/spring/CamelContext2.xml",OSGiMulitJettyCamelContextsTest.class.getResource("CamelContext2.xml")).add(JettyProcessor.class).set(Constants.BUNDLE_SYMBOLICNAME,"org.apache.camel.itest.osgi.CamelContextBundle2").set(Constants.DYNAMICIMPORT_PACKAGE,"*").set(Constants.BUNDLE_NAME,"CamelContext2").build()));
  return options;
}
