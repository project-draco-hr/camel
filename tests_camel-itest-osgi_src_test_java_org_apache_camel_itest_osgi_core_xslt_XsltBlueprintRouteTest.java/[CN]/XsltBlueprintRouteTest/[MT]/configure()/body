{
  Option[] options=combine(getDefaultCamelKarafOptions(),loadCamelFeatures("camel-blueprint"),bundle(TinyBundles.bundle().add("OSGI-INF/blueprint/test.xml",XsltBlueprintRouteTest.class.getResource("XsltBlueprintRouter.xml")).add("transform.xsl",XsltBlueprintRouteTest.class.getResource("transform.xsl")).set(Constants.BUNDLE_SYMBOLICNAME,"XsltBlueprintRouteTest").set(Constants.DYNAMICIMPORT_PACKAGE,"*").build(TinyBundles.withBnd())).noStart());
