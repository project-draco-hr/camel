{
  Option[] options=combine(getDefaultCamelKarafOptions(),scanFeatures(getKarafFeatureUrl(),"war"),KarafDistributionOption.editConfigurationFilePut("etc/org.ops4j.pax.web.cfg","org.osgi.service.http.port","9080"),scanFeatures(getCamelKarafFeatureUrl(),"camel-blueprint","camel-http","camel-servlet"));
