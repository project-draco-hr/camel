{
  StringBuffer sb=new StringBuffer(" {phoneNumber: " + phoneNumber + ", message: "+ message+ ", sender="+ sender+ ", unicode: "+ unicode+ ", multipart: "+ multipart);
  if (idAsString != null && !idAsString.isEmpty()) {
    sb.append(", idAsString=" + idAsString);
  }
  sb.append(" }");
  return sb.toString();
}
