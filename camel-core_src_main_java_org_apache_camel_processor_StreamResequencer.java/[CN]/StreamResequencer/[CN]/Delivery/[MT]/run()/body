{
  while (true) {
    try {
      Thread.sleep(DELIVERY_ATTEMPT_INTERVAL);
    }
 catch (    InterruptedException e) {
      if (cancelRequested) {
        return;
      }
    }
    try {
      engine.deliver();
    }
 catch (    Exception e) {
      exceptionHandler.handleException(e);
    }
  }
}
