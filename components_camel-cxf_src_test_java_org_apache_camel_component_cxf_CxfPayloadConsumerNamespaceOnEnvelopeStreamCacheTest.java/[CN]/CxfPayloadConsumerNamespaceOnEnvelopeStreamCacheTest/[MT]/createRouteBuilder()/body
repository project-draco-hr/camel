{
  return new RouteBuilder(){
    public void configure(){
      from("direct:router").to("cxf:bean:serviceEndpoint?dataFormat=MESSAGE").convertBodyTo(String.class);
      from("cxf:bean:serviceEndpoint?dataFormat=PAYLOAD").streamCaching().convertBodyTo(Document.class).setBody().constant(RESPONSE_PAYLOAD);
    }
  }
;
}
