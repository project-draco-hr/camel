{
  String parent;
  if (isAbsolute()) {
    String name=getAbsoluteFilePath();
    File path=new File(name);
    parent=path.getParent();
  }
 else {
    String name=getRelativeFilePath();
    File path=new File(endpointPath,name);
    parent=path.getParent();
  }
  return normalizePathToProtocol(parent);
}
