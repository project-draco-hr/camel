{
  ByteArrayOutputStream bos=new ByteArrayOutputStream();
  createEncryptedNonCompressedData(bos,PUB_KEY_RING_FILE_NAME);
  MockEndpoint mock=getMockEndpoint("mock:exception");
  mock.expectedMessageCount(1);
  template.sendBody("direct:subkeyUnmarshal",bos.toByteArray());
  assertMockEndpointsSatisfied();
  checkThrownException(mock,PGPException.class,null,"Message is encrypted with a key which could not be found in the Secret Key Ring");
}
