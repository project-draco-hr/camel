{
  long elapsedTime=System.currentTimeMillis() - startTime;
  groupElapsed+=elapsedTime;
  totalElapsed+=elapsedTime;
  loopCount++;
  totalLoops++;
  if (elapsedTime > maxTime) {
    maxTime=elapsedTime;
  }
  if (elapsedTime < minTime) {
    minTime=elapsedTime;
  }
  if (logFrequency > 0 && loopCount % logFrequency == 0) {
    LOG.info(toString());
    reset();
  }
}
