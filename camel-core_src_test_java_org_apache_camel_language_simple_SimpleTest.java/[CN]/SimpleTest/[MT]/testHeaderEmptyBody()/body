{
  exchange.getIn().setBody(null);
  assertExpression("header.foo","abc");
  assertExpression("headers.foo","abc");
  assertExpression("in.header.foo","abc");
  assertExpression("in.headers.foo","abc");
  assertExpression("${header.foo}","abc");
  assertExpression("${headers.foo}","abc");
  assertExpression("${in.header.foo}","abc");
  assertExpression("${in.headers.foo}","abc");
}
