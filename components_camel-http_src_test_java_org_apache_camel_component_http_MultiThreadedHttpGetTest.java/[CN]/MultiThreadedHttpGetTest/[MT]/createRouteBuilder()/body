{
  return new RouteBuilder(){
    public void configure(){
      from("seda:withConversion").thread(5).to("http://www.google.com/search").convertBodyTo(String.class).to("mock:results");
      from("seda:withoutConversion").thread(5).to("http://www.google.com/search").to("mock:results");
    }
  }
;
}
