{
  return new RouteBuilder(){
    public void configure(){
      from("seda:withConversion").thread(5).to("http://localhost:5430/search").convertBodyTo(String.class).to("mock:results");
      from("seda:withoutConversion").thread(5).to("http://localhost:5430/search").to("mock:results");
      from("jetty:http://localhost:5430/search").process(new Processor(){
        public void process(        Exchange exchange) throws Exception {
          exchange.getOut().setBody("<html>Bye World</html>");
        }
      }
);
    }
  }
;
}
