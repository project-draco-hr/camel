{
  createBoxClient();
  this.sharedLink=configuration.getSharedLink();
  this.sharedPassword=configuration.getSharedPassword();
switch (getApiName()) {
case SHARED_COMMENTS:
case SHARED_FILES:
case SHARED_FOLDERS:
case SHARED_ITEMS:
    if (ObjectHelper.isEmpty(sharedLink)) {
      log.warn("Header properties sharedLink and sharedPassword MUST be provided for endpoint {}",getEndpointUri());
    }
  break;
default :
}
}
