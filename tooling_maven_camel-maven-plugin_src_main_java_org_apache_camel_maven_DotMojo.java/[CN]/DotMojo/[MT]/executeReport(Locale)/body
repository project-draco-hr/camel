{
  try {
    this.execute(this.outputDirectory,locale);
    Sink kitchenSink=getSink();
    if (kitchenSink != null) {
      kitchenSink.rawText(htmlBuffer.toString());
    }
 else {
      writeIndexHtmlFile();
    }
  }
 catch (  Exception e) {
    final MavenReportException ex=new MavenReportException(e.getMessage());
    ex.initCause(e.getCause());
    throw ex;
  }
}
