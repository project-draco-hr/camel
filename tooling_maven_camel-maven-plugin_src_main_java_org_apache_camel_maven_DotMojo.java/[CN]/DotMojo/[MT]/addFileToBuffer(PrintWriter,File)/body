{
  BufferedReader reader=null;
  try {
    reader=IOHelper.buffered(new FileReader(file));
    while (true) {
      String line=reader.readLine();
      if (line == null) {
        break;
      }
 else {
        out.println(line);
      }
    }
  }
 catch (  IOException e) {
    throw new MojoExecutionException("Failed: " + e,e);
  }
 finally {
    close(reader,"cmapx file");
  }
}
