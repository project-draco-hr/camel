{
  String name=file.getName();
  if (regexPattern != null && regexPattern.length() > 0) {
    if (!name.matches(getRegexPattern())) {
      return false;
    }
  }
  String[] prefixes=endpoint.getExcludedNamePrefixes();
  for (  String prefix : prefixes) {
    if (name.startsWith(prefix)) {
      return false;
    }
  }
  return true;
}
