{
  for (  File file : files) {
    String name=file.getName();
    if (name.endsWith(FileComponent.DEFAULT_LOCK_FILE_POSTFIX)) {
      String before=ObjectHelper.before(name,FileComponent.DEFAULT_LOCK_FILE_POSTFIX);
      if (target.getName().equals(before)) {
        return true;
      }
    }
  }
  return false;
}
