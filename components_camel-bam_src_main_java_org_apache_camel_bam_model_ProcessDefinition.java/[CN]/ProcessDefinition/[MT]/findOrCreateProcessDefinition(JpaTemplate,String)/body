{
  List<ProcessDefinition> list=CastUtils.cast(template.find("select x from " + ProcessDefinition.class.getName() + " x where x.name = ?1",processName));
  if (!list.isEmpty()) {
    return list.get(0);
  }
 else {
    ProcessDefinition answer=new ProcessDefinition();
    answer.setName(processName);
    template.persist(answer);
    return answer;
  }
}
