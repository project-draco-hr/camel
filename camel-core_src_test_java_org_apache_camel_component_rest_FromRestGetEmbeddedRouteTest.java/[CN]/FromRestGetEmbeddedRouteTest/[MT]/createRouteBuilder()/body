{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      rest().path("/say").get("/hello").routeId("hello").to("mock:hello").transform(constant("Hello World")).get("/bye").accept("application/json").routeId("bye").to("mock:bye").transform(constant("Bye World")).post().to("mock:update");
    }
  }
;
}
