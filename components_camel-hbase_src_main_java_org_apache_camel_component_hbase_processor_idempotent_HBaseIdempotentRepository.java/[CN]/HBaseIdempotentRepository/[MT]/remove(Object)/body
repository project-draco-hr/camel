{
  try {
    byte[] b=toBytes(o);
    if (table.exists(new Get(b))) {
      Delete delete=new Delete(b);
      table.delete(delete);
      return true;
    }
 else {
      return false;
    }
  }
 catch (  Exception e) {
    LOG.warn("Error removing object {} from HBase repository.",o);
    return false;
  }
}
