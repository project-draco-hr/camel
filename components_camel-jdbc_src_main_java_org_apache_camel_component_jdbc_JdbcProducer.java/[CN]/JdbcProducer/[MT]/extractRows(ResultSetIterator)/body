{
  try {
    List<Map<String,Object>> result=new ArrayList<Map<String,Object>>();
    int maxRowCount=readSize == 0 ? Integer.MAX_VALUE : readSize;
    for (int i=0; iterator.hasNext() && i < maxRowCount; i++) {
      result.add(iterator.next());
    }
    return result;
  }
  finally {
    iterator.close();
  }
}
