{
  objects=new ArrayBlockingQueue<T>(getMaxSize(),false);
  for (int i=0; i < maxSize; i++) {
    try {
      T t=createObject();
      objects.add(t);
      poolCount.incrementAndGet();
    }
 catch (    Exception e) {
      logger.error("Unable to create Object and add it to the pool. Reason: " + e.getLocalizedMessage(),e);
    }
  }
}
