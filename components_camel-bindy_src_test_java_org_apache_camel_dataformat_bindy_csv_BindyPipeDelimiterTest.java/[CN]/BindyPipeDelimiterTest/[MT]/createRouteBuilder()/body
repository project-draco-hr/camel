{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:unmarshal").unmarshal().bindy(BindyType.Csv,"org.apache.camel.dataformat.bindy.model.simple.pipeline").to("mock:result");
      from("direct:marshal").marshal().bindy(BindyType.Csv,"org.apache.camel.dataformat.bindy.model.simple.pipeline").to("mock:result");
    }
  }
;
}
