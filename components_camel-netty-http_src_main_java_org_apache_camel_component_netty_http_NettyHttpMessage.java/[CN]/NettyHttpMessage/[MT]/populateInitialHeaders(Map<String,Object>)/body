{
  try {
    if (httpRequest != null) {
      httpBinding.populateCamelHeaders(httpRequest,map,getExchange());
    }
 else {
      httpBinding.populateCamelHeaders(httpResponse,map,getExchange());
    }
  }
 catch (  Exception e) {
    throw new RuntimeCamelException("Error populating initial headers",e);
  }
}
