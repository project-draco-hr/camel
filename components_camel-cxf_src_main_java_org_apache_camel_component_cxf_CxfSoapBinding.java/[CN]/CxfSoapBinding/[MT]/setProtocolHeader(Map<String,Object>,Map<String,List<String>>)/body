{
  if (protocolHeader != null) {
    StringBuilder value=new StringBuilder(256);
    for (    Map.Entry<String,List<String>> entry : protocolHeader.entrySet()) {
      value.setLength(0);
      boolean first=true;
      for (      String s : entry.getValue()) {
        if (!first) {
          value.append("; ");
        }
        value.append(s);
        first=false;
      }
      headers.put(entry.getKey(),value.toString());
    }
  }
}
