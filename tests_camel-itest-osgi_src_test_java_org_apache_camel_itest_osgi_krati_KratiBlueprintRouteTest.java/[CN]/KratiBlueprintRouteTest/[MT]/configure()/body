{
  Option[] options=combine(getDefaultCamelKarafOptions(),loadCamelFeatures("camel-blueprint","camel-krati"),provision(TinyBundles.bundle().add(SomeObject.class).add("META-INF/persistence.xml",KratiBlueprintRouteTest.class.getResource("/META-INF/persistence.xml")).add("OSGI-INF/blueprint/test.xml",KratiBlueprintRouteTest.class.getResource("blueprintCamelContext.xml")).set(Constants.BUNDLE_SYMBOLICNAME,"CamelBlueprintKratiTestBundle").set(Constants.DYNAMICIMPORT_PACKAGE,"*").build()));
  return options;
}
