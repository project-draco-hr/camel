{
  return new RouteBuilder(){
    public void configure(){
      from(String.format("mina:tcp://localhost:%1$s?sync=false&textline=true&textlineDelimiter=UNIX&clientMode=true",getPort())).id("minaRoute").noAutoStartup().to("log:before?showAll=true").to("mock:result").to("log:after?showAll=true");
    }
  }
;
}
