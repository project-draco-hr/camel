{
  super.setUp();
  if (context != null) {
    context.stop();
  }
  ApplicationContext c=new ClassPathXmlApplicationContext("org/apache/camel/spring/config/scan/componentScan.xml");
  context=(ModelCamelContext)c.getBean("camelScan");
  template=context.createProducerTemplate();
  template.start();
}
