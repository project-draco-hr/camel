{
  from("file:src/data?noop=true").convertBodyTo(PersonDocument.class).to("jpa:org.apache.camel.example.etl.CustomerEntity");
  from("jpa:org.apache.camel.example.etl.CustomerEntity?consumeDelete=false&consumer.delay=3000&consumeLockEntity=false").setHeader(FileComponent.HEADER_FILE_NAME,el("${in.body.userName}.xml")).to("file:target/customers?append=false");
}
