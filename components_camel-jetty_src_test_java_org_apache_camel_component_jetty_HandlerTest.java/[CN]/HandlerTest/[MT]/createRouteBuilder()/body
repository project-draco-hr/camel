{
  return new RouteBuilder(){
    public void configure() throws Exception {
      from("jetty:http://localhost:9080/?handlers=#statisticsHandler1").process(new Processor(){
        public void process(        Exchange exchange) throws Exception {
          exchange.getOut().setBody(htmlResponse);
        }
      }
);
      from("jetty:http://localhost:9081/?handlers=#statisticsHandler2,#statisticsHandler3").process(new Processor(){
        public void process(        Exchange exchange) throws Exception {
          exchange.getOut().setBody(htmlResponse);
        }
      }
);
    }
  }
;
}
