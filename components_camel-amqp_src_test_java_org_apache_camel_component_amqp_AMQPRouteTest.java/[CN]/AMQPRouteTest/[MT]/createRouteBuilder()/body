{
  return new RouteBuilder(){
    public void configure() throws Exception {
      from("amqp:queue:ping").to("log:routing").to("mock:result");
      from("amqp:queue:inOut").setBody().constant("response");
      from("amqp:topic:ping").to("log:routing").to("mock:result");
      from("amqp:topic:ping").to("log:routing").to("mock:result");
      from("amqp:queue:wildcard.>").to("log:routing").to("mock:result");
    }
  }
;
}
