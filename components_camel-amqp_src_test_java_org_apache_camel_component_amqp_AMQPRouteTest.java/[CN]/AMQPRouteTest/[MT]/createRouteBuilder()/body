{
  return new RouteBuilder(){
    public void configure() throws Exception {
      from("amqp1-0:queue:ping").to("log:routing").to("mock:result");
      from("amqp1-0:queue:inOut").setBody().constant("response");
      from("amqp1-0:topic:ping").to("log:routing").to("mock:result");
      from("amqp1-0:topic:ping").to("log:routing").to("mock:result");
    }
  }
;
}
