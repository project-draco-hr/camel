{
  if (msgInfo != null) {
    int size=msgInfo.getMessageParts().size();
    for (int x=0; x < size; x++) {
      if (x < (size - 1)) {
        msgInfo.getMessageParts().get(x).setTypeClass(DOMSource.class);
      }
 else {
        msgInfo.getMessageParts().get(x).setTypeClass(Source.class);
      }
    }
  }
}
