{
  return new RouteBuilder(){
    public void configure() throws Exception {
      from("mina:tcp://localhost:4444?sync=true").choice().when(body().isEqualTo("Copenhagen")).transform(constant("Hello Claus")).otherwise().transform(constant(null));
      from("mina:tcp://localhost:4445?sync=true&disconnectOnNoReply=false&noReplyLogLevel=OFF").choice().when(body().isEqualTo("Copenhagen")).transform(constant("Hello Claus")).otherwise().transform(constant(null));
    }
  }
;
}
