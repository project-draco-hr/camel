{
  if (endpoint.isSynchronous()) {
    try {
      process(exchange);
    }
 catch (    Throwable e) {
      exchange.setException(e);
    }
    callback.done(true);
    return true;
  }
  try {
    ProducerRecord record=createRecorder(exchange);
    kafkaProducer.send(record,new KafkaProducerCallBack(exchange,callback));
    return false;
  }
 catch (  Exception ex) {
    exchange.setException(ex);
    callback.done(true);
    return true;
  }
}
