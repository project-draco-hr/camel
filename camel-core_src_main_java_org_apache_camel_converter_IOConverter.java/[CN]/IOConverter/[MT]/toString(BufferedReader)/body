{
  if (reader == null) {
    return null;
  }
  try {
    CollectionStringBuffer builder=new CollectionStringBuffer("\n");
    while (true) {
      String line=reader.readLine();
      if (line == null) {
        return builder.toString();
      }
      builder.append(line);
    }
  }
  finally {
    try {
      reader.close();
    }
 catch (    IOException e) {
      LOG.warn("Failed to close stream: " + e,e);
    }
  }
}
