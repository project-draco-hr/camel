{
  if (reader == null) {
    return null;
  }
  try {
    CollectionStringBuffer builder=new CollectionStringBuffer("\n");
    while (true) {
      String line=reader.readLine();
      if (line == null) {
        return builder.toString();
      }
      builder.append(line);
    }
  }
  finally {
    ObjectHelper.close(reader,"reader",LOG);
  }
}
