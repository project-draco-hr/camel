{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("file:src/test/resources/org/apache/camel/builder/zip/?consumer.delay=1000&noop=true").log("Start processing big file: ${header.CamelFileName}").split(new ZipSplitter()).streaming().convertBodyTo(String.class).to("mock:processZipEntry").end().log("Done processing big file: ${header.CamelFileName}");
    }
  }
;
}
