{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:forceCompletionTrue").aggregate(header("id"),new BodyInAggregatingStrategy()).forceCompletionOnStop().completionSize(10).delay(100).process(myCompletionProcessor);
      from("direct:forceCompletionFalse").aggregate(header("id"),new BodyInAggregatingStrategy()).completionSize(10).delay(100).process(myCompletionProcessor);
    }
  }
;
}
