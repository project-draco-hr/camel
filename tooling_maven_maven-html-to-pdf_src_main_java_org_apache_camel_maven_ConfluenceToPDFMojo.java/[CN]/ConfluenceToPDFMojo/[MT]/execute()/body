{
  File outputDir=new File(pdf).getParentFile();
  if (!outputDir.exists()) {
    outputDir.mkdirs();
  }
  try {
    String content=downloadContent();
    storeHTMLFile(content);
    convert();
  }
 catch (  MojoExecutionException e) {
    throw e;
  }
catch (  Exception e) {
    throw new MojoExecutionException("Download of '" + page + "' failed: "+ e.getMessage(),e);
  }
}
