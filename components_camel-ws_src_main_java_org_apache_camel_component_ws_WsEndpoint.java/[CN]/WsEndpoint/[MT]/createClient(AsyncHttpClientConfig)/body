{
  AsyncHttpClient client;
  if (config == null) {
    config=new AsyncHttpClientConfig.Builder().build();
  }
  AsyncHttpProvider ahp=getAsyncHttpProvider(config);
  if (ahp == null) {
    client=new AsyncHttpClient(config);
  }
 else {
    client=new AsyncHttpClient(ahp,config);
  }
  return client;
}
