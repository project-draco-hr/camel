{
  try (InputStream isNotice=getClass().getResourceAsStream("/spring-boot-starter-NOTICE.txt");FileWriter outNotice=new FileWriter(new File(starterDir(),"src/main/resources/META-INF/NOTICE.txt"));InputStream isLicense=getClass().getResourceAsStream("/spring-boot-starter-LICENSE.txt");FileWriter outLicense=new FileWriter(new File(starterDir(),"src/main/resources/META-INF/LICENSE.txt"))){
    IOUtils.copy(isNotice,outNotice);
    IOUtils.copy(isLicense,outLicense);
  }
 }
