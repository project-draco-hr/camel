{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:start").hystrix().configure().requestCacheEnabled(false).corePoolSize(5).end().to("direct:foo").fallback().transform().constant("Fallback message").end().to("mock:result");
      from("direct:foo").transform().constant("Bye World");
    }
  }
;
}
