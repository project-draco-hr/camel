{
  Set<T> set=new HashSet<>();
  for (  T qualifier : annotations) {
    boolean exclude=false;
    for (    Class<? extends T> exclusion : exclusions) {
      if (exclusion.isAssignableFrom(qualifier.getClass())) {
        exclude=true;
        break;
      }
    }
    if (!exclude) {
      set.add(qualifier);
    }
  }
  return set;
}
