{
  Set<T> set=new HashSet<>();
  for (  T annotation : annotations) {
    boolean exclude=false;
    for (    Class<? extends T> exclusion : exclusions) {
      if (exclusion.isAssignableFrom(annotation.getClass())) {
        exclude=true;
        break;
      }
    }
    if (!exclude) {
      set.add(annotation);
    }
  }
  return set;
}
