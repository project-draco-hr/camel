{
  if ("common".equals(group)) {
    return 1;
  }
 else   if ("common (advanced)".equals(group)) {
    return 2;
  }
 else   if ("consumer".equals(group)) {
    return 3;
  }
 else   if ("consumer (advanced)".equals(group)) {
    return 4;
  }
 else   if ("producer".equals(group)) {
    return 5;
  }
 else   if ("producer (advanced)".equals(group)) {
    return 6;
  }
 else {
    return 9;
  }
}
