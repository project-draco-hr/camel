{
  if (isPlatform("aix")) {
    return;
  }
  Endpoint endpoint=context.getEndpoint("direct:start");
  Producer producer=endpoint.createProducer();
  Exchange exchange=endpoint.createExchange();
  exchange.getIn().setBody("Hello World");
  producer.start();
  producer.process(exchange);
  producer.stop();
  context.stop();
}
