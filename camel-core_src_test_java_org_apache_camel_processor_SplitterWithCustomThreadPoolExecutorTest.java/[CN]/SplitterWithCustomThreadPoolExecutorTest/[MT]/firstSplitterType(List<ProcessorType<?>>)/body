{
  SplitterType result=null;
  for (  ProcessorType processorType : outputs) {
    if (processorType instanceof SplitterType) {
      result=(SplitterType)processorType;
    }
 else {
      result=firstSplitterType(processorType.getOutputs());
    }
    if (result != null) {
      break;
    }
  }
  return result;
}
