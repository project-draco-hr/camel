{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  DefaultContextPathMatcher that=(DefaultContextPathMatcher)o;
  if (matchOnUriPrefix != that.matchOnUriPrefix) {
    return false;
  }
  if (!path.equals(that.path)) {
    return false;
  }
  return true;
}
