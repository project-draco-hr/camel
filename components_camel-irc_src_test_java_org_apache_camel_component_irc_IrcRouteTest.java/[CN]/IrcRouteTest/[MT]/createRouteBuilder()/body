{
  return new RouteBuilder(){
    public void configure() throws Exception {
      from("irc://camel-con@irc.codehaus.org:6667/#camel-test").choice().when(header(IrcConstants.IRC_MESSAGE_TYPE).isEqualTo("PRIVMSG")).to("mock:result").when(header(IrcConstants.IRC_MESSAGE_TYPE).isEqualTo("JOIN")).to("seda:consumerJoined");
      from("seda:consumerJoined").process(new Processor(){
        public void process(        Exchange exchange) throws Exception {
          sendMessages();
        }
      }
);
    }
  }
;
}
