{
  WeightedLoadBalancer loadBalancer=null;
  try {
    if (!roundRobin) {
      loadBalancer=new WeightedRandomLoadBalancer(distributionRatioList);
    }
 else {
      loadBalancer=new WeightedRoundRobinLoadBalancer(distributionRatioList);
    }
  }
 catch (  Exception e) {
  }
  return loadBalancer;
}
