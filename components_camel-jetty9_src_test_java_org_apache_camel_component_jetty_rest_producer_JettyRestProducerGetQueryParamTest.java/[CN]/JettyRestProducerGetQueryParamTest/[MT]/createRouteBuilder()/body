{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      SwaggerComponent sc=new SwaggerComponent();
      sc.setComponentName("jetty");
      context.addComponent("swagger",sc);
      String host="localhost:" + getPort();
      from("direct:start").toF("swagger:get:bye?host=%s&apiDoc=%s",host,"hello-api.json").to("mock:result");
      from("jetty:http://localhost:{{port}}/api/bye/?matchOnUriPrefix=true").transform().simple("Bye ${header.name}");
    }
  }
;
}
