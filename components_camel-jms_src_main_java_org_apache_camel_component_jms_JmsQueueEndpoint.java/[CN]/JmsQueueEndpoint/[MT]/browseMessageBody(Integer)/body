{
  List<Exchange> exchanges=getExchanges();
  if (index >= exchanges.size()) {
    return null;
  }
  Exchange exchange=exchanges.get(index);
  if (exchange == null) {
    return null;
  }
  Object body;
  if (exchange.hasOut()) {
    body=exchange.getOut().getBody();
  }
 else {
    body=exchange.getIn().getBody();
  }
  return body != null ? body.toString() : null;
}
