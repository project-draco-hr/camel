{
  List<Exchange> exchanges=getExchanges();
  if (index >= exchanges.size()) {
    return null;
  }
  Exchange exchange=exchanges.get(index);
  if (exchange == null) {
    return null;
  }
  String body;
  if (exchange.hasOut()) {
    body=exchange.getOut().getBody(String.class);
  }
 else {
    body=exchange.getIn().getBody(String.class);
  }
  return body;
}
