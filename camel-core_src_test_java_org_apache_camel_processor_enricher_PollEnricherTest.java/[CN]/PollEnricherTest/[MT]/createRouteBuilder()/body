{
  return new RouteBuilder(){
    public void configure(){
      from("direct:enricher-test-1").pollEnrich("direct:foo1",aggregationStrategy).to("mock:mock");
      from("direct:enricher-test-2").pollEnrich("direct:foo2",1000,aggregationStrategy).to("mock:mock");
      from("direct:enricher-test-3").pollEnrich("direct:foo3",-1,aggregationStrategy).to("mock:mock");
      from("direct:enricher-test-4").pollEnrich("direct:foo4",aggregationStrategy);
    }
  }
;
}
