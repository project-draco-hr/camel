{
  headers.clear();
  Set<Map.Entry<String,Object>> entries=message.getHeaders().entrySet();
  for (  Map.Entry<String,Object> entry : entries) {
    Object value=entry.getValue();
    if (value != null) {
      headers.add(createHeader(entry.getKey(),value));
    }
  }
  setBody(message.getBody());
}
