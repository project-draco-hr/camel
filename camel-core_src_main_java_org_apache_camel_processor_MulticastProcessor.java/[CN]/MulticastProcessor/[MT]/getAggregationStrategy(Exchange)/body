{
  AggregationStrategy answer=null;
  if (exchange != null) {
    Map property=exchange.getProperty(Exchange.AGGREGATION_STRATEGY,Map.class);
    Map<Object,AggregationStrategy> map=CastUtils.cast(property);
    if (map != null) {
      answer=map.get(this);
    }
  }
  if (answer == null) {
    answer=getAggregationStrategy();
  }
  return answer;
}
