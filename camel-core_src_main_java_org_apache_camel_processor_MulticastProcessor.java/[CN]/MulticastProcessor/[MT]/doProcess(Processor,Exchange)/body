{
  TracedRouteNodes traced=exchange.getUnitOfWork() != null ? exchange.getUnitOfWork().getTracedRouteNodes() : null;
  try {
    if (traced != null) {
      traced.pushBlock();
    }
    setToEndpoint(exchange,producer);
    producer.process(exchange);
  }
 catch (  Exception e) {
    exchange.setException(e);
  }
 finally {
    if (traced != null) {
      traced.popBlock();
    }
  }
}
