{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("seda:start").delay(50).to("sjms:in.out.temp.queue?exchangePattern=InOut").to("mock:result");
      from("sjms:in.out.temp.queue?exchangePattern=InOut").log("Using ${threadName} to process ${body}").delay(50).transform(body().prepend("Bye "));
    }
  }
;
}
