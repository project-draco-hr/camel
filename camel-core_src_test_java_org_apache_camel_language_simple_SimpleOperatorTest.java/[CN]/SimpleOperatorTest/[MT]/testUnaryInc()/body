{
  assertExpression("${in.header.bar}++",124);
  assertExpression("+++++++++++++","+++++++++++++");
  assertExpression("Logging ++ start ++","Logging ++ start ++");
  assertExpression("Logging +++ start +++","Logging +++ start +++");
  assertExpression("++ start ++","++ start ++");
  assertExpression("+++ start +++","+++ start +++");
}
