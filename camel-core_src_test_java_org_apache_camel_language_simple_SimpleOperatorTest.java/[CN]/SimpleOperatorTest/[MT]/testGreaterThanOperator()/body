{
  assertPredicate("${in.header.foo} > 'aaa'",true);
  assertPredicate("${in.header.foo} > 'def'",false);
  assertPredicate("${in.header.bar} > '100'",true);
  assertPredicate("${in.header.bar} > 100",true);
  assertPredicate("${in.header.bar} > '123'",false);
  assertPredicate("${in.header.bar} > 123",false);
  assertPredicate("${in.header.bar} > '200'",false);
}
