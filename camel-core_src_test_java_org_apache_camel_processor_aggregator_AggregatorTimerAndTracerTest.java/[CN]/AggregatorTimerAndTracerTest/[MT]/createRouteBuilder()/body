{
  return new RouteBuilder(){
    public void configure() throws Exception {
      getContext().addInterceptStrategy(new Tracer());
      from("timer://kickoff?period=9999910000").setHeader("id").constant("foo").setBody().constant("a b c").splitter(body().tokenize(" ")).to("seda:splitted");
      from("seda:splitted").aggregator(header("id")).to("mock:result");
    }
  }
;
}
