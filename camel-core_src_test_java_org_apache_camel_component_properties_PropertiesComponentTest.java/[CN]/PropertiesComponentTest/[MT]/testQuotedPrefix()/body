{
  assertEquals("mock",context.resolvePropertyPlaceholders("{{cool.mock}}"));
  assertEquals("'{{' + something + '}}'",context.resolvePropertyPlaceholders("'{{' + something + '}}'"));
  assertEquals("\"{{\" + something + \"}}\"",context.resolvePropertyPlaceholders("\"{{\" + something + \"}}\""));
  assertEquals("mock'",context.resolvePropertyPlaceholders("{{cool.mock}}'"));
  assertEquals("mock\"",context.resolvePropertyPlaceholders("{{cool.mock}}\""));
  assertEquals("'mock",context.resolvePropertyPlaceholders("'{{cool.mock}}"));
  assertEquals("\"mock",context.resolvePropertyPlaceholders("\"{{cool.mock}}"));
}
