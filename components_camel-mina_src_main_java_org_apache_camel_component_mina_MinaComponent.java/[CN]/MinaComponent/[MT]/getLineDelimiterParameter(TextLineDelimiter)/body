{
  if (delimiter == null) {
    return LineDelimiter.AUTO;
  }
switch (delimiter) {
case AUTO:
    return LineDelimiter.AUTO;
case UNIX:
  return LineDelimiter.UNIX;
case WINDOWS:
return LineDelimiter.WINDOWS;
case MAC:
return LineDelimiter.MAC;
default :
throw new IllegalArgumentException("Unknown textline delimiter: " + delimiter);
}
}
