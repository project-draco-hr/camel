{
  Date updated=entry.getUpdated();
  if (updated == null) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("No updated time for entry so assuming its valid: entry=[" + entry + "]");
    }
    return true;
  }
  if (lastUpdate != null) {
    if (lastUpdate.after(updated)) {
      if (LOG.isDebugEnabled()) {
        LOG.debug("Entry is older than lastupdate=[" + lastUpdate + "], no valid entry=["+ entry+ "]");
      }
      return false;
    }
  }
  lastUpdate=updated;
  return true;
}
