{
  if (LOG.isTraceEnabled()) {
    LOG.trace("Executing work " + work);
  }
  Transaction tx=pageFile.tx();
  try {
    T rc=work.execute(tx);
    tx.commit();
    return rc;
  }
 catch (  RuntimeException e) {
    if (LOG.isTraceEnabled()) {
      LOG.trace("Error executing work " + work + " will do rollback",e);
    }
    tx.rollback();
    throw e;
  }
}
