{
  if (LOG.isDebugEnabled()) {
    LOG.debug("Service {} changed to {}",event,event.getType());
  }
  if (event.getType() == ServiceEvent.REGISTERED && event.getServiceReference().isAssignableTo(bundleContext.getBundle(),"org.osgi.service.blueprint.container.BlueprintContainer")) {
    try {
      maybeStart();
    }
 catch (    Exception e) {
      LOG.warn("Error occurred during starting " + this,e);
    }
  }
}
