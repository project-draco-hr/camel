{
  try {
    Object newProperty=expression.evaluate(exchange,Object.class);
    if (exchange.getException() != null) {
      callback.done(true);
      return true;
    }
    exchange.setProperty(propertyName,newProperty);
  }
 catch (  Exception e) {
    exchange.setException(e);
  }
  callback.done(true);
  return true;
}
