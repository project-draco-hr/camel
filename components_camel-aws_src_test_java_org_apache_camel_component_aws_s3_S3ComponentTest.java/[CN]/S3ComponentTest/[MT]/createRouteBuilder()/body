{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:start").to("aws-s3://mycamelbucket?amazonS3Client=#amazonS3Client&region=us-west-1");
      from("aws-s3://mycamelbucket?amazonS3Client=#amazonS3Client&region=us-west-1&maxMessagesPerPoll=5").to("mock:result");
    }
  }
;
}
