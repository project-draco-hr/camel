{
  Option[] options=combine(getDefaultCamelKarafOptions(),scanFeatures(getCamelKarafFeatureUrl("2.8.0"),"camel-core"),bundle(TinyBundles.bundle().add("OSGI-INF/blueprint/fileRoute.xml",OSGiBlueprintTestSupport.class.getResource("fileRouteBlueprint.xml")).set(Constants.BUNDLE_SYMBOLICNAME,"CamelBlueprintTestBundle1").build()).noStart(),loadCamelFeatures("camel-blueprint","camel-test"));
