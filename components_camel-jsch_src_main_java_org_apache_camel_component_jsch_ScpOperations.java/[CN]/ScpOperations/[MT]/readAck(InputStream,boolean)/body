{
  String message;
  int answer=is.read();
switch (answer) {
case -1:
    if (failOnEof) {
      message="[scp] Unexpected end of stream";
      LOG.info(message);
      throw new EOFException(message);
    }
  break;
case 1:
message="[scp] WARN " + readLine(is);
LOG.info(message);
break;
case 2:
message="[scp] NACK " + readLine(is);
LOG.info(message);
throw new IOException(message);
default :
break;
}
return answer;
}
