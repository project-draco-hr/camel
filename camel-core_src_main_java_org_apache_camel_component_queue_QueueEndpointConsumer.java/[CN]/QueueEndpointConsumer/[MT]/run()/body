{
  while (!isStopping()) {
    E exchange;
    try {
      exchange=endpoint.getQueue().poll(100,TimeUnit.MILLISECONDS);
    }
 catch (    InterruptedException e) {
      break;
    }
    if (exchange != null) {
      try {
        processor.process(exchange);
      }
 catch (      Throwable e) {
        e.printStackTrace();
      }
    }
  }
}
