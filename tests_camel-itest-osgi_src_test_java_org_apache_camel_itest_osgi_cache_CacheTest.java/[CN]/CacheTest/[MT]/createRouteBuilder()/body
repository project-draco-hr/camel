{
  return new RouteBuilder(){
    public void configure(){
      from("direct:add").setHeader("CACHE_OPERATION",constant("ADD")).setHeader("CACHE_KEY",constant("foo")).to("cache:foo");
      from("direct:get").setHeader("CACHE_OPERATION",constant("GET")).setHeader("CACHE_KEY",constant("foo")).to("cache:foo").to("mock:result");
    }
  }
;
}
