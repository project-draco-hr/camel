{
  Exception e;
  try {
    tryProcessor.process(exchange);
    e=exchange.getException();
    if (e != null && ExchangeHelper.isFailureHandled(exchange)) {
      e=null;
    }
  }
 catch (  Exception ex) {
    e=ex;
    exchange.setException(e);
  }
  try {
    if (e != null) {
      handleException(exchange,e);
    }
  }
  finally {
    processFinally(exchange);
  }
}
