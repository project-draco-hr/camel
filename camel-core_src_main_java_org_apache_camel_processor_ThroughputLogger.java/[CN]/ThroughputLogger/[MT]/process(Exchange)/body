{
  if (startTime == 0) {
    startTime=System.currentTimeMillis();
  }
  int receivedCount=receivedCounter.incrementAndGet();
  if (groupSize != null) {
    if (receivedCount % groupSize == 0) {
      lastLogMessage=createLogMessage(exchange,receivedCount);
      log.log(lastLogMessage);
    }
  }
}
