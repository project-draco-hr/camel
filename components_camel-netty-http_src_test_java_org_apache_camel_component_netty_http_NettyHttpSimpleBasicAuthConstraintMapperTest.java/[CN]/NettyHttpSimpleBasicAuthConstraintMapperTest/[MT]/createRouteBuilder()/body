{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("netty-http:http://0.0.0.0:{{port}}/foo?matchOnUriPrefix=true" + "&securityConfiguration.realm=karaf&securityConfiguration.constraintMapping=#myConstraint").to("mock:input").transform().constant("Bye World");
    }
  }
;
}
