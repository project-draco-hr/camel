{
  try {
    String msg=CamelExchangeException.createExceptionMessage(message,exchange,exception);
    if (isCausedByRollbackExchangeException(exception)) {
      logger.log(msg);
    }
 else {
      if (exception != null) {
        logger.log(msg,exception);
      }
 else {
        logger.log(msg);
      }
    }
  }
 catch (  Throwable e) {
  }
}
