{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("sftp://localhost:" + getPort() + "/"+ FTP_ROOT_DIR+ "?username=admin&knownHostsFile=./src/test/resources/known_hosts&privateKeyFile=./src/test/resources/id_rsa&privateKeyFilePassphrase=secret&delay=10s&disconnect=true").routeId("foo").noAutoStartup().to("mock:result");
    }
  }
;
}
