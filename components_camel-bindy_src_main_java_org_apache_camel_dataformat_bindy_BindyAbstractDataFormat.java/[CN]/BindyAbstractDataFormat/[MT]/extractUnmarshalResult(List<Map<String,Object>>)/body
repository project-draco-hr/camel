{
  if (getClassType() != null) {
    List<Object> answer=new ArrayList<Object>();
    for (    Map<String,Object> entry : models) {
      Object data=entry.get(getClassType().getName());
      if (data != null) {
        answer.add(data);
      }
    }
    if (answer.size() == 1) {
      return answer.get(0);
    }
 else {
      return answer;
    }
  }
 else {
    return models;
  }
}
