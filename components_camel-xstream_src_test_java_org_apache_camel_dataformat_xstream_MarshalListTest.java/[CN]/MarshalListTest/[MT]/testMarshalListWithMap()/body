{
  MockEndpoint mock=getMockEndpoint("mock:result");
  mock.expectedMessageCount(1);
  mock.expectedBodiesReceived("<?xml version='1.0' encoding='UTF-8'?><list><map><entry><string>city</string>" + "<string>London</string></entry></map></list>");
  List<Map<Object,String>> body=new ArrayList<Map<Object,String>>();
  Map<Object,String> row=new HashMap<Object,String>();
  row.put("city","London");
  body.add(row);
  template.sendBody("direct:in",body);
  mock.assertIsSatisfied();
}
