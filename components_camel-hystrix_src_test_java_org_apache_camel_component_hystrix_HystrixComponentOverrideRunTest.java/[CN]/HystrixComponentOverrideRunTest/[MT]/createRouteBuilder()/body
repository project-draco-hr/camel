{
  return new RouteBuilder(){
    public void configure(){
      from("direct:fallback").to("mock:error");
      from("direct:run").to("mock:result");
      from("direct:run2").to("mock:run2");
      from("direct:start").to("hystrix:testKey?runEndpoint=direct:run&fallbackEndpoint=direct:fallback&cacheKeyExpression=#headerExpression&initializeRequestContext=true");
    }
  }
;
}
