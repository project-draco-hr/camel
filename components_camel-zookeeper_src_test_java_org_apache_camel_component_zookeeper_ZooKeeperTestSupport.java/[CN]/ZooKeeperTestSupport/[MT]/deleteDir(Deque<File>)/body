{
  File f=deleteStack.pollLast();
  if (f != null) {
    if (f.isDirectory()) {
      File[] files=f.listFiles();
      if (files != null) {
        for (        File child : files) {
          deleteStack.addLast(child);
        }
      }
    }
    deleteDir(deleteStack);
    FileUtil.deleteFile(f);
  }
}
