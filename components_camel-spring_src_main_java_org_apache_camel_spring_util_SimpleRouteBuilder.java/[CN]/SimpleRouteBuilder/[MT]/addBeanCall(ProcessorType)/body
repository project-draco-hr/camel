{
  Class type=getBeanType();
  if (type != null) {
    if (beanMethod != null) {
      route=route.bean(type,beanMethod);
    }
 else {
      route=route.bean(type);
    }
  }
 else   if (beanRef != null) {
    if (beanMethod != null) {
      route=route.beanRef(beanRef,beanMethod);
    }
 else {
      route=route.beanRef(beanRef);
    }
  }
}
