{
  Customer input=new Customer();
  input.setName("Donald Duck");
  String response=template.requestBodyAndHeader("cxfrs:http://localhost:9002/customerservice/customers?throwExceptionOnFailure=true",input,Exchange.HTTP_METHOD,"POST",String.class);
  assertNotNull(response);
  assertTrue(response.endsWith("<name>Donald Duck</name></Customer>"));
}
