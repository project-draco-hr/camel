{
  mllpPort=AvailablePortFinder.getNextAvailable();
  return new RouteBuilder(){
    String routeId="mllp-receiver";
    String host="0.0.0.0";
    int port=mllpPort;
    public void configure(){
      fromF("mllp:%d?autoAck=false",port).log(LoggingLevel.INFO,routeId,"Receiving: ${body}").to(result);
    }
  }
;
}
