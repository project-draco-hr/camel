{
  if (eventObject instanceof ExchangeFailureEvent || eventObject instanceof CamelContextStartupFailureEvent || eventObject instanceof CamelContextStopFailureEvent|| eventObject instanceof ServiceStartupFailureEvent|| eventObject instanceof ServiceStopFailureEvent) {
    return Level.CRITICAL;
  }
  if (eventObject instanceof ExchangeFailureHandledEvent) {
    return Level.WARNING;
  }
  return Level.OK;
}
