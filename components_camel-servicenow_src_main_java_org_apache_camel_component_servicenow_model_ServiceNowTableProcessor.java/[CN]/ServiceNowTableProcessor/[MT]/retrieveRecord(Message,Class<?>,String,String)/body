{
  if (sysId == null) {
    setBody(in,model,client.retrieveRecord(tableName,in.getHeader(ServiceNowConstants.SYSPARM_QUERY,String.class),in.getHeader(ServiceNowConstants.SYSPARM_DISPLAY_VALUE,config.getDisplayValue(),String.class),in.getHeader(ServiceNowConstants.SYSPARM_EXCLUDE_REFERENCE_LINK,config.getExcludeReferenceLink(),Boolean.class),in.getHeader(ServiceNowConstants.SYSPARM_FIELDS,String.class),in.getHeader(ServiceNowConstants.SYSPARM_LIMIT,Integer.class),in.getHeader(ServiceNowConstants.SYSPARM_VIEW,String.class)));
  }
 else {
    setBody(in,model,client.retrieveRecordById(tableName,ObjectHelper.notNull(sysId,"sysId"),in.getHeader(ServiceNowConstants.SYSPARM_DISPLAY_VALUE,config.getDisplayValue(),String.class),in.getHeader(ServiceNowConstants.SYSPARM_EXCLUDE_REFERENCE_LINK,config.getExcludeReferenceLink(),Boolean.class),in.getHeader(ServiceNowConstants.SYSPARM_FIELDS,String.class),in.getHeader(ServiceNowConstants.SYSPARM_VIEW,String.class)));
  }
}
