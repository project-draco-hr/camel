{
  getMockEndpoint("mock:intercept").expectedMessageCount(5);
  getMockEndpoint("mock:sync").expectedBodiesReceived("Bye World");
  getMockEndpoint("mock:sync").expectedPropertyReceived(Exchange.ON_COMPLETION,true);
