{
  Thread up=new Thread(new Runnable(){
    public void run(){
      for (int i=0; i < COUNT; i++) {
        latch.increment();
      }
    }
  }
);
  Thread down=new Thread(new Runnable(){
    public void run(){
      for (int i=0; i < COUNT; i++) {
        latch.increment();
        latch.decrement();
        sleep();
        latch.decrement();
      }
    }
  }
);
  up.start();
  down.start();
  assertLatchDone();
}
