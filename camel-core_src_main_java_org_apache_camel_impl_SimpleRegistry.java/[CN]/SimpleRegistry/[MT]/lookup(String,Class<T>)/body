{
  Object answer=lookup(name);
  if (answer == null) {
    return null;
  }
  try {
    return type.cast(answer);
  }
 catch (  Throwable e) {
    String msg="Found bean: " + name + " in SimpleRegistry: "+ this+ " of type: "+ answer.getClass().getName()+ " expected type was: "+ type;
    throw new NoSuchBeanException(name,msg,e);
  }
}
