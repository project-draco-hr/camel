{
  if (sObjectBase != null) {
    if (sObjectId != null) {
      sObjectBase.setId(sObjectId);
    }
    if (sObjectExtIdName != null && oldValue != null) {
      try {
        setPropertyValue(sObjectBase,sObjectExtIdName,oldValue);
      }
 catch (      SalesforceException e) {
        exchange.setException(e);
      }
    }
  }
}
