{
  if (ref != null) {
    String methodText="";
    if (method != null) {
      methodText=" method: " + method;
    }
    return "ref:" + ref + methodText;
  }
 else   if (bean != null) {
    return bean.toString();
  }
 else   if (beanType != null) {
    return beanType.getName();
  }
 else {
    return "";
  }
}
