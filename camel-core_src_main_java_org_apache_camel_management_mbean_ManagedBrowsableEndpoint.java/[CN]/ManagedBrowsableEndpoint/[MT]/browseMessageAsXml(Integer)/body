{
  if (index >= endpoint.getExchanges().size()) {
    return null;
  }
  Exchange exchange=endpoint.getExchanges().get(index);
  if (exchange == null) {
    return null;
  }
  Message msg=exchange.hasOut() ? exchange.getOut() : exchange.getIn();
  String xml=MessageHelper.dumpAsXml(msg);
  return xml;
}
