{
  NonXmlCharFilterer nonXmlCharFiltererMock=createMockBuilder(NonXmlCharFilterer.class).addMockedMethod("filter",char[].class,int.class,int.class).createStrictMock();
  String string="abc";
  expect(nonXmlCharFiltererMock.filter(aryEq(new char[]{'a','b','c'}),eq(0),eq(3))).andReturn(false);
  replayAll();
  String result=nonXmlCharFiltererMock.filter(string);
  verifyAll();
  assertSame("Should have returned the same string if nothing was filtered",string,result);
}
