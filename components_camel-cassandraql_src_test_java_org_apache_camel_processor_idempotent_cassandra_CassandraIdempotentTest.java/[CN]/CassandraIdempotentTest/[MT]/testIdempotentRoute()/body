{
  if (!canTest()) {
    return;
  }
  MockEndpoint mockOutput=getMockEndpoint("mock:output");
  mockOutput.expectedMessageCount(2);
  mockOutput.expectedBodiesReceivedInAnyOrder("A","B");
  send("1","A");
  send("2","B");
  send("1","A");
  send("2","B");
  send("1","A");
  mockOutput.assertIsSatisfied();
}
