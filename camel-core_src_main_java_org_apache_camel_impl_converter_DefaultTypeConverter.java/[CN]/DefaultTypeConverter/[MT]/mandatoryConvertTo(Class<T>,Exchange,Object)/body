{
  Object answer;
  try {
    answer=doConvertTo(type,exchange,value);
  }
 catch (  Exception e) {
    throw new NoTypeConversionAvailableException(value,type,e);
  }
  if (answer == Void.TYPE || value == null) {
    throw new NoTypeConversionAvailableException(value,type);
  }
 else {
    return (T)answer;
  }
}
