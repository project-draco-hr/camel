{
  Object answer;
  try {
    answer=doConvertTo(type,exchange,value);
  }
 catch (  CamelExecutionException e) {
    throw e;
  }
catch (  Exception e) {
    if (LOG.isDebugEnabled()) {
      LOG.debug(NoTypeConversionAvailableException.createMessage(value,type) + " Caused by: " + e.getMessage()+ ". Will ignore this and continue.");
    }
    return null;
  }
  if (answer == Void.TYPE) {
    return null;
  }
 else {
    return (T)answer;
  }
}
