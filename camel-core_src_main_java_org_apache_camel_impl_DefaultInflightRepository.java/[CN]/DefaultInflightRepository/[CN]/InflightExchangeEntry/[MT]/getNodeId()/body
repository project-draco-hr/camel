{
  List<MessageHistory> list=exchange.getProperty(Exchange.MESSAGE_HISTORY,List.class);
  if (list == null || list.isEmpty()) {
    return null;
  }
  MessageHistory history=list.get(list.size() - 1);
  if (history != null) {
    return history.getNode().getId();
  }
 else {
    return null;
  }
}
