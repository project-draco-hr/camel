{
  if (redeliveryDelay > 0) {
    if (log.isDebugEnabled()) {
      log.debug("Sleeping for: " + redeliveryDelay + " until attempting redelivery");
    }
    try {
      Thread.sleep(redeliveryDelay);
    }
 catch (    InterruptedException e) {
      if (log.isDebugEnabled()) {
        log.debug("Thread interupted: " + e,e);
      }
    }
  }
}
