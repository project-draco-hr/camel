{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      intercept().when(toPredicate(simple("${body} contains 'Goofy'"))).choice().when(body().contains("Hello")).to("mock:hello").otherwise().to("mock:goofy").end().stop();
      from("direct:start").to("mock:foo").to("mock:end");
    }
  }
;
}
