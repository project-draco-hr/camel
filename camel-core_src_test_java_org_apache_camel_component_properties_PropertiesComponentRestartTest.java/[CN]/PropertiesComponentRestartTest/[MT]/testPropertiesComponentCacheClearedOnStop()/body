{
  context.start();
  context.resolvePropertyPlaceholders("{{cool.end}}");
  context.resolvePropertyPlaceholders("{{cool.end}}");
  context.resolvePropertyPlaceholders("{{cool.end}}");
  assertEquals(1,resolvedCount);
  context.stop();
  context.start();
  context.resolvePropertyPlaceholders("{{cool.end}}");
  context.resolvePropertyPlaceholders("{{cool.end}}");
  context.resolvePropertyPlaceholders("{{cool.end}}");
  assertEquals(2,resolvedCount);
}
