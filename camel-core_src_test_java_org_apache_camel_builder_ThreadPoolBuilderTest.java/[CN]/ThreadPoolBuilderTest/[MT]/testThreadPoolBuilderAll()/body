{
  ThreadPoolBuilder builder=new ThreadPoolBuilder(context);
  ExecutorService executor=builder.poolSize(50).maxPoolSize(100).maxQueueSize(2000).keepAliveTime(20000).timeUnit(TimeUnit.MILLISECONDS).rejectedPolicy(ThreadPoolRejectedPolicy.DiscardOldest).build(this,"myPool");
  assertNotNull(executor);
  assertEquals(false,executor.isShutdown());
  context.stop();
  assertEquals(true,executor.isShutdown());
}
