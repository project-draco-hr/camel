{
  String json=context.explainEndpointJson("mock:foo?retainFirst=10",true);
  assertNotNull(json);
  assertTrue(json.contains("\"retainFirst\": { \"kind\": \"parameter\", \"group\": \"producer\", \"label\": \"producer\", \"type\": \"integer\"," + " \"javaType\": \"int\", \"deprecated\": \"false\", \"value\": \"10\""));
}
