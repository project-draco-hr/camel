{
  try {
    JndiContext context=new JndiContext(null);
    if (injector != null) {
      JndiBindings.bindInjectorAndBindings(context,injector,new Properties());
    }
    return context;
  }
 catch (  Exception e) {
    throw new ProvisionException("Failed to create JNDI bindings. Reason: " + e,e);
  }
}
