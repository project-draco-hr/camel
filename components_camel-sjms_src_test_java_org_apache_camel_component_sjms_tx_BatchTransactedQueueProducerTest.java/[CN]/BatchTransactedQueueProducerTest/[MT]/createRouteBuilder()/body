{
  return new RouteBuilder(){
    @Override public void configure(){
      from("direct:start").to("log:test-before?showAll=true").to("sjms:queue:batch.queue?transacted=true").to("mock:test.prebatch");
      from("sjms:queue:batch.queue").to("log:test-after?showAll=true").to("mock:test.postbatch");
    }
  }
;
}
