{
  property=getSurefirePropertyFromPom("pom.xml",property);
  if (property != null && !isResolved(property)) {
    property=resolveProperty("pom.xml",property,0);
  }
  if (property != null && !isResolved(property)) {
    property=resolveProperty("../pom.xml",property,0);
  }
  if (property != null && !isResolved(property)) {
    property=resolveProperty("../../parent-spring-boot/pom.xml",property,0);
  }
  return property;
}
