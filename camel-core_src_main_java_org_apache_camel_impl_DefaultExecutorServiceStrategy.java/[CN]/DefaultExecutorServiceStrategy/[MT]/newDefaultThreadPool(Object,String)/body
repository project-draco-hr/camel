{
  ExecutorService answer=ExecutorServiceHelper.newThreadPool(threadNamePattern,name,defaultThreadPoolProfile.getPoolSize(),defaultThreadPoolProfile.getMaxPoolSize(),defaultThreadPoolProfile.getKeepAliveTime(),defaultThreadPoolProfile.getTimeUnit(),defaultThreadPoolProfile.getMaxQueueSize(),defaultThreadPoolProfile.getRejectedExecutionHandler(),false);
  onNewExecutorService(answer);
  return answer;
}
