{
  long time=0;
  if (delay != null) {
    Long longValue=delay.evaluate(exchange,Long.class);
    if (longValue != null) {
      delayValue=longValue;
      time=longValue;
    }
 else {
      delayValue=0;
    }
  }
  if (time <= 0) {
    return;
  }
  time+=defaultProcessTime(exchange);
  waitUntil(time,exchange);
}
