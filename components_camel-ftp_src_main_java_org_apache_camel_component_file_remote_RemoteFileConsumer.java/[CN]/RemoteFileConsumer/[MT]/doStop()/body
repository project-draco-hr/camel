{
  super.doStop();
  try {
    if (((RemoteFileOperations)operations).isConnected()) {
      loggedIn=false;
      log.debug("Disconnecting from " + remoteServer());
      ((RemoteFileOperations)operations).disconnect();
    }
  }
 catch (  GenericFileOperationFailedException e) {
    log.warn(e.getMessage());
  }
}
