{
  try {
    if (getOperations().isConnected()) {
      loggedIn=false;
      if (log.isDebugEnabled()) {
        log.debug("Disconnecting from: " + remoteServer());
      }
      getOperations().disconnect();
    }
  }
 catch (  GenericFileOperationFailedException e) {
    log.warn(e.getMessage());
  }
}
