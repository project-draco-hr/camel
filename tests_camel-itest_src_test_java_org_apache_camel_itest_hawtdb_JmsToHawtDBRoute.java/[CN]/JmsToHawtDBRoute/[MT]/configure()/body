{
  from("activemq:queue:foo").transacted().to("mock:input").aggregate(header("group"),new MyConcatAggregatationStrategy()).aggregationRepository(new HawtDBAggregationRepository("events","data/hawtdb.dat")).completionSize(10).log("Aggregated #${header.counter} ${header.group} with body ${body}").to("activemq:queue:out").to("mock:out");
  from("activemq:queue:out").to("mock:result");
}
