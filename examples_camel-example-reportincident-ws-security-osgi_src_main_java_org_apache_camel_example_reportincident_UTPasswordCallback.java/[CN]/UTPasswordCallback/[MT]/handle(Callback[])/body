{
  String user="";
  for (int i=0; i < callbacks.length; i++) {
    WSPasswordCallback pc=(WSPasswordCallback)callbacks[i];
    user=pc.getIdentifier();
    String pass=passwords.get(user);
    if (pass != null) {
      pc.setPassword(pass);
      return;
    }
  }
  throw new IOException("Password does not exist for the user : " + user);
}
