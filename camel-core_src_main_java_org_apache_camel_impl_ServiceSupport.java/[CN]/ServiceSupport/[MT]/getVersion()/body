{
  if (ObjectHelper.isNotNullAndNonEmpty(version)) {
    return version;
  }
  Package aPackage=getClass().getPackage();
  if (aPackage != null) {
    version=aPackage.getImplementationVersion();
    if (version == null) {
      version=aPackage.getSpecificationVersion();
      if (version == null) {
        version="";
      }
    }
  }
 else {
    version="";
  }
  return version;
}
