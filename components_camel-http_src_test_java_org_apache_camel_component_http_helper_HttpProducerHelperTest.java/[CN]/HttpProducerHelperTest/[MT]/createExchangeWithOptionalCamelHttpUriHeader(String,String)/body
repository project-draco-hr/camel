{
  CamelContext context=new DefaultCamelContext();
  Exchange exchange=new DefaultExchange(context);
  Message inMsg=exchange.getIn();
  if (endpointURI != null) {
    inMsg.setHeader("CamelHttpUri",endpointURI);
  }
  if (httpPath != null) {
    inMsg.setHeader("CamelHttpPath",httpPath);
  }
  return exchange;
}
