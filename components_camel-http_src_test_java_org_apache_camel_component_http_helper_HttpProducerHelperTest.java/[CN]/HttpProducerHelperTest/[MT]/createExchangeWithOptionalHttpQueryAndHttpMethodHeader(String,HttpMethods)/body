{
  CamelContext context=new DefaultCamelContext();
  Exchange exchange=new DefaultExchange(context);
  Message inMsg=exchange.getIn();
  if (httpQuery != null) {
    inMsg.setHeader("CamelHttpQuery",httpQuery);
  }
  if (httpMethod != null) {
    inMsg.setHeader("CamelHttpMethod",httpMethod);
  }
  return exchange;
}
