{
  if (is == null) {
    return null;
  }
  if (exchange.getProperty(Exchange.DISABLE_HTTP_STREAM_CACHE,Boolean.FALSE,Boolean.class)) {
    return is;
  }
 else {
    CachedOutputStream cos=new CachedOutputStream(exchange);
    IOHelper.copyAndCloseInput(is,cos);
    return cos.getStreamCache();
  }
}
