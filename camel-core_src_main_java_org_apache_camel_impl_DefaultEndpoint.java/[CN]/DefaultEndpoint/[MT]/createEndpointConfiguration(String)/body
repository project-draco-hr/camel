{
  if (getComponent() != null) {
    try {
      return getComponent().createConfiguration(uri);
    }
 catch (    Exception e) {
      throw ObjectHelper.wrapRuntimeCamelException(e);
    }
  }
 else   if (getCamelContext() != null) {
    return new MappedEndpointConfiguration(getCamelContext(),uri);
  }
  return null;
}
