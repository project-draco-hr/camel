{
  return new RouteBuilder(){
    public void configure() throws Exception {
      from("pop3://jones@localhost?password=secret").routeId("route1").shutdownRunningTask(ShutdownRunningTask.CompleteAllTasks).delay(1000).to("seda:foo");
      from("seda:foo").routeId("route2").to("mock:bar");
    }
  }
;
}
