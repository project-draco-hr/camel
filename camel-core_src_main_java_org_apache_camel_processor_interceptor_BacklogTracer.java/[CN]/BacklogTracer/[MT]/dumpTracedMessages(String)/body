{
  List<BacklogTracerEventMessage> answer=new ArrayList<BacklogTracerEventMessage>();
  if (nodeId != null) {
    for (    DefaultBacklogTracerEventMessage message : queue) {
      if (nodeId.equals(message.getToNode())) {
        answer.add(message);
      }
    }
  }
  if (removeOnDump) {
    queue.removeAll(answer);
  }
  return answer;
}
