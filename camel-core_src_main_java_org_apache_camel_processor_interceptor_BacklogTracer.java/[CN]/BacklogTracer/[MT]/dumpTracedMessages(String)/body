{
  List<BacklogTracerEventMessage> answer=new ArrayList<>();
  if (nodeId != null) {
    for (    BacklogTracerEventMessage message : queue) {
      if (nodeId.equals(message.getToNode()) || nodeId.equals(message.getRouteId())) {
        answer.add(message);
      }
    }
  }
  if (removeOnDump) {
    queue.removeAll(answer);
  }
  return answer;
}
