{
  validateDeadLetterUri(routeContext);
  DeadLetterChannel answer=new DeadLetterChannel(routeContext.getCamelContext(),processor,getLogger(),getOnRedelivery(),getRedeliveryPolicy(),getExceptionPolicyStrategy(),getFailureProcessor(),getDeadLetterUri(),isUseOriginalMessage(),getRetryWhilePolicy(routeContext.getCamelContext()),getExecutorServiceRef());
  configure(routeContext,answer);
  return answer;
}
