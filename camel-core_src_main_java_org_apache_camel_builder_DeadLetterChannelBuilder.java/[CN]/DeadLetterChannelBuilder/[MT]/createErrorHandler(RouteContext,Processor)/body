{
  validateDeadLetterUri(routeContext);
  DeadLetterChannel answer=new DeadLetterChannel(routeContext.getCamelContext(),processor,getLogger(),getOnRedelivery(),getRedeliveryPolicy(),getExceptionPolicyStrategy(),getFailureProcessor(),getDeadLetterUri(),isDeadLetterHandleNewException(),isUseOriginalMessage(),getRetryWhilePolicy(routeContext.getCamelContext()),getExecutorService(routeContext.getCamelContext()),getOnPrepareFailure());
  configure(routeContext,answer);
  return answer;
}
