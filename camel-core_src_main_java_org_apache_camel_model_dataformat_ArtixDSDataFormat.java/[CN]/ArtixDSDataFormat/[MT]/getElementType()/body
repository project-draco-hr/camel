{
  if (elementType == null) {
    if (elementTypeName != null) {
      elementType=ObjectHelper.loadClass(elementTypeName,getClass().getClassLoader());
      if (elementType == null) {
        LOG.warn("Could not load ArtixDS Element class: " + elementTypeName + " on the classpath");
      }
    }
  }
  return elementType;
}
