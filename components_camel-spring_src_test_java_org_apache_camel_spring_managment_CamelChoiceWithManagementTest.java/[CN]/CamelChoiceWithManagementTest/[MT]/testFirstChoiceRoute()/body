{
  final String body="<one/>";
  a.expectedBodiesReceived(body);
  a.expectedHeaderReceived("CBR1","Yes");
  c.expectedBodiesReceived(body);
  c.expectedHeaderReceived("CBR1","Yes");
  c.expectedHeaderReceived("Validation","Yes");
