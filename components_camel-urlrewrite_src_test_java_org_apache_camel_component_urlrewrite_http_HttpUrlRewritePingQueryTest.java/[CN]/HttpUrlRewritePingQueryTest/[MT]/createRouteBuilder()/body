{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("jetty:http://localhost:8080/?matchOnUriPrefix=true").to("http://localhost:8081/?bridgeEndpoint=true&throwExceptionOnFailure=false&urlRewrite=#myRewrite");
      from("jetty:http://localhost:8081/proxy/?matchOnUriPrefix=true").transform().simple("${header.CamelHttpUrl}?${header.CamelHttpQuery}");
    }
  }
;
}
