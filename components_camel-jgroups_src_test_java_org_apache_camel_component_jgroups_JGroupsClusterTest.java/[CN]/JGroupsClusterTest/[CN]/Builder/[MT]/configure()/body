{
  from("jgroups:cluster?enableViewMessages=true").filter(dropNonCoordinatorViews()).process(new Processor(){
    @Override public void process(    Exchange exchange) throws Exception {
      String camelContextName=exchange.getContext().getName();
      if (!camelContextName.equals(master)) {
        master=camelContextName;
        nominationCount++;
      }
    }
  }
);
}
