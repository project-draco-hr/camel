{
  String dsl="from(\"timer://kickoff?period=9999910000\").setHeader(\"id\").constant(\"foo\").setBody().constant(\"a b c\").split(body().tokenize(\" \")).to(\"seda:splitted\");" + "from(\"seda:splitted\").aggregate(header(\"id\")).to(\"mock:result\")";
  String expected="from(\"timer://kickoff?period=9999910000\").setHeader(\"id\").constant(\"foo\").setBody().constant(\"a b c\").split(body().tokenize(\" \")).to(\"seda:splitted\");\n" + "from(\"seda:splitted\").aggregate().header(\"id\").to(\"mock:result\")";
  assertEquals(expected,renderRoutes(dsl));
}
