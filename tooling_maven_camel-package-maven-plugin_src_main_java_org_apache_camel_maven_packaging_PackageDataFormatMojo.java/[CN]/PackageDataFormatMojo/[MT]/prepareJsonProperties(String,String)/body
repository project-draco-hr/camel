  String jsonXStream="\"enum\": [ \"Gson\", \"Jackson\", \"Johnzon\", \"XStream\" ], \"deprecated\": \"false\", \"secret\": \"false\", \"defaultValue\": \"XStream\"";
  if ("json-gson".equals(name)) {
    properties=properties.replace(json,jsonGson);
  }
