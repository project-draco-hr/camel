{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      restConfiguration().host("localhost");
      rest("/say/hello").id("hello").description("Hello Service").get().id("get-say").description("Says hello to you").to("direct:hello");
      rest("/say/bye").description("bye","Bye Service","en").get().description("Says bye to you").consumes("application/json").param().type(RestParamType.header).description("header param description1").dataType("integer").allowableValues("1","2","3","4").defaultValue("1").name("header_count").required(true).access("acc1").endParam().param().type(RestParamType.query).description("header param description2").dataType("string").allowableValues("a","b","c","d").defaultValue("b").collectionFormat(CollectionFormat.multi).name("header_letter").required(false).access("acc2").endParam().responseMessage().code(300).message("test msg").responseModel(Integer.class).endResponseMessage().responseMessage().code("error").message("does not work").endResponseMessage().to("direct:bye").post().description("Updates the bye message").to("mock:update");
      from("direct:hello").transform().constant("Hello World");
      from("direct:bye").transform().constant("Bye World");
    }
  }
;
}
