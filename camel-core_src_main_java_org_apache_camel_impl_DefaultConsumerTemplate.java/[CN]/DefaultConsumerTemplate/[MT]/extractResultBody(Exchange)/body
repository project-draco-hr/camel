{
  Object answer=null;
  if (result != null) {
    if (result.getException() != null) {
      throw wrapRuntimeCamelException(result.getException());
    }
    if (hasFaultMessage(result)) {
      return result.getFault().getBody();
    }
    if (result.hasOut()) {
      answer=result.getOut().getBody();
    }
 else {
      answer=result.getIn().getBody();
    }
  }
  return answer;
}
