{
  if (uri != null && verb.getUri() != null) {
    String s=FileUtil.stripTrailingSeparator(uri);
    String s2=FileUtil.stripLeadingSeparator(verb.getUri());
    return s + "/" + s2;
  }
 else   if (uri != null) {
    return uri;
  }
 else   if (verb.getUri() != null) {
    return verb.getUri();
  }
 else {
    return "";
  }
}
