{
  getMockEndpoint("mock:result").expectedBodiesReceived("<parent><child name='child1'><grandchild name='grandchild1'/> <grandchild name='grandchild2'/></child></parent>","<parent><child name='child2'><grandchild name='grandchild1'></grandchild><grandchild name='grandchild2'></grandchild></child></parent>");
  template.sendBody("direct:start","<parent><child name='child1'><grandchild name='grandchild1'/> <grandchild name='grandchild2'/></child>" + "<child name='child2'><grandchild name='grandchild1'></grandchild><grandchild name='grandchild2'></grandchild></child></parent>");
  assertMockEndpointsSatisfied();
}
