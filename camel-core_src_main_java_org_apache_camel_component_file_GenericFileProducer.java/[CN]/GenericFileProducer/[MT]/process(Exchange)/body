{
  String target=createFileName(exchange);
  Lock lock;
synchronized (locks) {
    lock=locks.get(target);
    if (lock == null) {
      lock=new ReentrantLock();
      locks.put(target,lock);
    }
  }
  lock.lock();
  try {
    processExchange(exchange,target);
  }
  finally {
    lock.unlock();
  }
}
