{
  Object answer=null;
  if (result != null) {
    boolean notOut=pattern != null && !pattern.isOutCapable();
    boolean hasOut=result.getOut(false) != null;
    if (hasOut && !notOut) {
      answer=result.getOut().getBody();
    }
 else {
      answer=result.getIn().getBody();
    }
  }
  return answer;
}
