{
  return new RouteBuilder(){
    public void configure(){
      from("direct:fallback").to("mock:error");
      from("direct:run").to("mock:result");
      from("direct:start").to("hystrix:testKey?runEndpointId=run&fallbackEndpointId=fallback&cacheKeyExpression=#headerExpression&initializeRequestContext=true");
    }
  }
;
}
