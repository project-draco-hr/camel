{
  if (level == null) {
    throw new NullPointerException("Log level may not be null");
  }
  return level == LoggingLevel.DEBUG && log.isDebugEnabled() || level == LoggingLevel.ERROR && log.isErrorEnabled() || level == LoggingLevel.INFO && log.isInfoEnabled() || level == LoggingLevel.TRACE && log.isTraceEnabled() || level == LoggingLevel.WARN && log.isWarnEnabled();
}
