{
  if (level == null) {
    throw new NullPointerException("Log level may not be null");
  }
switch (level) {
case DEBUG:
    log.debug(message,th);
  break;
case ERROR:
log.error(message,th);
break;
case INFO:
log.info(message,th);
break;
case TRACE:
log.trace(message,th);
break;
case WARN:
log.warn(message,th);
break;
}
}
