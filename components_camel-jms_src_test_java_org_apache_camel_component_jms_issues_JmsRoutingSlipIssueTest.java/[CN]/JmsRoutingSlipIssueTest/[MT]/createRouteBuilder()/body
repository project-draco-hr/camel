{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:start").setExchangePattern(ExchangePattern.InOut).routingSlip(header("mySlip")).to("mock:result");
      from("activemq:queue:a").to("mock:a").transform(body().append("A"));
      from("activemq:queue:b").to("mock:b").transform(body().append("B"));
      from("activemq:queue:c").to("mock:c").transform(body().append("C"));
    }
  }
;
}
