{
  Option[] options=combine(getDefaultCamelKarafOptions(),scanFeatures(getCamelKarafFeatureUrl(),"camel-hdfs"),new Customizer(){
    @Override public InputStream customizeTestProbe(    InputStream testProbe){
      return modifyBundle(testProbe).add(SendEmail.class).add("core-default.xml",HdfsRouteTest.class.getResource("core-default.xml")).set(Constants.BUNDLE_SYMBOLICNAME,"CamelHdfsTestBundle").set(Constants.DYNAMICIMPORT_PACKAGE,"*").build();
    }
  }
);
  return options;
}
