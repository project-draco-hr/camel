{
  Endpoint endpoint=context.getEndpoint("mock:foo");
  for (int i=0; i < 10; i++) {
    Producer producer=pool.acquire(endpoint);
    if (producer == null) {
      producer=pool.addAndAcquire(endpoint,new MyProducer(endpoint));
    }
    assertNotNull(producer);
    pool.release(endpoint,producer);
  }
}
