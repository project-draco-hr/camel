{
  for (  Field<String> c : criteria) {
    String value=null;
    try {
      if (fieldMap.isSetField(c.getField())) {
        value=fieldMap.getString(c.getField());
      }
    }
 catch (    FieldNotFound e) {
    }
    if (!c.getObject().equals(value)) {
      return false;
    }
  }
  return true;
}
