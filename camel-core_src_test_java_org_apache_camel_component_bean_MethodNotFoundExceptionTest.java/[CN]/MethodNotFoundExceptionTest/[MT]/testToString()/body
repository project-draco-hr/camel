{
  Exchange exchange=new DefaultExchange(new DefaultCamelContext());
  MyDummyBean bean=new MyDummyBean();
  String methodName="foo";
  List<Class> parameterTypes=new ArrayList<Class>();
  parameterTypes.add(String.class);
  parameterTypes.add(Long.class);
  MethodNotFoundException exception=new MethodNotFoundException(exchange,bean,methodName,parameterTypes);
  assertSame(exchange,exception.getExchange());
  assertSame(bean,exception.getBean());
  assertSame(methodName,exception.getMethodName());
  assertSame(parameterTypes,exception.getParameterTypes());
  assertEquals("Method with name: foo and parameter types: [class java.lang.String, " + "class java.lang.Long] not found on bean: " + "org.apache.camel.component.bean.MyDummyBean. Exchange[null]",exception.getMessage());
}
