{
  super.setUp();
  context1.addRoutes(new RouteBuilder(){
    public void configure(){
      from("vm:test.a").to("vm:test.b");
    }
  }
);
  context2.addRoutes(new RouteBuilder(){
    public void configure(){
      from("vm:test.b").to("mock:result");
    }
  }
);
  ServiceHelper.startServices(context1,context2);
}
