{
  Set toRemove=new HashSet();
  for (  Object key : parameters.keySet()) {
    if (key.toString().startsWith("mail.")) {
      config.getAdditionalJavaMailProperties().put(key,parameters.get(key));
      toRemove.add(key);
    }
  }
  for (  Object key : toRemove) {
    parameters.remove(key);
  }
}
