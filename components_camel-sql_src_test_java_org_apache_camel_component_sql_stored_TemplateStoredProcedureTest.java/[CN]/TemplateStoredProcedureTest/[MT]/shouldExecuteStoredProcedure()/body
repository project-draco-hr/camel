{
  TemplateStoredProcedure sp=new TemplateStoredProcedure(jdbcTemplate,parser.parseTemplate("ADDNUMBERS" + "(INTEGER ${header.v1},INTEGER ${header.v2},OUT INTEGER resultofsum)"));
  Exchange exchange=createExchangeWithBody(null);
  exchange.getIn().setHeader("v1",1);
  exchange.getIn().setHeader("v2",2);
  sp.execute(exchange);
  Assert.assertEquals(Integer.valueOf(3),exchange.getOut().getHeader("resultofsum"));
}
