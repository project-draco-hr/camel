{
  File outputDir=new File(outputPath);
  File imageDir=new File(imageLocation);
  if (!outputDir.exists()) {
    outputDir.mkdirs();
    imageDir.mkdirs();
  }
  this.createMainXML();
  for (int i=0; i < resources.length; ++i) {
    this.setChapterId(removeExtension(resources[i]));
    process(resources[i]);
  }
}
