{
  WebsocketComponent wc=getContext().getComponent("websocket",WebsocketComponent.class);
  wc.setPort(9090);
  wc.setStaticResources("src/main/resources");
  fromF("twitter://search?type=polling&delay=%s&keywords=%s" + "&consumerKey=%s&consumerSecret=%s&accessToken=%s&accessTokenSecret=%s",delay,searchTerm,consumerKey,consumerSecret,accessToken,accessTokenSecret).setHeader(WebsocketConstants.SEND_TO_ALL).constant(true).to("websocket:camel-tweet");
}
