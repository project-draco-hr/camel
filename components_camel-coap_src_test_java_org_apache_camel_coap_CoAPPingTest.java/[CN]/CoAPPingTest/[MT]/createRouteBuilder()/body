{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("coap://localhost:" + PORT + "/TestResource?coapMethod=PING").to("log:exch").transform(body().convertTo(Boolean.class)).to("log:exch");
      from("direct:start").to("coap://localhost:" + PORT + "/TestResource?coapMethod=PING").to("mock:result");
    }
  }
;
}
