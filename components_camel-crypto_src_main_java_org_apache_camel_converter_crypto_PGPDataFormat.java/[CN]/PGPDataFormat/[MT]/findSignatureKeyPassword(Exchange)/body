{
  String sigPassword=exchange.getIn().getHeader(SIGNATURE_KEY_PASSWORD,getSignaturePassword(),String.class);
  if (sigPassword != null) {
    return sigPassword;
  }
  if (passphraseAccessor != null) {
    return passphraseAccessor.getPassphrase(findSignatureKeyUserid(exchange));
  }
 else {
    return null;
  }
}
