{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      BindyCsvDataFormat camelDataFormat=new BindyCsvDataFormat("org.apache.camel.dataformat.bindy.model.car");
      camelDataFormat.setLocale("en");
      from("direct:out").unmarshal().bindy(BindyType.Csv,"org.apache.camel.dataformat.bindy.model.car").to("mock:out");
      from("direct:in").marshal(camelDataFormat).to("mock:in");
    }
  }
;
}
