{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      BindyCsvDataFormat dataFormat=new BindyCsvDataFormat();
      dataFormat.setClassType(org.apache.camel.dataformat.bindy.model.car.Car.class);
      dataFormat.setLocale("en");
      from("direct:out").unmarshal().bindy(BindyType.Csv,org.apache.camel.dataformat.bindy.model.car.Car.class).to("mock:out");
      from("direct:in").marshal(dataFormat).to("mock:in");
    }
  }
;
}
