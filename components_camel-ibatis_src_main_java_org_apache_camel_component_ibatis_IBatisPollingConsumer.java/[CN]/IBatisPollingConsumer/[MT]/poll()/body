{
  IBatisEndpoint endpoint=getEndpoint();
  if (LOG.isTraceEnabled()) {
    LOG.trace("Polling: " + endpoint);
  }
  List data=endpoint.getProcessingStrategy().poll(this,getEndpoint());
  if (useIterator) {
    for (    Object object : data) {
      if (isRunAllowed()) {
        process(object);
      }
    }
  }
 else {
    process(data);
  }
}
