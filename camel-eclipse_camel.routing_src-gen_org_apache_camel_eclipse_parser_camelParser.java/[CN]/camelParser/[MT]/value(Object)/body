{
  if (obj == null)   return null;
  if (obj instanceof Token) {
    Token t=(Token)obj;
    if (t.getType() == camelLexer.INT)     return Integer.valueOf(t.getText());
    if (t.getType() == camelLexer.STRING)     return t.getText().substring(1,t.getText().length() - 1);
    if (t.getText().startsWith("^"))     return t.getText().substring(1,t.getText().length());
    return t.getText();
  }
  return obj;
}
