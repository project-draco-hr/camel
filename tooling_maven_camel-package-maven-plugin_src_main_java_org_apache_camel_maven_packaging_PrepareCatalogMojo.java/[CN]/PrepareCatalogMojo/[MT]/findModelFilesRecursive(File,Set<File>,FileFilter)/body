{
  File[] files=dir.listFiles(filter);
  if (files != null) {
    for (    File file : files) {
      boolean jsonFile=file.isFile() && file.getName().endsWith(".json");
      if (jsonFile) {
        found.add(file);
      }
 else       if (file.isDirectory()) {
        findModelFilesRecursive(file,found,filter);
      }
    }
  }
}
