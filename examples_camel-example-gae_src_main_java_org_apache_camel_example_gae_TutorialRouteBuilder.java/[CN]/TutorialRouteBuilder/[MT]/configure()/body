{
  from("ghttp:///weather").to("gtask://default").setHeader(Exchange.CONTENT_TYPE,constant("text/plain")).transform(constant("Weather report will be sent to ").append(header("mailto")));
  from("gtask://default").setHeader(Exchange.HTTP_QUERY,constant("weather=").append(header("city"))).to("ghttp://www.google.com/ig/api").process(new WeatherProcessor()).setHeader(GMailBinding.GMAIL_SUBJECT,constant("Weather report")).setHeader(GMailBinding.GMAIL_TO,header("mailto")).to("gmail://replaceme@gmail.com");
}
