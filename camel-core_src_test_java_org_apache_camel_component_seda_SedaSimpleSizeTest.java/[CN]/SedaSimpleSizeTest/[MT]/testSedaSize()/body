{
  getMockEndpoint("mock:result").expectedBodiesReceived("0","1","2");
  template.sendBody("direct:start","");
  template.sendBody("seda:foo","A");
  template.sendBody("direct:start","");
  template.sendBody("seda:foo","B");
  template.sendBody("direct:start","");
  template.sendBody("seda:foo","C");
  assertMockEndpointsSatisfied();
}
