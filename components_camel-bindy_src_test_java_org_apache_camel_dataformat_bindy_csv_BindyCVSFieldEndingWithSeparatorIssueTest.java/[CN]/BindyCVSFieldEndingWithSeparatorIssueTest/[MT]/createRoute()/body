{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:fromCsv").unmarshal().bindy(BindyType.Csv,Record.class).setProperty("addressLine1",simple("${in.body.addressLine1}")).setProperty("addressLine2",simple("${in.body.addressLine2}")).log("${in.body}").to("mock:result");
    }
  }
;
}
