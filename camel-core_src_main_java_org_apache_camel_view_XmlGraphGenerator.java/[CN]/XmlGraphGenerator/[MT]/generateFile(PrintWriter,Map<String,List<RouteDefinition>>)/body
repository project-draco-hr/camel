{
  writer.println("<?xml version='1.0' encoding='UTF-8'?>");
  writer.println("<Graph>");
  writer.println();
  if (map.size() > 0) {
    writer.println("<Node id='root' name='Camel Routes' description='Collection of Camel Routes' nodeType='root'/>");
  }
  printRoutes(writer,map);
  writer.println();
  writer.println("</Graph>");
}
