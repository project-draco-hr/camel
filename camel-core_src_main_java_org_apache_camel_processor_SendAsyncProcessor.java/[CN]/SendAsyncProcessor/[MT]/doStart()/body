{
  super.doStart();
  if (poolSize <= 0) {
    throw new IllegalArgumentException("PoolSize must be a positive number, was: " + poolSize);
  }
  for (int i=0; i < poolSize; i++) {
    if (executorService == null) {
      executorService=destination.getCamelContext().getExecutorServiceStrategy().newFixedThreadPool(this,"SendAsyncProcessor-Consumer",poolSize);
    }
    executorService.execute(this);
  }
}
