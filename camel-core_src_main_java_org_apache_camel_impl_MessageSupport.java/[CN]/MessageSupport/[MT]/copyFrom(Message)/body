{
  setMessageId(that.getMessageId());
  setBody(that.getBody());
  if (hasHeaders()) {
    getHeaders().clear();
  }
  if (that.hasHeaders()) {
    getHeaders().putAll(that.getHeaders());
  }
  if (hasAttachments()) {
    getAttachments().clear();
  }
  if (that.hasAttachments()) {
    getAttachments().putAll(that.getAttachments());
  }
}
