{
  return new RouteBuilder(){
    @Override public void configure(){
      from("direct:start").to("infinispan://localhost?cacheContainer=#cacheContainer");
      from("direct:put").to("infinispan://localhost?cacheContainer=#cacheContainer&command=PUT");
      from("direct:get").to("infinispan://localhost?cacheContainer=#cacheContainer&command=GET");
      from("direct:remove").to("infinispan://localhost?cacheContainer=#cacheContainer&command=REMOVE");
      from("direct:clear").to("infinispan://localhost?cacheContainer=#cacheContainer&command=CLEAR");
    }
  }
;
}
