{
  if (oldExchange == null) {
    return newExchange;
  }
  BankQuote oldQuote=oldExchange.getIn().getBody(BankQuote.class);
  BankQuote newQuote=newExchange.getIn().getBody(BankQuote.class);
  if (oldQuote.getRate() <= newQuote.getRate()) {
    return oldExchange;
  }
 else {
    return newExchange;
  }
}
