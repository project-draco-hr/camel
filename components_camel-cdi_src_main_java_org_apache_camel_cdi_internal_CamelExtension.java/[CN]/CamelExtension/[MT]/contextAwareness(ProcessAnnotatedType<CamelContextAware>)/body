{
  AnnotatedType<CamelContextAware> at=process.getAnnotatedType();
  Method method=at.getJavaClass().getMethod("setCamelContext",CamelContext.class);
  AnnotatedTypeBuilder<CamelContextAware> builder=new AnnotatedTypeBuilder<CamelContextAware>().readFromType(at).addToMethod(method,new InjectLiteral());
