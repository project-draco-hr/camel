{
  MockEndpoint a=getMockEndpoint("mock:a");
  a.expectedBodiesReceived("Hello a");
  MockEndpoint b=getMockEndpoint("mock:b");
  b.expectedBodiesReceived("Hello b");
  String out=template.requestBody(uri,"Hello " + uri,String.class);
