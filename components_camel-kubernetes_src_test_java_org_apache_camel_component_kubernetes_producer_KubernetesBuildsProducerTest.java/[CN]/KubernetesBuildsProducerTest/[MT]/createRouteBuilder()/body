{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:list").toF("kubernetes://%s?oauthToken=%s&category=builds&operation=listBuilds",host,authToken);
      from("direct:listByLabels").toF("kubernetes://%s?oauthToken=%s&category=builds&operation=listBuildsByLabels",host,authToken);
    }
  }
;
}
