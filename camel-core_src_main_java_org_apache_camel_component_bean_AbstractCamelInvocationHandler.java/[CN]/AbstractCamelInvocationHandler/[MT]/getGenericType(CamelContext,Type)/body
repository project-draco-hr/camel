{
  if (type == null) {
    return Object.class;
  }
  String name=ObjectHelper.between(type.toString(),"<",">");
  if (name != null) {
    if (name.contains("<")) {
      name=ObjectHelper.before(name,"<");
    }
    return context.getClassResolver().resolveMandatoryClass(name);
  }
 else {
    return Object.class;
  }
}
