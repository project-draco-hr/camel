{
  if (level != null) {
    if (!level.equals(target.getLevel())) {
      return false;
    }
  }
  if (logger != null) {
    if (!logger.equals(target.getLogger())) {
      return false;
    }
  }
  if (message != null) {
    if (!message.matches(target.getMessage())) {
      return false;
    }
  }
  return true;
}
