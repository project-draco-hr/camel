{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("file:target/splitter?delete=true&sortBy=file:name").split(body().tokenize(",")).to("seda:queue").process(new Processor(){
        public void process(        Exchange exchange) throws Exception {
          Thread.sleep(200);
        }
      }
).end().log("End of file ${file:name}").to("mock:result");
      from("seda:queue").log("Token: ${body}").to("mock:foo");
    }
  }
;
}
