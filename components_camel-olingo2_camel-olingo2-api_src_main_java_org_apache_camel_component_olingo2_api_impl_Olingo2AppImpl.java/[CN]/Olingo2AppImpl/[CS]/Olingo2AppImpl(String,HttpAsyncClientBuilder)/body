{
  if (serviceUri == null) {
    throw new IllegalArgumentException("serviceUri");
  }
  this.serviceUri=serviceUri;
  if (builder == null) {
    this.client=HttpAsyncClients.createDefault();
  }
 else {
    this.client=builder.build();
  }
  this.client.start();
  this.contentType=ContentType.APPLICATION_JSON_CS_UTF_8;
}
