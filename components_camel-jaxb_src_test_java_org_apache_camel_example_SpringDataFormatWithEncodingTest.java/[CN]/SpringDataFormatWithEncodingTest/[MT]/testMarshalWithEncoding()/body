{
  PurchaseOrder bean=new PurchaseOrder();
  bean.setName("Beer");
  bean.setAmount(23);
  bean.setPrice(2.5);
  MockEndpoint mock=resolveMandatoryEndpoint("mock:result",MockEndpoint.class);
  mock.expectedBodiesReceived("<?xml version=\"1.0\" encoding=\"iso-8859-1\" standalone=\"yes\"?>" + "<purchaseOrder amount=\"23.0\" price=\"2.5\" name=\"Beer\"/>");
  template.sendBody("direct:start",bean);
  mock.assertIsSatisfied();
}
