{
  try {
    getDocument();
    while (hasNextEntry()) {
      Entry entry=list.get(entryIndex--);
      if (entryFilter.isValidEntry(endpoint,document,entry)) {
        return endpoint.createExchange(document,entry);
      }
    }
    document=null;
    return null;
  }
 catch (  Exception e) {
    throw new RuntimeCamelException(e);
  }
}
