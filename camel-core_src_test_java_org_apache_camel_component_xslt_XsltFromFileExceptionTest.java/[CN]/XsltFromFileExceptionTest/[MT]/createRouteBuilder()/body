{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("file:target/xslt?moveFailed=error&move=ok").onException(Exception.class).to("mock:error").end().convertBodyTo(String.class).to("xslt:org/apache/camel/component/xslt/example.xsl").to("mock:result");
    }
  }
;
}
