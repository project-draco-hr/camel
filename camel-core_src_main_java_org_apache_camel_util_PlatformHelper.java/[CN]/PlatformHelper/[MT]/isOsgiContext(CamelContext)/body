{
  String contextType=camelContext.getClass().getSimpleName();
  if (contextType.startsWith("Osgi") || contextType.equals("BlueprintCamelContext")) {
    LOG.trace("{} used - assuming running in the OSGi container.",contextType);
    return true;
  }
 else {
    LOG.trace("{} used - assuming running in the OSGi container.",contextType);
    return false;
  }
}
