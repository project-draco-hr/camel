{
  Bundle bundle=getBundle(classFromBundle);
  if (bundle != null) {
    LOG.trace("Found OSGi bundle {} for class {} so assuming running in the OSGi container.",bundle.getSymbolicName(),classFromBundle.getSimpleName());
    return true;
  }
 else {
    LOG.trace("Cannot find OSGi bundle for class {} so assuming not running in the OSGi container.",classFromBundle.getSimpleName());
    return false;
  }
}
