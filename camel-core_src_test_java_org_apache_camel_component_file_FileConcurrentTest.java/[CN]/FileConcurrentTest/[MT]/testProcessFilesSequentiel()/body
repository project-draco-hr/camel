{
  context.addRoutes(new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("file://target/concurrent").setHeader("id",simple("${file:onlyname.noext}")).beanRef("business").aggregate(header("country"),new BodyInAggregatingStrategy()).completionTimeout(2000L).to("mock:result");
    }
  }
);
  context.start();
  long start=System.currentTimeMillis();
  MockEndpoint result=getMockEndpoint("mock:result");
  result.expectedBodiesReceivedInAnyOrder("A+C+E+G+I","B+D+F+H+J");
  assertMockEndpointsSatisfied();
  long delta=System.currentTimeMillis() - start;
  LOG.debug("Time taken sequentiel: " + delta);
}
