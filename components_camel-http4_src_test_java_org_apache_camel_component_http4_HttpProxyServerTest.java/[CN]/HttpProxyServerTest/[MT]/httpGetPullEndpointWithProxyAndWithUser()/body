{
  proxy.register("*",new ProxyAuthenticationValidationHandler("GET",null,null,getExpectedContent(),user,password));
  Exchange exchange=consumer.receive("http4://" + getHostName() + ":"+ getPort()+ "?proxyAuthHost="+ getProxyHost()+ "&proxyAuthPort="+ getProxyPort()+ "&proxyAuthUsername=camel&proxyAuthPassword=password");
  assertExchange(exchange);
}
