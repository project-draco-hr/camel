{
  proxy.register("*",new ProxyAuthenticationValidationHandler("GET",null,null,getExpectedContent(),user,password));
  Exchange exchange=template.request("http4://" + getHostName() + ":"+ getPort()+ "?proxyAuthHost="+ getProxyHost()+ "&proxyAuthPort="+ getProxyPort()+ "&proxyAuthUsername=camel&proxyAuthPassword=password",new Processor(){
    public void process(    Exchange exchange) throws Exception {
    }
  }
);
  assertExchange(exchange);
}
