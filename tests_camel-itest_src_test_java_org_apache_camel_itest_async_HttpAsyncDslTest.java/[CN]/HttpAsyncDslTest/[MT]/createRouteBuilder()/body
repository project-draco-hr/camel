{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      DataFormat mySecureDataFormat=new StringDataFormat("iso-8859-1");
      from("jms:queue:order").to("bean:validateOrder").to("mock:validate").async(20).unmarshal(mySecureDataFormat).delay(100).to("bean:handleOrder").to("mock:order");
    }
  }
;
}
