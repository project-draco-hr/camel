{
  Class ret;
switch (sqlType) {
case Types.INTEGER:
    ret=Integer.class;
  break;
case Types.VARCHAR:
ret=String.class;
break;
case Types.BIGINT:
ret=BigInteger.class;
break;
case Types.CHAR:
ret=String.class;
break;
case Types.BOOLEAN:
ret=Boolean.class;
break;
case Types.DATE:
ret=Date.class;
break;
case Types.TIMESTAMP:
ret=Date.class;
break;
default :
throw new ParseRuntimeException("Unable to map SQL type " + sqlTypeStr + " to Java type");
}
return ret;
}
