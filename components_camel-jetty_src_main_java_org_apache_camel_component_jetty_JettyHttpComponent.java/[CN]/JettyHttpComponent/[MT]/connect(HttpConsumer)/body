{
  HttpEndpoint endpoint=(HttpEndpoint)consumer.getEndpoint();
  String connectorKey=endpoint.getProtocol() + ":" + endpoint.getPort();
synchronized (connectors) {
    ConnectorRef connectorRef=connectors.get(connectorKey);
    if (connectorRef == null) {
      Connector connector;
      if ("https".equals(endpoint.getProtocol())) {
        SslSocketConnector socketConnector=new SslSocketConnector();
        socketConnector.setPassword(sslPassword);
        socketConnector.setKeyPassword(sslKeyPassword);
        connector=socketConnector;
      }
 else {
        connector=new SelectChannelConnector();
      }
      connector.setPort(endpoint.getPort());
      getServer().addConnector(connector);
      connector.start();
      connectorRef=new ConnectorRef(connector);
      connectors.put(connectorKey,connectorRef);
    }
 else {
      connectorRef.increment();
    }
  }
  camelServlet.connect(consumer);
}
