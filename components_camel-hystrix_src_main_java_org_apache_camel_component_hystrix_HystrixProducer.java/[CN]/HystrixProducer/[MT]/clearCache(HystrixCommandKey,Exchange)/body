{
  Boolean clearCache=exchange.getIn().getHeader(HystrixConstants.CAMEL_HYSTRIX_CLEAR_CACHE_FIRST,Boolean.class);
  if (clearCache != null && clearCache) {
    HystrixRequestCache.getInstance(camelHystrixCommand,HystrixPlugins.getInstance().getConcurrencyStrategy()).clear(String.valueOf(getCacheKey(exchange)));
  }
}
