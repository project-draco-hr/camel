{
  return new RouteBuilder(){
    public void configure() throws Exception {
      DataFormat hl7=new HL7DataFormat();
      from("mina:tcp://localhost:8888?sync=true&codec=hl7codec").unmarshal(hl7).choice().when(header("hl7.msh.triggerEvent").isEqualTo("A19")).beanRef("hl7service","handleA19").to("mock:a19").when(header("hl7.msh.triggerEvent").isEqualTo("A01")).to("mock:a01").beanRef("hl7service","handleA01").to("mock:a19").otherwise().to("mock:unknown").end().marshal(hl7);
    }
  }
;
}
