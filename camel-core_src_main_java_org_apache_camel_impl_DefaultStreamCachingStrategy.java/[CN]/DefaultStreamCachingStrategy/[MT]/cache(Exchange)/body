{
  StreamCache cache=exchange.getIn().getBody(StreamCache.class);
  try {
    if (cache != null) {
      if (cache.inMemory()) {
        cacheMemoryCounter++;
        cacheMemorySize+=cache.length();
      }
 else {
        cacheSpoolCounter++;
        cacheSpoolSize+=cache.length();
      }
    }
  }
 catch (  Exception e) {
    LOG.debug("Error updating cache statistics. This exception is ignored.",e);
  }
  return cache;
}
