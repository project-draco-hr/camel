{
  if (spoolTasks.isEmpty()) {
    return false;
  }
  boolean all=true;
  boolean any=false;
  for (  ShouldSpoolTask task : spoolTasks) {
    boolean result=task.shouldSpoolCache(length);
    if (!result) {
      all=false;
    }
 else {
      any=true;
      if (anySpoolTasks) {
        break;
      }
    }
  }
  return anySpoolTasks ? any : all;
}
