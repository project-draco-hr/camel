{
  if (spoolRules.isEmpty()) {
    return false;
  }
  boolean all=true;
  boolean any=false;
  for (  SpoolRule rule : spoolRules) {
    boolean result=rule.shouldSpoolCache(length);
    if (!result) {
      all=false;
    }
 else {
      any=true;
      if (anySpoolRules) {
        break;
      }
    }
  }
  return anySpoolRules ? any : all;
}
