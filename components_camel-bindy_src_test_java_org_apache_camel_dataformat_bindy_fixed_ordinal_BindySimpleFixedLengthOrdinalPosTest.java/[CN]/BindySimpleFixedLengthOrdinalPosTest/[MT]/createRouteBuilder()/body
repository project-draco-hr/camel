{
  RouteBuilder routeBuilder=new RouteBuilder(){
    @Override public void configure() throws Exception {
      from(URI_DIRECT_MARSHALL).marshal().bindy(BindyType.Fixed,BindySimpleFixedLengthOrdinalPosTest.Order.class).to(URI_MOCK_MARSHALL_RESULT);
      from(URI_DIRECT_UNMARSHALL).unmarshal().bindy(BindyType.Fixed,BindySimpleFixedLengthOrdinalPosTest.Order.class).to(URI_MOCK_UNMARSHALL_RESULT);
    }
  }
;
  return routeBuilder;
}
