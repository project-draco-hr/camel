{
  if (headers.containsKey(key)) {
    Object existing=headers.get(key);
    List<Object> list;
    if (existing instanceof List) {
      list=(List)existing;
    }
 else {
      list=new ArrayList<Object>();
      list.add(existing);
    }
    list.add(value);
    value=list;
  }
  headers.put(key,value);
}
