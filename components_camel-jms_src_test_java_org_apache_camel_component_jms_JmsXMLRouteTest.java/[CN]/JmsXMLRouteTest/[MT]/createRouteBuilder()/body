{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:start").to("activemq:queue:foo");
      from("activemq:queue:foo").process(new Processor(){
        public void process(        Exchange exchange) throws Exception {
          Object body=exchange.getIn().getBody();
          assertIsInstanceOf(Source.class,body);
        }
      }
).choice().when().xpath("/person/city = 'London'").to("mock:london").when().xpath("/person/city = 'Tampa'").to("mock:tampa").otherwise().to("mock:unknown").end();
    }
  }
;
}
