{
  super.doStart();
  if (scheduledExecutorService == null) {
    this.scheduledExecutorService=getEndpoint().getCamelContext().getExecutorServiceManager().newScheduledThreadPool(this,getEndpoint().getEndpointUri(),1);
    this.shutdownExecutor=true;
  }
  ObjectHelper.notNull(scheduledExecutorService,"scheduledExecutorService",this);
  ObjectHelper.notNull(pollStrategy,"pollStrategy",this);
  if (isStartScheduler()) {
    startScheduler();
  }
}
