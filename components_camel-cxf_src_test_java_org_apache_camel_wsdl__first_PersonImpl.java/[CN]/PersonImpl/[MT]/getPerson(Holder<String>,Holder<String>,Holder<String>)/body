{
  if (personId.value == null || personId.value.length() == 0) {
    org.apache.camel.wsdl_first.types.UnknownPersonFault fault=new org.apache.camel.wsdl_first.types.UnknownPersonFault();
    fault.setPersonId(personId.value);
    throw new UnknownPersonFault("Get the null value of person name",fault);
  }
  name.value="Bonjour";
  ssn.value="000-000-0000";
}
