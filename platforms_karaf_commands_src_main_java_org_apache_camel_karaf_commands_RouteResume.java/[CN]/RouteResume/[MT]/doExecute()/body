{
  Route camelRoute=camelController.getRoute(route,context);
  if (camelRoute == null) {
    System.err.println("Camel route " + route + " not found.");
    return null;
  }
  CamelContext camelContext=camelRoute.getRouteContext().getCamelContext();
  camelContext.resumeRoute(route);
  return null;
}
