{
  try {
    if (channel.isConnected()) {
      LOG.trace("Putting channel back to pool {}",channel);
      pool.returnObject(channel);
    }
  }
 catch (  Exception e) {
    LOG.warn("Error returning channel to pool {}. This exception will be ignored.",channel);
  }
 finally {
    callback.done(doneSync);
  }
}
