{
  UserPojo user=new UserPojo();
  user.setId(123);
  user.setName("Donald Duck");
  CountryPojo pojo=fluentTemplate.to("rest:post:users/lives").withHeader(Exchange.CONTENT_TYPE,"application/json").withBody(user).request(CountryPojo.class);
  assertNotNull(pojo);
  assertEquals("EN",pojo.getIso());
  assertEquals("England",pojo.getCountry());
}
