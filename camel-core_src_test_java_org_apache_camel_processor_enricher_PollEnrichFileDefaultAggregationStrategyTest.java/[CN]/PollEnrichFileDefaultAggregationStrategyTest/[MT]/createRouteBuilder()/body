{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("timer:foo?period=1000&repeatCount=1").setBody().constant("Hello from Camel.").pollEnrich("file:target/pollEnrich?fileName=enrich.txt&readLock=markerFile").convertBodyTo(String.class).log("The body is ${body}").to("mock:result");
    }
  }
;
}
