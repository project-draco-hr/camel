{
  exchange.getIn().setHeader("foo",123);
  assertExpression(exchange,"${header.foo}",123);
  assertExpression(exchange,"header.foo",123);
  assertPredicate("${header.foo}",true);
  assertPredicate("header.foo",true);
  assertPredicate("${header.unknown}",false);
  assertPredicate("header.unknown",false);
}
