{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:start").wireTap().constant("direct:tap").newExchangeBody(constant("Bye World")).newExchangeHeader("id",constant(123)).newExchangeHeader("date",simple("${date:now:yyyyMMdd}")).end().to("mock:result");
      from("direct:tap").to("mock:tap");
    }
  }
;
}
