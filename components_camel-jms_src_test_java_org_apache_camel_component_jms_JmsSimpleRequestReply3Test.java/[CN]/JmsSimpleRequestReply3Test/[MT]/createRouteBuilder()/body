{
  return new RouteBuilder(){
    public void configure() throws Exception {
      from("direct:start").process(new Processor(){
        @Override public void process(        Exchange exchange) throws Exception {
          exchange.getIn().setHeader(JmsConstants.JMS_REPLY_TO_NAME,"queue:myqueue");
        }
      }
).inOut("activemq:queue:foo").to("mock:result");
      from("activemq:queue:foo").process(new Processor(){
        public void process(        Exchange exchange) throws Exception {
          assertEquals("Excepect a wrong replyto name","queue://queue:myqueue",exchange.getIn().getHeader("JMSReplyTo").toString());
        }
      }
).transform(body().prepend("Hello "));
    }
  }
;
}
