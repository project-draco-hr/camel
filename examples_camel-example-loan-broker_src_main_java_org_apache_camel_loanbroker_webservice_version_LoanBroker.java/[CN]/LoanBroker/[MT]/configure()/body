{
  from(Constants.LOANBROKER_URI).process(new CreditScoreProcessor(Constants.CREDITAGENCY_ADDRESS)).multicast(new BankResponseAggregationStrategy()).to(Constants.BANK1_URI,Constants.BANK2_URI,Constants.BANK3_URI);
  from(Constants.PARALLEL_LOANBROKER_URI).process(new CreditScoreProcessor(Constants.CREDITAGENCY_ADDRESS)).multicast(new BankResponseAggregationStrategy(),true).to(Constants.BANK1_URI,Constants.BANK2_URI,Constants.BANK3_URI);
}
