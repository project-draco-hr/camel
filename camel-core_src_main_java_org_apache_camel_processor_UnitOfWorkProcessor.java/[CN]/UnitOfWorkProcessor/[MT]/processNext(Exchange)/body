{
  if (exchange.getUnitOfWork() == null) {
    final DefaultUnitOfWork uow=new DefaultUnitOfWork(exchange);
    exchange.setUnitOfWork(uow);
    try {
      uow.start();
    }
 catch (    Exception e) {
      throw wrapRuntimeCamelException(e);
    }
    try {
      processor.process(exchange);
    }
 catch (    Exception e) {
      exchange.setException(e);
    }
 finally {
      done(uow,exchange);
    }
  }
 else {
    processor.process(exchange);
  }
}
