{
  LOG.trace("Exchange marked UnitOfWork to be processed synchronously: {}",exchange);
  try {
    AsyncProcessorHelper.process(processor,exchange);
  }
 catch (  Throwable e) {
    exchange.setException(e);
  }
  try {
    callback.done(true);
  }
  finally {
    doneUow(uow,exchange);
  }
  return true;
}
