{
  Object value=getHeader(name);
  if (value == null) {
    return null;
  }
  if (type.isInstance(value)) {
    return type.cast(value);
  }
  Exchange e=getExchange();
  if (e != null) {
    return e.getContext().getTypeConverter().convertTo(type,e,value);
  }
 else {
    return type.cast(value);
  }
}
