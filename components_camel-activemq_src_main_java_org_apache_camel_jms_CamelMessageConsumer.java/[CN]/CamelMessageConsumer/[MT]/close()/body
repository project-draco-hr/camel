{
  if (!closed) {
    closed=true;
    try {
      if (consumer != null) {
        consumer.stop();
      }
      if (pollingConsumer != null) {
        pollingConsumer.stop();
      }
    }
 catch (    JMSException e) {
      throw e;
    }
catch (    Exception e) {
      throw JMSExceptionSupport.create(e);
    }
  }
}
