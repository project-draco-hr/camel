{
  if (closeable != null) {
    try {
      closeable.close();
    }
 catch (    IOException e) {
      if (log == null) {
        log=LOG;
      }
      if (name != null) {
        log.warn("Cannot close: " + name + ". Reason: "+ e.getMessage(),e);
      }
 else {
        log.warn("Cannot close. Reason: " + e.getMessage(),e);
      }
    }
  }
}
