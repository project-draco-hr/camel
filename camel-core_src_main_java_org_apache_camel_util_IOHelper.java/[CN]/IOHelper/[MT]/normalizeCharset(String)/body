{
  if (charset != null) {
    String answer=charset.trim();
    if (answer.startsWith("'") || answer.startsWith("\"")) {
      answer=answer.substring(1);
    }
    if (answer.endsWith("'") || answer.endsWith("\"")) {
      answer=answer.substring(0,answer.length() - 1);
    }
    return answer.trim();
  }
 else {
    return null;
  }
}
