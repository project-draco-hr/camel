{
  try {
    channel.force(true);
  }
 catch (  Exception e) {
    if (log != null) {
      if (name != null) {
        log.warn("Cannot force FileChannel: " + name + ". Reason: "+ e.getMessage(),e);
      }
 else {
        log.warn("Cannot force FileChannel. Reason: " + e.getMessage(),e);
      }
    }
  }
}
