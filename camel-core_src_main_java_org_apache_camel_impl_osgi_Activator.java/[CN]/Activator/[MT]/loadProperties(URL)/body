{
  Properties properties=new Properties();
  BufferedInputStream reader=null;
  try {
    reader=new BufferedInputStream(url.openStream());
    properties.load(reader);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
 finally {
    try {
      if (reader != null) {
        reader.close();
      }
    }
 catch (    Exception ignore) {
    }
  }
  return properties;
}
