{
  String userDir="";
  String answer="";
  if (ObjectHelper.isNotEmpty(base)) {
    try {
      userDir=FileUtil.getUserDir().toString();
    }
 catch (    Exception ex) {
    }
    if (ObjectHelper.isNotEmpty(base) && base.startsWith("file") && base.startsWith(userDir)) {
      answer=FileUtil.onlyPath(base.substring(userDir.length())) + "/";
    }
  }
  return answer;
}
