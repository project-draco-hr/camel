{
  if (jmsMessage != null) {
    try {
      Object value=jmsMessage.getObjectProperty(name);
      try {
        return value;
      }
 catch (      ClassCastException e) {
        throw new InvalidHeaderTypeException(e.getMessage(),value);
      }
    }
 catch (    JMSException e) {
      throw new MessagePropertyAcessException(name,e);
    }
  }
  return null;
}
