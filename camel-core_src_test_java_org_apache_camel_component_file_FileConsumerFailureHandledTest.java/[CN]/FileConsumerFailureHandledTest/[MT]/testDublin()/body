{
  MockEndpoint mock=getMockEndpoint("mock:beer");
  mock.expectedBodiesReceived("Dublin");
  template.sendBodyAndHeader("file:target/messages/input/?delete=true&delay=5000","Dublin",Exchange.FILE_NAME,"dublin.txt");
