{
  super.doStop();
  try {
    if (subscriptionHelper != null) {
      ServiceHelper.stopService(subscriptionHelper);
    }
    if (session != null && session.getAccessToken() != null) {
      try {
        ServiceHelper.stopService(session);
      }
 catch (      SalesforceException ignored) {
      }
    }
  }
  finally {
    if (httpClient != null) {
      httpClient.stop();
    }
  }
  if (classMap != null) {
    classMap.clear();
  }
}
