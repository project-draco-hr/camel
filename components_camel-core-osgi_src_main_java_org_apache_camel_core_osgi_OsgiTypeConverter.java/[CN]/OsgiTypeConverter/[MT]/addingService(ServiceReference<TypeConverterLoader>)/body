{
  LOG.trace("AddingService: {}",serviceReference);
  TypeConverterLoader loader=bundleContext.getService(serviceReference);
  if (loader != null) {
    try {
      loader.load(getDelegate());
    }
 catch (    Throwable t) {
      throw ObjectHelper.wrapRuntimeCamelException(t);
    }
  }
  return loader;
}
