{
  stopping=true;
  if (channel == null) {
    return;
  }
  if (tag != null && isChannelOpen()) {
    channel.basicCancel(tag);
  }
  try {
    lock.acquire();
    if (isChannelOpen()) {
      channel.close();
    }
    lock.release();
  }
 catch (  TimeoutException e) {
    log.error("Timeout occured");
    throw e;
  }
catch (  InterruptedException e1) {
    log.error("Thread Interrupted!");
  }
}
