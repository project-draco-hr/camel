{
  Class clazz=doLoadClass(name,Thread.currentThread().getContextClassLoader());
  if (clazz == null) {
    clazz=doLoadClass(name,loader);
  }
  if (clazz == null) {
    clazz=doLoadClass(name,ObjectHelper.class.getClassLoader());
  }
  if (clazz == null) {
    LOG.warn("Cannot find class: " + name);
  }
  return clazz;
}
