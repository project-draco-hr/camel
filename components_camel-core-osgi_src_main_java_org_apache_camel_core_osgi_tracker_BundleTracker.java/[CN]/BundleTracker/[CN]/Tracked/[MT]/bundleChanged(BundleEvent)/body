{
  if (closed) {
    return;
  }
  final Bundle bundle=event.getBundle();
  final int state=bundle.getState();
  if (DEBUG) {
    System.out.println("BundleTracker.Tracked.bundleChanged[" + state + "]: "+ bundle);
  }
  if ((state & mask) != 0) {
    track(bundle,event);
  }
 else {
    untrack(bundle,event);
  }
}
