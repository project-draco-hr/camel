{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  Book book=(Book)o;
  if (id != null ? !id.equals(book.id) : book.id != null) {
    return false;
  }
  if (isbn != null ? !isbn.equals(book.isbn) : book.isbn != null) {
    return false;
  }
  if (title != null ? !title.equals(book.title) : book.title != null) {
    return false;
  }
  return true;
}
