{
  List<Byte> bytes=new ArrayList<>();
  ByteBuffer buf=record.getData().asReadOnlyBuffer();
  while (buf.hasRemaining()) {
    bytes.add(buf.get());
  }
  byte[] a=new byte[bytes.size()];
  for (int i=0; i < bytes.size(); ++i) {
    a[i]=bytes.get(i);
  }
  return new String(a,Charset.forName("UTF-8"));
}
