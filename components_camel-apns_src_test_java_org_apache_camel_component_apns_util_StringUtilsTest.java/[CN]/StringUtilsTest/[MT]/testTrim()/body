{
  Assert.assertEquals("",StringUtils.trim(""));
  Assert.assertEquals("",StringUtils.trim(" "));
  Assert.assertEquals("test",StringUtils.trim("test"));
  Assert.assertEquals("test",StringUtils.trim("test "));
  Assert.assertEquals("test",StringUtils.trim(" test"));
  Assert.assertEquals("test",StringUtils.trim(" test "));
  Assert.assertEquals(null,StringUtils.trim(null));
}
