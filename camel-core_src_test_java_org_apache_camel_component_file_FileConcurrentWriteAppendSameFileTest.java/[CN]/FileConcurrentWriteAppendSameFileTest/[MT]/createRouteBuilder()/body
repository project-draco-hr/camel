{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("file:target/concurrent").routeId("foo").noAutoStartup().split(body().tokenize("\n")).parallelProcessing().streaming().setBody(body().append(":Status=OK\n")).to("file:target/concurrent/outbox?fileExist=Append&fileName=result.txt").to("mock:result").end();
    }
  }
;
}
