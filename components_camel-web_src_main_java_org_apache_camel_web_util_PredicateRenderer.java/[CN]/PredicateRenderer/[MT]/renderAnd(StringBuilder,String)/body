{
  buffer.append("and(");
  String predicate1=predicate.substring(1,predicate.indexOf(") and ("));
  String predicate2=predicate.substring(predicate.indexOf(") and (") + 7,predicate.length() - 1);
  render(buffer,predicate1);
  buffer.append(", ");
  render(buffer,predicate2);
  buffer.append(")");
}
