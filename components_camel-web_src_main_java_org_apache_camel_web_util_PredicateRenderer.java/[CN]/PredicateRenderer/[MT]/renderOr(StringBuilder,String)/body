{
  buffer.append("or(");
  String predicate1=predicate.substring(1,predicate.indexOf(") or ("));
  String predicate2=predicate.substring(predicate.indexOf(") or (") + 6,predicate.length() - 1);
  render(buffer,predicate1);
  buffer.append(", ");
  render(buffer,predicate2);
  buffer.append(")");
}
