{
  for (int i=0; i < endpoint.getConcurrentConsumers(); i++) {
    createConsumer();
  }
  try {
    for (    RabbitConsumer consumer : this.consumers) {
      consumer.start();
    }
  }
 catch (  Exception e) {
    log.info("Connection failed, will start background thread to retry!",e);
    reconnect();
  }
}
