{
  Map<String,Object> properties=new HashMap<String,Object>();
  properties.put(PackagesResourceConfig.PROPERTY_PACKAGES,CamelContextResource.class.getPackage().getName());
  WadlGeneratorConfig config=WadlGeneratorConfig.generator(WadlGeneratorApplicationDoc.class).prop("applicationDocsFile","classpath:/application-doc.xml").generator(WadlGeneratorGrammarsSupport.class).prop("grammarsFile","classpath:/application-grammars.xml").generator(WadlGeneratorResourceDocSupport.class).prop("resourceDocFile","classpath:/resourcedoc.xml").build();
  properties.put(ResourceConfig.PROPERTY_WADL_GENERATOR_CONFIG,config);
  return properties;
}
