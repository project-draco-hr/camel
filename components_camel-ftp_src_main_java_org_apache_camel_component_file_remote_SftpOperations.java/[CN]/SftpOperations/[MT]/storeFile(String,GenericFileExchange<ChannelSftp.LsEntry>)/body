{
  try {
    InputStream in=ExchangeHelper.getMandatoryInBody(exchange,InputStream.class);
    channel.put(in,name);
    return true;
  }
 catch (  SftpException e) {
    throw new GenericFileOperationFailedException("Cannot store file: " + name,e);
  }
catch (  InvalidPayloadException e) {
    throw new GenericFileOperationFailedException("Cannot store file: " + name,e);
  }
}
