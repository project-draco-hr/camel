{
  LOG.trace("fastExistsFile({})",name);
  try {
    Vector files=channel.ls(name);
    if (files == null) {
      return false;
    }
    return files.size() >= 1;
  }
 catch (  SftpException e) {
    if (ChannelSftp.SSH_FX_NO_SUCH_FILE == e.id) {
      return false;
    }
    throw new GenericFileOperationFailedException(e.getMessage(),e);
  }
}
