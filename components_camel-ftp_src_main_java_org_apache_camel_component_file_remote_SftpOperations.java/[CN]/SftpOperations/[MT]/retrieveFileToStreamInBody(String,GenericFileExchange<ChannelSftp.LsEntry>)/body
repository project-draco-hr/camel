{
  try {
    GenericFile<ChannelSftp.LsEntry> target=exchange.getGenericFile();
    OutputStream os=new ByteArrayOutputStream();
    target.setBody(os);
    channel.get(name,os);
    return true;
  }
 catch (  SftpException e) {
    throw new GenericFileOperationFailedException("Cannot retrieve file: " + name,e);
  }
}
