{
  return new RouteBuilder(){
    public void configure() throws Exception {
      DataFormat hl7=new HL7DataFormat();
      from("netty4:tcp://127.0.0.1:" + getPort() + "?sync=true&decoder=#hl7decoder&encoder=#hl7encoder").unmarshal(hl7).choice().when(header("CamelHL7TriggerEvent").isEqualTo("A19")).beanRef("hl7service","handleA19").to("mock:a19").when(header("CamelHL7TriggerEvent").isEqualTo("A01")).to("mock:a01").beanRef("hl7service","handleA01").to("mock:a19").otherwise().to("mock:unknown").end().marshal(hl7);
    }
  }
;
}
