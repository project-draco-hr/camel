{
  if (isStarting()) {
    return ServiceStatus.Starting;
  }
  if (isStarted()) {
    return ServiceStatus.Started;
  }
  if (isStopping()) {
    return ServiceStatus.Stopping;
  }
  if (isStopped()) {
    return ServiceStatus.Stopped;
  }
  if (isSuspending()) {
    return ServiceStatus.Suspending;
  }
  if (isSuspended()) {
    return ServiceStatus.Suspended;
  }
  return ServiceStatus.Stopped;
}
