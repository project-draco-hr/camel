{
  if (isStarting() || isStarted()) {
    LOG.trace("Service already started");
    return;
  }
  if (starting.compareAndSet(false,true)) {
    LOG.trace("Starting service");
    doStart();
    started.set(true);
    starting.set(false);
    stopping.set(false);
    stopped.set(false);
    suspending.set(false);
    suspended.set(false);
    shutdown.set(false);
    shuttingdown.set(false);
  }
}
