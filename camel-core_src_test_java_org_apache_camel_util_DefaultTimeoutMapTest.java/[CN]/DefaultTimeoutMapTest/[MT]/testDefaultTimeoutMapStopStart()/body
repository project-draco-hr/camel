{
  DefaultTimeoutMap<String,Integer> map=new DefaultTimeoutMap<String,Integer>(executor);
  map.put("A",1,5000);
  assertEquals(1,map.size());
  map.stop();
  assertEquals(0,map.size());
  map.put("A",1,500);
  Thread.sleep(1000);
  assertEquals(1,map.size());
  map.start();
  Thread.sleep(2000);
  assertEquals(0,map.size());
}
