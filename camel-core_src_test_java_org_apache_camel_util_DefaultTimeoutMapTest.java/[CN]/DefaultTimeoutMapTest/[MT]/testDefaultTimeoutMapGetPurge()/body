{
  DefaultTimeoutMap<String,Integer> map=new DefaultTimeoutMap<String,Integer>();
  assertTrue(map.currentTime() > 0);
  assertEquals(0,map.size());
  map.put("A",123,500);
  assertEquals(1,map.size());
  Thread.sleep(2000);
  assertEquals(123,(int)map.get("A"));
  map.purge();
  assertEquals(1,map.size());
}
