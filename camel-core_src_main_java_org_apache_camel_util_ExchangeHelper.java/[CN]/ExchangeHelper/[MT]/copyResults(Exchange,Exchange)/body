{
  if (result != source) {
    result.setException(source.getException());
    Message fault=source.getFault(false);
    if (fault != null) {
      result.getFault(true).copyFrom(fault);
    }
    Message out=source.getOut(false);
    if (out != null) {
      result.getOut(true).copyFrom(out);
    }
  }
}
