{
  Endpoint<E> endpoint;
  if (value instanceof Endpoint) {
    endpoint=(Endpoint<E>)value;
  }
 else {
    String uri=value.toString();
    endpoint=(Endpoint<E>)exchange.getContext().getEndpoint(uri);
    if (endpoint == null) {
      throw new NoSuchEndpointException(uri);
    }
  }
  return endpoint;
}
