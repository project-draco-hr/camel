{
  if (oldExchange != null) {
    if (oldExchange.hasOut()) {
      oldExchange.getIn().copyFrom(oldExchange.getOut());
      oldExchange.setOut(null);
    }
  }
  if (newExchange != null) {
    if (newExchange.hasOut()) {
      newExchange.getIn().copyFrom(newExchange.getOut());
      newExchange.setOut(null);
    }
  }
}
