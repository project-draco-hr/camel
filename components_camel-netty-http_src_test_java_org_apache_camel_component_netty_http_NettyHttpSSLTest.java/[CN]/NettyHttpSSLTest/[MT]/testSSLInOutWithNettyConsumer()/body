{
  if (isJavaVendor("ibm")) {
    return;
  }
  getMockEndpoint("mock:input").expectedBodiesReceived("Hello World");
  context.addRoutes(new RouteBuilder(){
    public void configure(){
      from("netty-http:http://localhost:{{port}}?ssl=true&passphrase=changeit&keyStoreResource=jsse/localhost.ks&trustStoreResource=jsse/localhost.ks").to("mock:input").transform().constant("Bye World");
    }
  }
);
  context.start();
  String out=template.requestBody("https://localhost:{{port}}/foo","Hello World",String.class);
  assertEquals("Bye World",out);
  assertMockEndpointsSatisfied();
}
