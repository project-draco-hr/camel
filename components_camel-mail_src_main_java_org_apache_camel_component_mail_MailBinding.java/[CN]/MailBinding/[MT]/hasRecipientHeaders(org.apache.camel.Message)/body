{
  for (  String key : camelMessage.getHeaders().keySet()) {
    if (Message.RecipientType.TO.toString().equals(key)) {
      return true;
    }
 else     if (Message.RecipientType.CC.toString().equals(key)) {
      return true;
    }
 else     if (Message.RecipientType.BCC.toString().equals(key)) {
      return true;
    }
  }
  return false;
}
