{
  ConsumerIterator<byte[],byte[]> it=stream.iterator();
  while (it.hasNext()) {
    MessageAndMetadata<byte[],byte[]> mm=it.next();
    Exchange exchange=endpoint.createKafkaExchange(mm);
    try {
      processor.process(exchange);
    }
 catch (    Exception e) {
      LOG.error(e.getMessage(),e);
    }
  }
}
