{
  char buffer[]=new char[len];
  int readed=in.read(buffer,0,len - off);
  if (readed >= 0) {
    int copyed=0;
    for (int i=0; i < readed; i++) {
      if (!isFiltered(buffer[i])) {
        cbuf[off + copyed]=buffer[i];
        copyed++;
      }
 else {
      }
    }
    return copyed;
  }
 else {
    return readed;
  }
}
