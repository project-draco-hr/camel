{
switch (operationName) {
case CREATE_JOB:
case GET_JOB:
case CLOSE_JOB:
case ABORT_JOB:
case CREATE_BATCH:
case GET_BATCH:
case GET_ALL_BATCHES:
case GET_REQUEST:
case GET_RESULTS:
case CREATE_BATCH_QUERY:
case GET_QUERY_RESULT_IDS:
case GET_QUERY_RESULT:
    return true;
default :
  return false;
}
}
