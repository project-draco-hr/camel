{
  final CountDownLatch latch=new CountDownLatch(1);
  boolean sync=processor.process(exchange,new AsyncCallback(){
    public void done(    boolean doneSync){
      if (!doneSync) {
        latch.countDown();
      }
    }
    @Override public String toString(){
      return "Done " + processor;
    }
  }
);
  if (!sync) {
    latch.await();
  }
}
