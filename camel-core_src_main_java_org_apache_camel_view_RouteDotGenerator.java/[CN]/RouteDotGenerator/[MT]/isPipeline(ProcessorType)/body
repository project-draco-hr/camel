{
  if (node instanceof MulticastType) {
    return false;
  }
  if (node instanceof PipelineType) {
    return true;
  }
  if (node.getOutputs().size() > 1) {
    for (    Object type : node.getOutputs()) {
      if (!(type instanceof ToType)) {
        return false;
      }
    }
    return true;
  }
  return false;
}
