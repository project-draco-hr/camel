{
  String toID=getID(node);
  writer.println();
  writer.print(toID);
  writer.println(" [");
  printNodeAttributes(writer,node,fromID);
  writer.println("];");
  writer.println();
  writer.print(fromID);
  writer.print(" -> ");
  writer.print(toID);
  writer.println(" [");
  writer.println("];");
  List<ProcessorType> outputs=node.getOutputs();
  for (  ProcessorType output : outputs) {
    printNode(writer,toID,output);
  }
  return toID;
}
