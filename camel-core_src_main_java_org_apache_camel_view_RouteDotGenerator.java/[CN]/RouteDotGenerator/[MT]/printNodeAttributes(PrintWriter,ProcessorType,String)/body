{
  NodeData nodeData=new NodeData();
  configureNodeData(node,nodeData);
  String label=nodeData.label;
  if (label == null) {
    label=node.toString();
  }
  writer.println("label = \"" + label + "\"");
  String image=nodeData.image;
  if (image != null) {
    writer.println("shapefile = \"" + image + "\"");
    writer.println("shape = custom");
  }
}
