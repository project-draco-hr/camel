{
  writer.println("label = \"" + nodeData.label + "\"");
  writer.println("tooltip = \"" + nodeData.tooltop + "\"");
  String image=nodeData.image;
  if (image != null) {
    writer.println("shapefile = \"" + image + "\"");
    writer.println("shape = custom");
    writer.println("peripheries=0");
  }
  return nodeData;
}
