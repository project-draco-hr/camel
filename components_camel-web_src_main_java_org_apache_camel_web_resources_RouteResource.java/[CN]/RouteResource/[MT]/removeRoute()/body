{
  URI routesURI=null;
  try {
    routesURI=new URI("/routes");
    getCamelContext().removeRouteDefinition(route);
    return Response.seeOther(routesURI).build();
  }
 catch (  Exception e) {
    LOG.error("failed to remove route " + id + ", error "+ e.getMessage());
    return Response.seeOther(routesURI).build();
  }
}
