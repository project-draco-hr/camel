{
  for (  Map<String,String> row : rows) {
    String type=null;
    boolean found=false;
    if (row.containsKey("name")) {
      found=name.equals(row.get("name"));
    }
    if (row.containsKey("type")) {
      type=row.get("type");
    }
    if (found) {
      return "number".equals(type);
    }
  }
  return false;
}
