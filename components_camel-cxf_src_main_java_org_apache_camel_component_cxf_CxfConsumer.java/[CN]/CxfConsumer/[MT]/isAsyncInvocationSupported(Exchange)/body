{
  Message cxfMessage=cxfExchange.getInMessage();
  AddressingProperties addressingProperties=(AddressingProperties)cxfMessage.get(CxfConstants.WSA_HEADERS_INBOUND);
  if (addressingProperties != null && !ContextUtils.isGenericAddress(addressingProperties.getReplyTo())) {
    return false;
  }
  return true;
}
