{
  return new RouteBuilder(){
    public void configure(){
      from("direct:start").to("xslt:xslt/template.xsl").setHeader("foo",constant("bar")).to("fop:pdf").setHeader(Exchange.FILE_NAME,constant("result.pdf")).to("file:target/data").to("mock:result");
      from("direct:error").to("xslt:xslt/template.xsl").setHeader("foo",constant("bar")).to("fop:test").setHeader(Exchange.FILE_NAME,constant("result.pdf")).to("file:target/data").to("mock:result");
    }
  }
;
}
