{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:start").streamCaching().toF("sap-netweaver:%s?username=%s&password=%s",url,username,password).to("log:response?showStreams=true").to("velocity:flight-info.vm").to("log:info").to("mock:result");
    }
  }
;
}
