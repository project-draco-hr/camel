{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      Processor serviceProc=new Processor(){
        public void process(        Exchange exchange) throws Exception {
          String uri=exchange.getIn().getHeader(Exchange.HTTP_URI,String.class);
          exchange.getOut().setBody(uri);
        }
      }
;
      from("jetty:http://localhost:9080/test/hello").to("ahc:http://localhost:9081?throwExceptionOnFailure=false&bridgeEndpoint=true");
      from("jetty:http://localhost:9081?matchOnUriPrefix=true").process(serviceProc);
    }
  }
;
}
