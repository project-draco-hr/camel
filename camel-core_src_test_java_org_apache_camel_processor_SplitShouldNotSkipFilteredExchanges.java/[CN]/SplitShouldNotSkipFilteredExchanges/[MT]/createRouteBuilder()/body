{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:start").to("direct:split").to("mock:result");
      Predicate goodWord=body().contains("World");
      from("direct:split").split(body(List.class),new MyAggregationStrategy()).filter(goodWord).to("mock:filtered");
    }
  }
;
}
