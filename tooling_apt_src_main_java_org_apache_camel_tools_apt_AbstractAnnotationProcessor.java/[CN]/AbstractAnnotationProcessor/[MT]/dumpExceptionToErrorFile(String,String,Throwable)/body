{
  File file=new File(fileName);
  try {
    FileOutputStream fos=new FileOutputStream(file);
    StringWriter sw=new StringWriter();
    PrintWriter pw=new PrintWriter(sw);
    e.printStackTrace(pw);
    fos.write(sw.toString().getBytes());
    pw.close();
    sw.close();
    fos.close();
  }
 catch (  Throwable t) {
  }
}
