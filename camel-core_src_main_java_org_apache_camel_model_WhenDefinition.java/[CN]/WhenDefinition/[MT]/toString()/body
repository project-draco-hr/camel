{
  StringBuilder sb=new StringBuilder();
  if (getExpression() != null) {
    String language=getExpression().getLanguage();
    if (language != null) {
      sb.append(language).append("{");
    }
    sb.append(getExpression().getLabel());
    if (language != null) {
      sb.append("}");
    }
  }
  return "When[" + sb.toString() + " -> "+ getOutputs()+ "]";
}
