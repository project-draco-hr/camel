{
  Map<String,Object> params=new HashMap<String,Object>(1);
  params.put("name",processName);
  List<ProcessDefinition> list=CastUtils.cast(jpaTemplate.findByNamedParams("select x from " + QueryUtils.getTypeName(ProcessDefinition.class) + " x where x.name = :name",params));
  if (!list.isEmpty()) {
    return list.get(0);
  }
 else {
    ProcessDefinition answer=new ProcessDefinition();
    answer.setName(processName);
    jpaTemplate.persist(answer);
    return answer;
  }
}
