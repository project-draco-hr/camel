{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("netty-http:http://localhost/foo?nettySharedHttpServer=#myNettyServer").to("mock:foo").transform().constant("Bye World");
      from("netty-http:http://localhost/bar?nettySharedHttpServer=#myNettyServer").to("mock:bar").transform().constant("Bye Camel");
    }
  }
;
}
