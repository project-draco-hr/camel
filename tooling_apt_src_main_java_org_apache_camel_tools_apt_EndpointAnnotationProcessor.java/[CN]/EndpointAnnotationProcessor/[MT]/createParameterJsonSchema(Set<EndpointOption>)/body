{
  StringBuilder buffer=new StringBuilder("{\n  \"properties\": {");
  boolean first=true;
  for (  EndpointOption entry : options) {
    if (first) {
      first=false;
    }
 else {
      buffer.append(",");
    }
    buffer.append("\n    ");
    buffer.append(JsonSchemaHelper.toJson(entry.getName(),entry.getType(),entry.getDocumentation()));
  }
  buffer.append("\n  }\n}\n");
  return buffer.toString();
}
