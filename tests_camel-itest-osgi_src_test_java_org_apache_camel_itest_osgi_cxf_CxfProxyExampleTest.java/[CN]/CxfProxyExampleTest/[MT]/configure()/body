{
  Option[] options=combine(getDefaultCamelKarafOptions(),loadCamelFeatures("camel-http","camel-cxf"),provision(newBundle().add(org.apache.camel.example.reportincident.InputReportIncident.class).add(org.apache.camel.example.reportincident.OutputReportIncident.class).add(org.apache.camel.example.reportincident.ReportIncidentEndpoint.class).add(org.apache.camel.example.reportincident.ReportIncidentEndpointService.class).add(org.apache.camel.example.reportincident.ObjectFactory.class).build(withBnd())));
  return options;
}
