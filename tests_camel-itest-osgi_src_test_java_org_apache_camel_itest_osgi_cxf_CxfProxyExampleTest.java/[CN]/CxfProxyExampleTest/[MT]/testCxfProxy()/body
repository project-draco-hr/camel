{
  InputReportIncident input=new InputReportIncident();
  input.setIncidentId("123");
  input.setIncidentDate("2010-09-28");
  input.setGivenName("Claus");
  input.setFamilyName("Ibsen");
  input.setSummary("Bla");
  input.setDetails("Bla bla");
