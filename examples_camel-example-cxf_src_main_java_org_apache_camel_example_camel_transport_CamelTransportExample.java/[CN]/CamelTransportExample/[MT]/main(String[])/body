{
  Server server=new Server();
  try {
    server.prepare();
    server.start();
    Client client=new Client("http://localhost:9091/GreeterContext/GreeterPort");
    client.invoke();
    Thread.sleep(1000);
  }
 catch (  Exception ex) {
    ex.printStackTrace();
  }
 finally {
    server.stop();
    System.exit(0);
  }
}
