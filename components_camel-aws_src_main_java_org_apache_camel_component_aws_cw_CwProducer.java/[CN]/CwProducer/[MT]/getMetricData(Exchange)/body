{
  Object body=exchange.getIn().getBody();
  if (body instanceof List) {
    return CastUtils.cast((List<?>)body);
  }
  if (body instanceof MetricDatum) {
    return Arrays.asList((MetricDatum)body);
  }
  return Arrays.asList(new MetricDatum().withMetricName(determineName(exchange)).withValue(determineValue(exchange)).withUnit(determineUnit(exchange)).withTimestamp(determineTimestamp(exchange)));
}
