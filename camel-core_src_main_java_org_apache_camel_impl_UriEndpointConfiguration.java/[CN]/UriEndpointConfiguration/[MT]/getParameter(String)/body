{
  ParameterConfiguration config=getPropertyConfiguration(name);
  try {
    @SuppressWarnings("unchecked") T answer=(T)IntrospectionSupport.getProperty(endpoint,name);
    if (config == null) {
      warnMissingUriParamOnProperty(name);
    }
    return answer;
  }
 catch (  Exception e) {
    throw new RuntimeCamelException("Failed to get property '" + name + "' on "+ endpoint+ " due "+ e.getMessage(),e);
  }
}
