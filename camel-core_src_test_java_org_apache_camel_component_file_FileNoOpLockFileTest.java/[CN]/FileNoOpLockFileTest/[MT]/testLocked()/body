{
  deleteDirectory("target/reports");
  MockEndpoint mock=getMockEndpoint("mock:report");
  mock.expectedBodiesReceived("Hello Locked");
  template.sendBodyAndHeader("file:target/reports/locked","Hello Locked",FileComponent.HEADER_FILE_NAME,"report.txt");
  mock.assertIsSatisfied();
  Thread.sleep(200);
  checkLockFile(false);
}
