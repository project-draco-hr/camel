{
  MockEndpoint mock=getMockEndpoint("mock:report");
  mock.expectedBodiesReceived("Hello Locked");
  template.sendBodyAndHeader("newfile:target/reports/locked","Hello Locked",NewFileComponent.HEADER_FILE_NAME,"report.txt");
  mock.assertIsSatisfied();
  Thread.sleep(200);
  checkLockFile(false);
}
