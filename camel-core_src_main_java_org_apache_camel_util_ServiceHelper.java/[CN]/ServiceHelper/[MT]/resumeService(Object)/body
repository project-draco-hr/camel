{
  if (service instanceof SuspendableService) {
    SuspendableService ss=(SuspendableService)service;
    if (ss.isSuspended()) {
      if (LOG.isDebugEnabled()) {
        LOG.debug("Resuming service " + service);
      }
      ss.resume();
      return true;
    }
 else {
      return false;
    }
  }
 else {
    startService(service);
    return true;
  }
}
