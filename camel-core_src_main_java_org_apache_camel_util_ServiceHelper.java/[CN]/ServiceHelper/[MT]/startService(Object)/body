{
  if (isStarted(value)) {
    LOG.trace("Service already started: {}",value);
    return;
  }
  if (value instanceof Service) {
    Service service=(Service)value;
    LOG.trace("Starting service: {}",service);
    service.start();
  }
 else   if (value instanceof Collection) {
    startServices((Collection<?>)value);
  }
}
