{
  if (service instanceof SuspendableService) {
    SuspendableService ss=(SuspendableService)service;
    if (!ss.isSuspended()) {
      LOG.trace("Suspending service {}",service);
      ss.suspend();
      return true;
    }
 else {
      return false;
    }
  }
 else {
    stopService(service);
    return true;
  }
}
