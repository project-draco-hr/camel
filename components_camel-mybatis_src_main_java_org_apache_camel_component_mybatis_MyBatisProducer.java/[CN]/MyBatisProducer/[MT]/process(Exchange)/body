{
switch (endpoint.getStatementType()) {
case SelectOne:
    doSelectOne(exchange);
  break;
case SelectList:
doSelectList(exchange);
break;
case Insert:
doInsert(exchange);
break;
case Update:
doUpdate(exchange);
break;
case Delete:
doDelete(exchange);
break;
default :
throw new IllegalArgumentException("Unsupported statementType: " + endpoint.getStatementType());
}
}
