{
  CamelContext context=new DefaultCamelContext();
  String json=context.getComponentParameterJsonSchema("direct");
  assertNotNull("Should have found some auto-generated JSON",json);
  log.info(json);
  assertTrue("Should include javadoc",json.contains("\"timeout\": { \"kind\": \"parameter\", \"type\": \"integer\", \"javaType\": \"long\"," + " \"defaultValue\": \"30000\"," + " \"description\": \"The timeout value to use if block is enabled. Is by default 30000.\""));
}
