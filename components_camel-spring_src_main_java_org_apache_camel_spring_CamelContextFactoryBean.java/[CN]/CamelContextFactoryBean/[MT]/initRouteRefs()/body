{
  if (routeRefs != null) {
    for (    RouteContextRefDefinition ref : routeRefs) {
      List<RouteDefinition> defs=ref.lookupRoutes(getContext());
      for (      RouteDefinition def : defs) {
        if (LOG.isDebugEnabled()) {
          LOG.debug("Adding route from " + ref + " -> "+ def);
        }
        routes.add(0,def);
      }
    }
  }
}
