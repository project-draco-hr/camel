{
  String uri=ep.getEndpointUri();
  int pos=uri.indexOf(':');
  String id=(pos == -1) ? uri : "[" + uri.substring(0,pos) + "]"+ uri.substring(pos + 1);
  if (!ep.isSingleton()) {
    id+="." + Integer.toString(ep.hashCode());
  }
  id=id.replace("=","_eq_");
  id=id.replace(",","_cm_");
  return id;
}
