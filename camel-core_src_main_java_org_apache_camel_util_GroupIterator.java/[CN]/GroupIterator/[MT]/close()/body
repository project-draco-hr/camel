{
  try {
    if (it instanceof Scanner) {
      Scanner scanner=(Scanner)it;
      scanner.close();
      IOException ioException=scanner.ioException();
      if (ioException != null) {
        throw ioException;
      }
    }
 else     if (it instanceof Closeable) {
      IOHelper.closeWithException((Closeable)it);
    }
  }
  finally {
    bos.close();
    closed=true;
  }
}
