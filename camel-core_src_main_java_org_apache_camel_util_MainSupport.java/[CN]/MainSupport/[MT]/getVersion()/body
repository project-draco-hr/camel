{
  Package aPackage=Package.getPackage("org.apache.camel");
  if (aPackage != null) {
    String version=aPackage.getImplementationVersion();
    if (version == null) {
      version=aPackage.getSpecificationVersion();
      if (version == null) {
        version="";
      }
    }
    return version;
  }
  return "";
}
