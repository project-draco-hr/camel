{
  for (  FilterProcessor filterProcessor : filters) {
    Predicate predicate=filterProcessor.getPredicate();
    boolean matches=false;
    try {
      if (predicate != null) {
        matches=predicate.matches(exchange);
      }
    }
 catch (    Throwable e) {
      exchange.setException(e);
      callback.done(true);
      return true;
    }
    if (matches) {
      return filterProcessor.processNext(exchange,callback);
    }
  }
  if (otherwise != null) {
    return AsyncProcessorHelper.process(otherwise,exchange,callback);
  }
 else {
    callback.done(true);
    return true;
  }
}
