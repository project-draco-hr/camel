{
  localPort=port;
  localHost=host;
  try {
    socket=new DatagramSocket();
    address=InetAddress.getByName(localHost);
  }
 catch (  UnknownHostException ex) {
    java.util.logging.Logger.getLogger(Mina2UdpNoCamelTest.class.getName()).log(Level.SEVERE,null,ex);
  }
catch (  SocketException ex) {
