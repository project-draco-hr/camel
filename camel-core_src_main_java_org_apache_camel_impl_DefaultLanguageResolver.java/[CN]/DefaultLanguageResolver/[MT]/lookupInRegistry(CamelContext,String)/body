{
  Object bean=null;
  for (  String name : names) {
    try {
      bean=context.getRegistry().lookupByName(name);
      if (bean != null) {
        getLog().debug("Found language: {} in registry: {}",name,bean);
      }
    }
 catch (    Exception e) {
      if (getLog().isDebugEnabled()) {
        getLog().debug("Ignored error looking up bean: " + name + ". Error: "+ e);
      }
    }
    if (bean != null) {
      return bean;
    }
  }
  return null;
}
