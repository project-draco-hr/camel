{
  context.addRoutes(new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:start").to("{{foo:mock:foo}}").to("{{bar}}");
    }
  }
);
  try {
    context.start();
    fail("Should have thrown exception");
  }
 catch (  FailedToCreateRouteException e) {
