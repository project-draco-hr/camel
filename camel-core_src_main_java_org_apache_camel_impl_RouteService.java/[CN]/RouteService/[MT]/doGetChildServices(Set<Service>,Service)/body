{
  services.add(service);
  if (service instanceof Navigate) {
    Navigate<?> nav=(Navigate<?>)service;
    if (nav.hasNext()) {
      List<?> children=nav.next();
      for (      Object child : children) {
        if (child instanceof Service) {
          doGetChildServices(services,(Service)child);
        }
      }
    }
  }
}
