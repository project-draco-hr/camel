{
  for (  Bundle b : bundleContext.getBundles()) {
    if (b.getSymbolicName().equals(symbolicName)) {
      return b;
    }
  }
  for (  Bundle b : bundleContext.getBundles()) {
    LOG.warn("Bundle: " + b.getSymbolicName());
  }
  throw new RuntimeException("Bundle " + symbolicName + " does not exist");
}
