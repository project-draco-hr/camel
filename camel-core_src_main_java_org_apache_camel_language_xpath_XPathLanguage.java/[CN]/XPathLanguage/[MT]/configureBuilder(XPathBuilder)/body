{
  if (resultType != null) {
    builder.setResultQName(resultType);
  }
  if (isUseSaxon()) {
    builder.enableSaxon();
  }
 else {
    if (xpathFactory != null) {
      builder.setXPathFactory(xpathFactory);
    }
    if (objectModelUri != null) {
      builder.setObjectModelUri(objectModelUri);
    }
  }
}
