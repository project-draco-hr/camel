{
  return new RouteBuilder(){
    public void configure() throws Exception {
      AggregationCollection ag=new PredicateAggregationCollection(header("id"),new UseLatestAggregationStrategy(),header(Exchange.AGGREGATED_SIZE).isEqualTo(3));
      from("direct:start").aggregate(ag).batchTimeout(500L).to("mock:result");
    }
  }
;
}
