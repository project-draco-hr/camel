{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      context.addComponent("async",new MyAsyncComponent());
      from("jetty:http://localhost:8876/myservice").convertBodyTo(String.class).choice().when(body().contains("Camel")).to("async:Bye World").end().to("mock:result");
    }
  }
;
}
