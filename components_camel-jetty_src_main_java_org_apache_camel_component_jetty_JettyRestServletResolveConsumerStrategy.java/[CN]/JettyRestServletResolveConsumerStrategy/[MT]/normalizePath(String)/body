{
  StringBuilder sb=new StringBuilder();
  String[] paths=path.split("/");
  for (  String s : paths) {
    if (ObjectHelper.isEmpty(s)) {
      continue;
    }
    sb.append("/");
    if (s.startsWith("{") && s.endsWith("}")) {
      sb.append("*");
    }
 else {
      sb.append(s);
    }
  }
  return sb.toString();
}
