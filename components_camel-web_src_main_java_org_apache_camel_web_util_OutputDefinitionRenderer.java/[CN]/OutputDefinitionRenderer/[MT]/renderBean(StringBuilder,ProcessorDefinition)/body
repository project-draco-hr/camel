{
  BeanDefinition beanDef=(BeanDefinition)processor;
  if (beanDef.getRef() != null) {
    buffer.append("Ref(\"").append(beanDef.getRef()).append("\"");
    if (beanDef.getMethod() != null) {
      buffer.append(", \"").append(beanDef.getMethod()).append("\"");
    }
    buffer.append(")");
  }
}
