{
  Processor processor=getOutput();
  if (processor instanceof AsyncProcessor) {
    return ((AsyncProcessor)processor).process(exchange,callback);
  }
 else   if (processor != null) {
    try {
      processor.process(exchange);
    }
 catch (    Exception e) {
      exchange.setException(e);
    }
  }
  callback.done(true);
  return true;
}
