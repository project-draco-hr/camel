{
  service.submit(new Callable<Object>(){
    public Object call() throws Exception {
      return "A";
    }
  }
);
  service.submit(new Callable<Object>(){
    public Object call() throws Exception {
      Thread.sleep(200);
      return "B";
    }
  }
);
  Object a=service.take().get();
  Object b=service.take().get();
  assertEquals("A",a);
  assertEquals("B",b);
}
