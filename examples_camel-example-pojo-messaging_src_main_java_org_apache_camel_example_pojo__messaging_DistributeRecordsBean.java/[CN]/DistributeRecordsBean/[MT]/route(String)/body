{
  if (city.equals("London")) {
    LOG.info("Person is from EMEA region");
    return new String[]{"file:target/messages/emea/hr_pickup","file:target/messages/emea/finance_pickup"};
  }
 else {
    LOG.info("Person is from AMER region");
    return new String[]{"file:target/messages/amer/hr_pickup","file:target/messages/amer/finance_pickup"};
  }
}
