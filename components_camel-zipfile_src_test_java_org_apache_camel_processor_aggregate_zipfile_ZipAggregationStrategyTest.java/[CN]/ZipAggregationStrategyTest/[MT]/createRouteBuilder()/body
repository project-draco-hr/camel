{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("file:src/test/resources/org/apache/camel/aggregate/zipfile/data?consumer.delay=1000&noop=true").aggregate(new ZipAggregationStrategy()).constant(true).completionFromBatchConsumer().eagerCheckCompletion().to("mock:aggregateToZipEntry").log("Done processing big file: ${header.CamelFileName}");
    }
  }
;
}
