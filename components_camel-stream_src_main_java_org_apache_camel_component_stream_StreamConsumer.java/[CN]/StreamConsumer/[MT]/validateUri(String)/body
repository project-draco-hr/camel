{
  String[] s=uri.split(":");
  if (s.length < 2) {
    throw new IllegalArgumentException(INVALID_URI);
  }
  String[] t=s[1].split("\\?");
  if (t.length < 1) {
    throw new IllegalArgumentException(INVALID_URI);
  }
  this.uri=t[0].trim();
  if (!TYPES_LIST.contains(this.uri)) {
    throw new IllegalArgumentException(INVALID_URI);
  }
}
