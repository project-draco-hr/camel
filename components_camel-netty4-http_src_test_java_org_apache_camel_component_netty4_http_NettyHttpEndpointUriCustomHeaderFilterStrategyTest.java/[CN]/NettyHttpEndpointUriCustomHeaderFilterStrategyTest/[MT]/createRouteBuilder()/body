{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:request").setHeader("Date",constant("31-03-2014")).to("netty4-http:http://localhost:{{port}}/myapp/mytest?headerFilterStrategy=#customHeaderFilterStrategy");
      from("netty4-http:http://localhost:{{port}}/myapp/mytest").to("mock:outbound").setHeader("sub-date",constant("31-05-2014"));
    }
  }
;
}
