{
  setReceiveTimeout(timeout);
  Message message;
  if (ObjectHelper.isNotEmpty(jmsEndpoint.getSelector())) {
    message=template.receiveSelected(jmsEndpoint.getSelector());
  }
 else {
    message=template.receive();
  }
  if (message != null) {
    return getEndpoint().createExchange(message);
  }
  return null;
}
