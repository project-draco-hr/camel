{
  LOG.info("Indexing current JIRA issues...");
  List<BasicIssue> issues=new ArrayList<BasicIssue>();
  while (true) {
    SearchResult searchResult=client.getSearchClient().searchJqlWithFullIssues(jql,maxPerQuery,start,null);
    for (    BasicIssue issue : searchResult.getIssues()) {
      issues.add(issue);
    }
    if (start >= searchResult.getTotal() || (maxResults > 0 && issues.size() >= maxResults)) {
      break;
    }
    start+=maxPerQuery;
  }
  return issues;
}
