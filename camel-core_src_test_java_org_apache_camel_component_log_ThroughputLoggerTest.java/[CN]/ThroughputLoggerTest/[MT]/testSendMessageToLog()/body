{
  MockEndpoint mock=getMockEndpoint("mock:result");
  mock.expectedMessageCount(4);
  template.sendBody("seda:in","Hello World");
  template.sendBody("seda:in","Hello World");
  template.sendBody("seda:in","Bye World");
  template.sendBody("seda:in","Bye World");
