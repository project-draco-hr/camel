{
  Object answer=null;
  if (dataFormat == DataFormat.POJO) {
    answer=out.getBody();
  }
 else   if (dataFormat == DataFormat.PAYLOAD) {
    answer=out.getBody(CxfPayload.class);
  }
 else   if (dataFormat == DataFormat.MESSAGE) {
    answer=out.getBody(InputStream.class);
  }
  return answer;
}
