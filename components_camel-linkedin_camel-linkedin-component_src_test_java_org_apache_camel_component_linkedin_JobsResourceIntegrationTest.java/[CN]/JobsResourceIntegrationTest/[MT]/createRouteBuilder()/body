{
  return new RouteBuilder(){
    public void configure(){
      from("direct://ADDJOB").to("linkedin://" + PATH_PREFIX + "/addJob?inBody=job");
      from("direct://EDITJOB").to("linkedin://" + PATH_PREFIX + "/editJob");
      from("direct://GETJOB").to("linkedin://" + PATH_PREFIX + "/getJob");
      from("direct://REMOVEJOB").to("linkedin://" + PATH_PREFIX + "/removeJob?inBody=partner_job_id");
    }
  }
;
}
