{
  localServer=ServerBootstrap.bootstrap().setHttpProcessor(getBasicHttpProcessor()).setConnectionReuseStrategy(getConnectionReuseStrategy()).setResponseFactory(getHttpResponseFactory()).setExpectationVerifier(getHttpExpectationVerifier()).setSslContext(getSSLContext()).registerHandler("/search",new BasicValidationHandler("GET",null,null,getExpectedContent())).registerHandler("/test%20/path",new BasicValidationHandler("GET",null,null,getExpectedContent())).create();
  localServer.start();
  super.setUp();
}
