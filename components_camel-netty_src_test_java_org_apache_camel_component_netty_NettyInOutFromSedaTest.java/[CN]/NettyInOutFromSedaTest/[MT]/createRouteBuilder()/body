{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("seda:start").log("before ${body}").to("netty:tcp://localhost:8555?textline=true&sync=true").log("after ${body}").to("mock:result");
      from("netty:tcp://localhost:8555?textline=true&sync=true").transform(body().prepend("Bye "));
    }
  }
;
}
