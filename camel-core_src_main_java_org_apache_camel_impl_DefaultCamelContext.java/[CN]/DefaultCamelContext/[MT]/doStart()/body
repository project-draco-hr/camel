{
  doWithDefinedClassLoader(new Callable<Void>(){
    @Override public Void call() throws Exception {
      try {
        doStartCamel();
        return null;
      }
 catch (      Exception e) {
        EventHelper.notifyCamelContextStartupFailed(DefaultCamelContext.this,e);
        throw e;
      }
    }
  }
);
}
