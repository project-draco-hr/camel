{
  if (!hasNext()) {
    throw new NoSuchElementException();
  }
  List<String> result=Arrays.asList(line);
  try {
    line=parser.getLine();
  }
 catch (  IOException e) {
    line=null;
    IOHelper.close(in);
    throw new IllegalStateException(e);
  }
  if (line == null) {
    IOHelper.close(in);
  }
  return result;
}
