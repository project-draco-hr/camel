{
  if (name.equals("imap") || name.equals("imaps") || name.equals("pop3")|| name.equals("pop3s")|| name.equals("smtp")|| name.equals("smtps")) {
    return "mail";
  }
 else   if (name.equals("ftp") || name.equals("sftp") || name.equals("ftps")) {
    return "ftp";
  }
 else {
    return name;
  }
}
