{
  OutputReportIncident ok=new OutputReportIncident();
  ok.setCode("OK");
  OutputReportIncident accepted=new OutputReportIncident();
  accepted.setCode("Accepted");
  from("cxf:bean:reportIncident").convertBodyTo(InputReportIncident.class).setHeader(Exchange.FILE_NAME,constant("request-${date:now:yyyy-MM-dd-HHmmssSSS}")).wireTap("file://target/inbox/").choice().when(simple("${body.givenName} == 'Claus'")).transform(constant(ok)).otherwise().transform(constant(accepted));
}
