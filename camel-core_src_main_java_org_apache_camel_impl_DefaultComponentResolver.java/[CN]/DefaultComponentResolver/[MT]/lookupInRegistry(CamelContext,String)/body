{
  Object bean=null;
  for (  String name : names) {
    try {
      bean=context.getRegistry().lookupByName(name);
      getLog().debug("Lookup component with name {} in registry. Found: {}",name,bean);
    }
 catch (    Exception e) {
      getLog().debug("Ignored error looking up bean: " + name,e);
    }
    if (bean != null) {
      return bean;
    }
  }
  return null;
}
