{
  return new RouteBuilder(){
    public void configure(){
      from(String.format("mina2:tcp://localhost:%1$s?sync=false&textline=true",getPort())).to("mock:results");
      from("file:src/test/data?noop=true&fileName=message1.txt").to(String.format("mina2:tcp://localhost:%1$s?sync=false&textline=true",getPort()));
    }
  }
;
}
