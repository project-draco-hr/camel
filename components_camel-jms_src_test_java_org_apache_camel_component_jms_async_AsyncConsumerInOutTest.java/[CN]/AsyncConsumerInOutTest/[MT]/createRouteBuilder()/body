{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("activemq:queue:start?asyncConsumer=true").choice().when(body().contains("Camel")).to("async:camel?delay=2000").inOut("activemq:queue:camel").to("mock:result").otherwise().to("log:other").to("mock:result");
      from("activemq:queue:camel").to("log:camel").transform(constant("Bye Camel"));
    }
  }
;
}
