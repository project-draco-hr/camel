{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:start").multicast(new MyAggregationStrategy()).parallelProcessing().timeout(2000).to("direct:a","direct:b","direct:c").end().to("mock:result");
      from("direct:a").delay(3000).setBody(constant("A"));
      from("direct:b").setBody(constant("B"));
      from("direct:c").delay(500).setBody(constant("C"));
    }
  }
;
}
