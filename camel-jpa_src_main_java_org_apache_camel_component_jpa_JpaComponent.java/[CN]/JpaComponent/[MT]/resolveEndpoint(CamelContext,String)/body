{
  if (!uri.startsWith("jpa:")) {
    return null;
  }
  URI u=new URI(uri);
  String path=u.getSchemeSpecificPart();
  JpaEndpoint endpoint=new JpaEndpoint(uri,this);
  if (path != null) {
    Class<?> type=ObjectHelper.loadClass(path);
    if (type != null) {
      endpoint.setEntityType(type);
    }
  }
  return endpoint;
}
