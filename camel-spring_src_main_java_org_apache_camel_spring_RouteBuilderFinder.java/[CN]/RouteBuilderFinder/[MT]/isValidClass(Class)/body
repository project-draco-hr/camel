{
  if (!Modifier.isAbstract(type.getModifiers()) && !type.isInterface()) {
    Constructor[] constructors=type.getDeclaredConstructors();
    for (    Constructor constructor : constructors) {
      Class[] classes=constructor.getParameterTypes();
      if (classes.length == 0) {
        return true;
      }
    }
  }
  return false;
}
