{
  List<Object> arguments=ARGUMENTS_MAP.get(name);
  if (arguments == null) {
    if (ALIASES.containsKey(name)) {
      arguments=ARGUMENTS_MAP.get(ALIASES.get(name));
    }
  }
  if (arguments == null) {
    throw new IllegalArgumentException(name);
  }
  return Collections.unmodifiableList(arguments);
}
