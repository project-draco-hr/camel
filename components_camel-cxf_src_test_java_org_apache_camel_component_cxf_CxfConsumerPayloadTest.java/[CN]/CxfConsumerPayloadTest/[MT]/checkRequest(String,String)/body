{
  if (expect.equals("ECHO_REQUEST")) {
    assertTrue("Get a wrong request",request.startsWith(ECHO_REQUEST.substring(0,60)) && request.endsWith(ECHO_REQUEST.substring(61)));
  }
 else {
    assertTrue("Get a wrong request",request.startsWith(ECHO_BOOLEAN_REQUEST.substring(0,67)) && request.endsWith(ECHO_BOOLEAN_REQUEST.substring(68)));
  }
}
