{
  Object destination=getReplyToDestination();
  if (destination == null) {
    try {
      destination=message.getJMSReplyTo();
    }
 catch (    JMSException e) {
      if (LOG.isDebugEnabled()) {
        LOG.debug("Cannot read JMSReplyTo header. Will ignore this exception.",e);
      }
    }
  }
  return destination;
}
