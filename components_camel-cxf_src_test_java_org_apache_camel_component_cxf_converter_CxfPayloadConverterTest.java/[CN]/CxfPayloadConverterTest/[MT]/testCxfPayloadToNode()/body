{
  exchange.getIn().setBody(payload);
  Node node=exchange.getIn().getBody(Node.class);
  assertNotNull(node);
  exchange.getIn().setBody(emptyPayload);
  node=exchange.getIn().getBody(Node.class);
  assertNull(node);
  exchange.getIn().setBody(payload);
  node=exchange.getIn().getBody(Node.class);
  assertNotNull(node);
}
