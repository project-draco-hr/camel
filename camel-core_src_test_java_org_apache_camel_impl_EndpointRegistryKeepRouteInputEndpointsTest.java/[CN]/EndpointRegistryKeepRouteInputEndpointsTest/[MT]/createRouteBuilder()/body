{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("seda:start?multipleConsumers=true").routeId("foo").to("log:foo").to("mock:result");
      from("seda:start?multipleConsumers=true").routeId("bar").to("log:bar").to("log:bar").to("mock:result");
      from("seda:stop").routeId("baz").to("mock:stop");
    }
  }
;
}
