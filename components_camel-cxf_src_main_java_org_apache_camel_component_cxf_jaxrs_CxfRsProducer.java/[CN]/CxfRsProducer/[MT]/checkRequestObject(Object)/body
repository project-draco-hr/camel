{
  if (request != null) {
    if (request instanceof MessageContentsList) {
      request=((MessageContentsList)request).get(0);
    }
 else     if (request instanceof List) {
      request=((List)request).get(0);
    }
 else     if (request.getClass().isArray()) {
      request=((Object[])request)[0];
    }
  }
  return request;
}
