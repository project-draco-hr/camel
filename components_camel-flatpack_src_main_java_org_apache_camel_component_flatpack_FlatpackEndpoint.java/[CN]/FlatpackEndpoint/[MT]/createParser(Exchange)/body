{
  Reader bodyReader=exchange.getIn().getMandatoryBody(Reader.class);
  try {
    if (FlatpackType.fixed == type) {
      return createFixedParser(resourceUri,bodyReader);
    }
 else {
      return createDelimitedParser(exchange);
    }
  }
 catch (  Exception e) {
    IOHelper.close(bodyReader);
    throw e;
  }
}
