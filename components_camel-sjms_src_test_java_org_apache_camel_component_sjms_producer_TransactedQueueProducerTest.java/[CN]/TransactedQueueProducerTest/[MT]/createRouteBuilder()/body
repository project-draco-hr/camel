{
  return new RouteBuilder(){
    public void configure(){
      from("direct:start.transacted").to("sjms:queue:" + TEST_DESTINATION_NAME + "?transacted=true");
      from("direct:start.transacted.async.override").to("sjms:queue:" + TEST_DESTINATION_NAME + "?transacted=true&synchronous=false");
      from("direct:finish").to("log:test.log.1?showBody=true","mock:result");
    }
  }
;
}
