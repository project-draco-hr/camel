{
  if (log.isTraceEnabled()) {
    log.trace("Processing " + endpoint.getConfiguration());
  }
  try {
    connectIfNecessary();
    if (!loggedIn) {
      String message="Could not connect/login to " + endpoint.getConfiguration();
      log.warn(message);
      throw new FtpOperationFailedException(client.getReplyCode(),client.getReplyString(),message);
    }
    process((RemoteFileExchange)endpoint.createExchange(exchange));
  }
 catch (  Exception e) {
    loggedIn=false;
    if (isStopping() || isStopped()) {
      log.warn("Producer is stopping. Ignoring caught exception: " + e.getClass().getCanonicalName() + " message: "+ e.getMessage());
    }
 else {
      log.warn("Exception occured during processing: " + e.getClass().getCanonicalName() + " message: "+ e.getMessage());
      disconnect();
      throw e;
    }
  }
}
