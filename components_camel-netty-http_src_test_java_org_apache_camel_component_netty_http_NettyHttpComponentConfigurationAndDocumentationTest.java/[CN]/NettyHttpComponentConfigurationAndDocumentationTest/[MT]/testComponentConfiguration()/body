{
  NettyHttpComponent comp=context.getComponent("netty-http",NettyHttpComponent.class);
  EndpointConfiguration conf=comp.createConfiguration("netty-http:tcp://localhost:5150?sync=true" + "&httpMethodRestrict=POST&traceEnabled=true");
  assertEquals("true",conf.getParameter("traceEnabled"));
  assertEquals("POST",conf.getParameter("httpMethodRestrict"));
  ComponentConfiguration compConf=comp.createComponentConfiguration();
  String json=compConf.createParameterJsonSchema();
  assertNotNull(json);
  assertTrue(json.contains("\"httpMethodRestrict\": { \"type\": \"string\" }"));
  assertTrue(json.contains("\"traceEnabled\": { \"type\": \"boolean\" }"));
}
