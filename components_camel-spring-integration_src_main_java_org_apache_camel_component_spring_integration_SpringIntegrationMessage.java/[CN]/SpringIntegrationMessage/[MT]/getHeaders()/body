{
  if (siMessage != null) {
    Map<String,Object> answer=new HashMap<String,Object>();
    MessageHeader header=siMessage.getHeader();
    for (    String name : header.getAttributeNames()) {
      answer.put(name,header.getAttribute(name));
    }
    return answer;
  }
 else {
    return super.getHeaders();
  }
}
