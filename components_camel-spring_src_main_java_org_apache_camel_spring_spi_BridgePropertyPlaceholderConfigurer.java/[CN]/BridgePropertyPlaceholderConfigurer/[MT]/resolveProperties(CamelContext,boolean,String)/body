{
  Properties answer=new Properties();
  for (  String u : uri) {
    String ref="ref:" + id;
    if (ref.equals(u)) {
      answer.putAll(properties);
    }
 else     if (resolver != null) {
      Properties p=resolver.resolveProperties(context,ignoreMissingLocation,u);
      if (p != null) {
        answer.putAll(p);
      }
    }
  }
  return answer;
}
