{
  assertThat(component.getMetricsName("meter:metric-a"),is("metric-a"));
  assertThat(component.getMetricsName("meter:metric-a:sub-b"),is("metric-a:sub-b"));
  assertThat(component.getMetricsName("metric-a"),is("metric-a"));
  assertThat(component.getMetricsName("//metric-a"),is("//metric-a"));
  assertThat(component.getMetricsName("meter://metric-a"),is("//metric-a"));
}
