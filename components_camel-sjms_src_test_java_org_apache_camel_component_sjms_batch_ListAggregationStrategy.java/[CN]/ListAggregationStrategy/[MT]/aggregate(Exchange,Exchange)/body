{
  String body=newExchange.getIn().getBody(String.class);
  if (oldExchange == null) {
    List<String> list=new ArrayList<String>();
    list.add(body);
    newExchange.getIn().setBody(list);
    return newExchange;
  }
 else {
    List<String> list=oldExchange.getIn().getBody(List.class);
    list.add(body);
    return oldExchange;
  }
}
