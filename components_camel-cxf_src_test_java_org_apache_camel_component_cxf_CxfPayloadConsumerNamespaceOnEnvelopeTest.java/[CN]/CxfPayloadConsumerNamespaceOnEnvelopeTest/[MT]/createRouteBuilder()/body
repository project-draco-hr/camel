{
  return new RouteBuilder(){
    public void configure(){
      from("direct:router").to("cxf:bean:serviceEndpoint?dataFormat=MESSAGE").convertBodyTo(String.class);
      from("cxf:bean:serviceEndpoint?DataFormat=PAYLOAD").convertBodyTo(String.class).convertBodyTo(Document.class).setBody().constant(responsePayload);
    }
  }
;
}
