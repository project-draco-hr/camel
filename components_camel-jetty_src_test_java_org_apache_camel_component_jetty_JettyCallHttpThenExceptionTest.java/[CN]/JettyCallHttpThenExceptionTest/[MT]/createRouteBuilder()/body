{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("jetty://http://localhost:8234/myserver").to("log:A").removeHeaders("CamelHttp*").to("http://localhost:8234/other").removeHeaders("CamelHttp*").to("mock:bar").throwException(new IllegalArgumentException("I cannot do this"));
      from("jetty://http://localhost:8234/other").convertBodyTo(String.class).to("log:C").to("mock:foo").transform().simple("Bye ${body}");
    }
  }
;
}
