{
  for (  XMLObject xo : objects) {
    @SuppressWarnings("unchecked") List<XMLStructure> content=xo.getContent();
    for (    XMLStructure xs : content) {
      if (xs instanceof Manifest) {
        Manifest man=(Manifest)xs;
        if (id.equals(man.getId())) {
          return man;
        }
      }
    }
  }
  return null;
}
