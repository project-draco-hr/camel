{
  if (isCorrelationKeyIsPrimary()) {
    return template.find(getEntityType(),key);
  }
 else {
    Map<String,Object> params=new HashMap<String,Object>(1);
    params.put("key",key);
    List<T> list=template.findByNamedParams(getFindByKeyQuery(),params);
    if (list.isEmpty()) {
      return null;
    }
 else {
      return list.get(0);
    }
  }
}
