{
  List<String> list=Hazelcast.getList("bar");
  list.clear();
  list.add("foo1");
  list.add("foo2");
  list.add("foo3");
  assertEquals(3,list.size());
  template.sendBody("direct:removevalue","foo2");
  assertEquals(2,list.size());
  assertTrue(list.contains("foo1") && list.contains("foo3"));
  list.clear();
}
