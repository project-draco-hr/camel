{
  return new RouteBuilder(){
    public void configure(){
      from("jetty:http://localhost:8192/test").setBody(body(String.class)).to("log:in").doTry().to("validator:OptimizationRequest.xsd").setBody(constant("<ok/>")).doCatch(ValidationException.class).setBody(constant("<error/>")).end().to("log:out");
    }
  }
;
}
