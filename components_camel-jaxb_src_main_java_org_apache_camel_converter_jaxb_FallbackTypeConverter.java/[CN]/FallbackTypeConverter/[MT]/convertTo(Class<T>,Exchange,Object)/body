{
  try {
    if (isJaxbType(type)) {
      return unmarshall(type,exchange,value);
    }
    if (value != null) {
      if (isJaxbType(value.getClass()) && isNotStreamCacheType(type)) {
        return marshall(type,exchange,value);
      }
    }
    return null;
  }
 catch (  Exception e) {
    throw new RuntimeCamelException(e);
  }
}
