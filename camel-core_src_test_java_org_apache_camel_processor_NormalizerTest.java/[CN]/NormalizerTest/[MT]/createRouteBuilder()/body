{
  return new RouteBuilder(){
    public void configure(){
      from("direct:start").choice().when().xpath("/employee").to("bean:normalizer?method=employeeToPerson").to("seda:queue").when().xpath("/customer").to("bean:normalizer?method=customerToPerson").to("seda:queue");
      from("seda:queue").filter().xpath("/person[@name='Jon']").to("mock:result");
    }
  }
;
}
