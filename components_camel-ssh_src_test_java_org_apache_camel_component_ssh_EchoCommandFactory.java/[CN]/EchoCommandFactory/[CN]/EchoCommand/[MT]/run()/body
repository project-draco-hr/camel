{
  boolean succeeded=true;
  String message=null;
  try {
    out.write(command.getBytes());
    out.flush();
  }
 catch (  Exception e) {
    succeeded=false;
    message=e.toString();
  }
 finally {
    if (succeeded) {
      callback.onExit(0);
    }
 else {
      callback.onExit(1,message);
    }
  }
}
