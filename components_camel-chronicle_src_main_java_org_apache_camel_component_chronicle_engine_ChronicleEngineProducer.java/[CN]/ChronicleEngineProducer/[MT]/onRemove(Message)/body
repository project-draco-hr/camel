{
  Object oldValue=message.getHeader(ChronicleEngineConstants.OLD_VALUE);
  if (oldValue != null) {
    message.setHeader(ChronicleEngineConstants.RESULT,mapView.get().remove(mandatoryKey(message),oldValue));
  }
 else {
    message.setHeader(ChronicleEngineConstants.OLD_VALUE,mapView.get().remove(mandatoryKey(message)));
  }
}
