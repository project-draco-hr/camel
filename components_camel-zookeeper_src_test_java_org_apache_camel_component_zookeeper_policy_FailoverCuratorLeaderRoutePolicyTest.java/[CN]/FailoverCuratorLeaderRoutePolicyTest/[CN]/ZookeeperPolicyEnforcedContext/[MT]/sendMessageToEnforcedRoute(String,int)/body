{
  mock.expectedMessageCount(expected);
  try {
    template.sendBody("vm:" + routename,ExchangePattern.InOut,message);
  }
 catch (  Exception e) {
    if (expected > 0) {
      LOG.error(e.getMessage(),e);
      fail("Expected messages...");
    }
  }
  mock.await(2,TimeUnit.SECONDS);
  mock.assertIsSatisfied(2000);
}
