{
  return new RouteBuilder(){
    public void configure(){
      errorHandler(deadLetterChannel().maximumRedeliveries(1).delay(0));
      from("direct:a").doTry().process(validator).process(toProcessor).doCatch(ValidationException.class).process(orderProcessor).doFinally().process(orderProcessor2).end().process(orderProcessor3);
    }
  }
;
}
