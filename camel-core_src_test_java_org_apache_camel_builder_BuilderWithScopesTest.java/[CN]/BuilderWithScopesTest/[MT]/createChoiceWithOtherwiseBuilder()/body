{
  return new RouteBuilder(){
    public void configure(){
      errorHandler(deadLetterChannel("mock:error").redeliveryDelay(0).maximumRedeliveries(3));
      from("direct:a").choice().when(header("foo").isEqualTo("bar")).process(orderProcessor).when(header("foo").isEqualTo("cheese")).process(orderProcessor2).otherwise().process(orderProcessor3).end().process(toProcessor);
    }
  }
;
}
