{
  return new RouteBuilder(){
    public void configure(){
      errorHandler(deadLetterChannel("mock:error").delay(0).maximumRedeliveries(1));
      from("direct:a").tryBlock().process(validator).process(toProcessor).handle(ValidationException.class).process(orderProcessor).finallyBlock().process(orderProcessor2).process(orderProcessor3);
    }
  }
;
}
