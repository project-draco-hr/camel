{
  LOG.debug("Writing slow file...");
  FileOutputStream fos=new FileOutputStream("target/changed/in/slowfile.dat");
  for (int i=0; i < 20; i++) {
    fos.write(("Line " + i + "\n").getBytes());
    LOG.debug("Writing line " + i);
    Thread.sleep(200);
  }
  fos.flush();
  fos.close();
  LOG.debug("Writing slow file DONE...");
}
