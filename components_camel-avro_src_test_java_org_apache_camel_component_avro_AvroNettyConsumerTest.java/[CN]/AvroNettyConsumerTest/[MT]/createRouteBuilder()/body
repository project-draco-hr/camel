{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("avro:netty:localhost:" + avroPort).choice().when().el("${in.headers." + AvroConstants.AVRO_MESSAGE_NAME + " == 'put'}").process(new PutProcessor(keyValue)).when().el("${in.headers." + AvroConstants.AVRO_MESSAGE_NAME + " == 'get'}").process(new GetProcessor(keyValue));
    }
  }
;
}
