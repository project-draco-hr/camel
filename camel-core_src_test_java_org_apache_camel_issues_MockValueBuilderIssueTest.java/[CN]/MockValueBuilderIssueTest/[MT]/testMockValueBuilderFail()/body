{
  MockEndpoint mock=getMockEndpoint("mock:result");
  mock.expectedMessageCount(1);
  mock.message(0).exchangeProperty("foo").convertTo(String.class).contains("4");
  template.sendBody("direct:start","Hello World");
  try {
    assertMockEndpointsSatisfied();
    fail("Should fail");
  }
 catch (  Throwable e) {
    assertEquals("Assertion error at index 0 on mock mock://result with predicate: exchangeProperty(foo) contains 4" + " evaluated as: 123 contains 4 on Exchange[Message: Hello World]",e.getMessage());
  }
}
