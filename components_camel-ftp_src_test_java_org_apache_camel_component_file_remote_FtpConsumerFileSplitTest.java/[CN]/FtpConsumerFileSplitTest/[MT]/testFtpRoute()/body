{
  MockEndpoint resultEndpoint=getMockEndpoint("mock:result");
  resultEndpoint.expectedMessageCount(3);
  resultEndpoint.expectedBodiesReceived("line1","line2");
  resultEndpoint.assertIsSatisfied();
