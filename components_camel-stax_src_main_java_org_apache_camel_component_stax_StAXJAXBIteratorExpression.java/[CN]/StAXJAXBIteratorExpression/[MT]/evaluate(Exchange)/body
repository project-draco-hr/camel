{
  try {
    XMLEventReader reader=exchange.getIn().getMandatoryBody(XMLEventReader.class);
    return createIterator(reader,handled);
  }
 catch (  InvalidPayloadException e) {
    exchange.setException(e);
    return null;
  }
catch (  JAXBException e) {
    exchange.setException(e);
    return null;
  }
}
