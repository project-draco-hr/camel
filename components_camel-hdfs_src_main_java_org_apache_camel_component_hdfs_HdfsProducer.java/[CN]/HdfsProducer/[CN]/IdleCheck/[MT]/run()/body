{
  if (ostream == null) {
    return;
  }
  HdfsProducer.this.log.trace("IdleCheck running");
  if (System.currentTimeMillis() - ostream.getLastAccess() > strategy.value && !idle.get() && !ostream.isBusy().get()) {
    idle.set(true);
    try {
      ostream.close();
    }
 catch (    IOException e) {
    }
  }
}
