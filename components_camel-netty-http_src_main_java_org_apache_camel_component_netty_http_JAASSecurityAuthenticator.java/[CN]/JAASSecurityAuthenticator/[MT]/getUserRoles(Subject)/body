{
  StringBuilder sb=new StringBuilder();
  for (  Principal p : subject.getPrincipals()) {
    if (roleClassNames == null) {
      if (p.getName().toLowerCase(Locale.US).contains("role")) {
        if (sb.length() > 0) {
          sb.append(",");
        }
        sb.append(p.getName());
      }
    }
  }
  if (sb.length() > 0) {
    return sb.toString();
  }
 else {
    return null;
  }
}
