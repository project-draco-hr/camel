{
  return new RouteBuilder(){
    @Override public void configure(){
      from("direct:start").to("metrics:A").to("metrics:counter://B").to("metrics:counter:C?increment=19291").to("metrics:counter:C?decrement=19292").to("metrics:counter:C").to("metrics:meter:D").to("metrics:meter:D?mark=90001").to("mock:result");
    }
  }
;
}
