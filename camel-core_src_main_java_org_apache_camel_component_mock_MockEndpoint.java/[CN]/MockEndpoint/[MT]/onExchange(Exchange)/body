{
  try {
    if (reporter != null) {
      reporter.process(exchange);
    }
    Exchange copy=ExchangeHelper.createCopy(exchange,true);
    performAssertions(exchange,copy);
  }
 catch (  Throwable e) {
    failures.add(e);
  }
 finally {
    if (latch != null) {
      latch.countDown();
    }
  }
}
