{
  try {
    log.debug(getEndpointUri() + " >>>> " + exchange);
    exchangesReceived.add(exchange);
    Processor<Exchange> processor=processors.get(getReceivedCounter());
    if (processor != null) {
      processor.process(exchange);
    }
    if (latch != null) {
      latch.countDown();
    }
  }
 catch (  Exception e) {
    failures.add(e);
  }
}
