{
  int count=0;
  for (  MockEndpoint endpoint : endpoints) {
    if (endpoint.getExpectedCount() != 0) {
      endpoint.assertIsSatisfied();
      count++;
    }
  }
  for (  MockEndpoint endpoint : endpoints) {
    if (endpoint.getExpectedCount() == 0) {
      if (count == 0) {
        endpoint.assertIsSatisfied();
        count++;
      }
 else {
        endpoint.assertIsSatisfied(0);
      }
    }
  }
}
