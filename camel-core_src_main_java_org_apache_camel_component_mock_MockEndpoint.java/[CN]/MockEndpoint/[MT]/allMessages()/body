{
  AssertionClause clause=new AssertionClause(){
    public void run(){
      List<Exchange> list=getExchangesReceived();
      int index=0;
      for (      Exchange exchange : list) {
        applyAssertionOn(MockEndpoint.this,index++,exchange);
      }
    }
  }
;
  expects(clause);
  return clause;
}
