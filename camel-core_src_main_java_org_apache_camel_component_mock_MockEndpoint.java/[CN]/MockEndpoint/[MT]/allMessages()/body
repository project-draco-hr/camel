{
  final AssertionClause clause=new AssertionClause(this){
    public void run(){
      List<Exchange> list=getReceivedExchanges();
      int index=0;
      for (      Exchange exchange : list) {
        applyAssertionOn(MockEndpoint.this,index++,exchange);
      }
    }
  }
;
  expects(clause);
  return clause;
}
