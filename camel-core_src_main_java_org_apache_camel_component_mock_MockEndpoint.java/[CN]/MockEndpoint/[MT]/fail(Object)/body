{
  if (LOG.isDebugEnabled()) {
    List<Exchange> list=getReceivedExchanges();
    int index=0;
    for (    Exchange exchange : list) {
      LOG.debug(getEndpointUri() + " failed and received[" + (++index)+ "]: "+ exchange);
    }
  }
  throw new AssertionError(getEndpointUri() + " " + message);
}
