{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      String uri="jcr://user:pass@repository";
      uri+=absPath;
      uri+="?eventTypes=" + eventTypes;
      uri+="&deep=" + deep;
      uri+="&noLocal=" + noLocal;
      from(uri).to("direct:a");
    }
  }
;
}
