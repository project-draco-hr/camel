{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("direct:createAndRun").to("aws-ec2://test?amazonEc2Client=#amazonEc2Client&operation=createAndRunInstances").to("mock:result");
      from("direct:start").to("aws-ec2://test?amazonEc2Client=#amazonEc2Client&operation=startInstances").to("mock:result");
      from("direct:stop").to("aws-ec2://test?amazonEc2Client=#amazonEc2Client&operation=stopInstances").to("mock:result");
      from("direct:terminate").to("aws-ec2://test?amazonEc2Client=#amazonEc2Client&operation=terminateInstances").to("mock:result");
    }
  }
;
}
