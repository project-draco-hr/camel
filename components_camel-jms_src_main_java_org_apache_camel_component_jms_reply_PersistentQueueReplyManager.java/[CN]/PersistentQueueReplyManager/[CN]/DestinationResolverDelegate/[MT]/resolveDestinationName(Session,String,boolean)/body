{
synchronized (PersistentQueueReplyManager.this) {
    try {
      if (destination == null) {
        destination=delegate.resolveDestinationName(session,destinationName,pubSubDomain);
        setReplyTo(destination);
      }
    }
  finally {
      PersistentQueueReplyManager.this.notifyAll();
    }
  }
  return destination;
}
