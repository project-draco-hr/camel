{
  PersistentQueueReplyHandler handler=new PersistentQueueReplyHandler(replyManager,exchange,callback,originalCorrelationId,requestTimeout,dynamicMessageSelector);
  correlation.put(correlationId,handler,requestTimeout);
  if (dynamicMessageSelector != null) {
    dynamicMessageSelector.addCorrelationID(correlationId);
  }
  return correlationId;
}
