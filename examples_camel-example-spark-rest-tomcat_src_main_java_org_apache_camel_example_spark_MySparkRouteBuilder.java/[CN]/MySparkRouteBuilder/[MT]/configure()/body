{
  restConfiguration().component("spark-rest");
  rest().uri("hello/:me").get().consumes("text/plain").to("log:input").transform().simple("Hello ${header.me}").endRest().get().consumes("application/json").to("log:input").transform().simple("{ \"message\": \"Hello ${header.me}\" }").endRest().get().consumes("text/xml").to("log:input").transform().simple("<message>Hello ${header.me}</message>");
}
