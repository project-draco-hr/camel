{
  rest().component("spark-rest").path("hello/:me").get().consumes("text/plain").to("log:input").transform().simple("Hello ${header.me}").endPath().path("hello/:me").get().consumes("application/json").to("log:input").transform().simple("{ \"message\": \"Hello ${header.me}\" }").endPath().path("hello/:me").get().consumes("text/xml").to("log:input").transform().simple("<message>Hello ${header.me}</message>");
}
