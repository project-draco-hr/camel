{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      DataFormat format=new BindyCsvDataFormat(Employee.class);
      from("direct:unmarshal").unmarshal(format).split(simple("body")).to("mock:bindy-unmarshal");
      from("direct:marshal").marshal(format).to("mock:bindy-marshal");
    }
  }
;
}
