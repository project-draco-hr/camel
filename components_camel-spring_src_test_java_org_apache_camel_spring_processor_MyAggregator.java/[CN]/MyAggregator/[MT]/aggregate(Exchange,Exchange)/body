{
  String body=oldExchange.getIn().getBody(String.class);
  if (body != null) {
    Message newIn=newExchange.getIn();
    String newBody=newIn.getBody(String.class);
    if (newBody != null) {
      body+=" " + newBody;
    }
    LOG.debug("**** invoked my strategy with result: " + body);
    newIn.setBody(body);
  }
  return newExchange;
}
