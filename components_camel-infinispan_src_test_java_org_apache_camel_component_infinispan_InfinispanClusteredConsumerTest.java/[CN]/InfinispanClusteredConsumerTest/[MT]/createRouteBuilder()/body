{
  return new RouteBuilder(){
    @Override public void configure(){
      from("infinispan://localhost?cacheContainer=#cacheContainer&clustered=true&eventTypes=CACHE_ENTRY_CREATED").to("mock:resultCreated");
      from("infinispan://localhost?cacheContainer=#cacheContainer&clustered=true&eventTypes=CACHE_ENTRY_EXPIRED").to("mock:resultExpired");
    }
  }
;
}
