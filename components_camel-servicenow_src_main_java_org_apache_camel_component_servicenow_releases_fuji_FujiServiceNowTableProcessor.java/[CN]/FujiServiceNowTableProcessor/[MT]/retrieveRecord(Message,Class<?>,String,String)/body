{
  return ObjectHelper.isEmpty(sysId) ? client.reset().types(MediaType.APPLICATION_JSON_TYPE).path("now").path("table").path(tableName).query(ServiceNowParams.SYSPARM_QUERY,in).query(ServiceNowParams.SYSPARM_DISPLAY_VALUE,in).query(ServiceNowParams.SYSPARM_EXCLUDE_REFERENCE_LINK,in).query(ServiceNowParams.SYSPARM_FIELDS,in).query(ServiceNowParams.SYSPARM_LIMIT,in).query(ServiceNowParams.SYSPARM_VIEW,in).invoke(HttpMethod.GET) : client.reset().types(MediaType.APPLICATION_JSON_TYPE).path("now").path("table").path(tableName).path(sysId).query(ServiceNowParams.SYSPARM_DISPLAY_VALUE,in).query(ServiceNowParams.SYSPARM_EXCLUDE_REFERENCE_LINK,in).query(ServiceNowParams.SYSPARM_FIELDS,in).query(ServiceNowParams.SYSPARM_VIEW,in).invoke(HttpMethod.GET);
}
