{
  Message in=exchange.getIn();
  List<Object> list=CastUtils.cast(expression.evaluate(exchange,List.class));
  Collections.sort(list,comparator);
  if (exchange.getPattern().isOutCapable()) {
    Message out=exchange.getOut();
    out.copyFrom(in);
    out.setBody(list);
  }
 else {
    in.setBody(list);
  }
}
