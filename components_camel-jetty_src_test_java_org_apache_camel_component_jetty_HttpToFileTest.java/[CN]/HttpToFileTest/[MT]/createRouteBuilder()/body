{
  return new RouteBuilder(){
    public void configure() throws Exception {
      from("jetty:http://localhost:8080/myworld").to("seda:in").setBody(constant("We got the file"));
      from("seda:in").setHeader(FileComponent.HEADER_FILE_NAME,constant("hello.txt")).convertBodyTo(String.class).to("file://target/myworld/").to("mock:result");
    }
  }
;
}
