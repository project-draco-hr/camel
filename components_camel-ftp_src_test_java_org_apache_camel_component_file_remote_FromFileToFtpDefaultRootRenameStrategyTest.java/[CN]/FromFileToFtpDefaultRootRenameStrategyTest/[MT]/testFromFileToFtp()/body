{
  File expectedOnFtpServer=new File(FTP_ROOT_DIR + "logo.jpeg");
  assertTrue(expectedOnFtpServer.exists());
  MockEndpoint mock=getMockEndpoint("mock:result");
  mock.expectedMessageCount(1);
  Thread.sleep(500);
  assertMockEndpointsSatisfied();
  Thread.sleep(500);
  assertTrue(!expectedOnFtpServer.exists());
}
