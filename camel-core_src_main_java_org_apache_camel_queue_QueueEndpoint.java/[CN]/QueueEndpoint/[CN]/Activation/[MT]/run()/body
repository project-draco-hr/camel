{
  while (!stop.get()) {
    E exchange=null;
    try {
      exchange=queue.poll(100,TimeUnit.MILLISECONDS);
    }
 catch (    InterruptedException e) {
      break;
    }
    if (exchange != null) {
      try {
        getInboundProcessor().onExchange(exchange);
      }
 catch (      Throwable e) {
        e.printStackTrace();
      }
    }
  }
}
