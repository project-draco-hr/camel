{
  if (value instanceof Closeable) {
    IOHelper.close((Closeable)value,value.getClass().getName(),LOG);
  }
 else   if (value instanceof Scanner) {
    Scanner scanner=(Scanner)value;
    scanner.close();
    IOException ioException=scanner.ioException();
    if (ioException != null) {
      throw ioException;
    }
  }
}
