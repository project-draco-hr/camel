{
  Boolean answer=null;
  if (hasProperties()) {
    answer=getProperty(Exchange.TRANSACTED_REDELIVERED,null,Boolean.class);
  }
  if (answer == null) {
    DefaultMessage msg=getIn(DefaultMessage.class);
    if (msg != null) {
      answer=msg.isTransactedRedelivered();
      setProperty(Exchange.TRANSACTED_REDELIVERED,answer);
    }
  }
  return answer;
}
