{
  ExchangeProperty<?> property=ExchangeProperty.getByName(name);
  if (property != null) {
    Class type=value.getClass();
    validateExchangePropertyIsExpectedType(property,type,value);
  }
  if (value != null) {
    getProperties().put(name,value);
  }
 else {
    if (name != null) {
      getProperties().remove(name);
    }
  }
}
