{
  assertExpression("${in.header.foo} > 'aaa'",true);
  assertExpression("${in.header.foo} > aaa",true);
  assertExpression("${in.header.foo} > 'def'",false);
  assertExpression("${in.header.foo} > def",false);
  assertExpression("${in.header.bar} > '100'",true);
  assertExpression("${in.header.bar} > 100",true);
  assertExpression("${in.header.bar} > '123'",false);
  assertExpression("${in.header.bar} > 123",false);
  assertExpression("${in.header.bar} > '200'",false);
}
