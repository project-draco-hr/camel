    }
  }
,bundle(newBundle().add("OSGI-INF/blueprint/test.xml",BlobStoreBlueprintRouteTest.class.getResource("blueprintBlobStoreService.xml")).set(Constants.BUNDLE_SYMBOLICNAME,"org.apache.camel.jclouds.blobstore.service").set(Constants.BUNDLE_VERSION,"1.0.0").set(Constants.DYNAMICIMPORT_PACKAGE,"*").build()).start(),scanFeatures(getCamelKarafFeatureUrl(),"camel-blueprint","camel-jclouds"),workingDirectory("target/paxrunner/"),felix(),equinox());
