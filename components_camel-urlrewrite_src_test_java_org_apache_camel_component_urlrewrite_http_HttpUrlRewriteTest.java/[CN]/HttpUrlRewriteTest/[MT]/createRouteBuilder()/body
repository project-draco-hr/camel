{
  return new RouteBuilder(){
    @Override public void configure() throws Exception {
      from("jetty:http://localhost:{{port}}/myapp?matchOnUriPrefix=true").to("http://localhost:{{port2}}/myapp2?bridgeEndpoint=true&throwExceptionOnFailure=false&urlRewrite=#myRewrite");
      from("jetty:http://localhost:{{port2}}/myapp2?matchOnUriPrefix=true").transform().simple("${header.CamelHttpUrl}?${header.CamelHttpQuery}");
    }
  }
;
}
