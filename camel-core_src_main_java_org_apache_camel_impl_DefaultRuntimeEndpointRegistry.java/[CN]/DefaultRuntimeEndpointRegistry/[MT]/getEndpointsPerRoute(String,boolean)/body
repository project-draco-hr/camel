{
  List<String> answer=new ArrayList<String>();
  if (includeInputs) {
    Set<String> uris=inputs.get(routeId);
    if (uris != null) {
      answer.addAll(uris);
    }
  }
  Map<String,String> uris=outputs.get(routeId);
  if (uris != null) {
    answer.addAll(uris.keySet());
  }
  return Collections.unmodifiableList(answer);
}
