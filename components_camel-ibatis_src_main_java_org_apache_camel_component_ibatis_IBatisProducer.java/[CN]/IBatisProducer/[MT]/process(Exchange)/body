{
switch (endpoint.getStatementType()) {
case QueryForObject:
    doQueryForObject(exchange);
  break;
case QueryForList:
doQueryForList(exchange);
break;
case Insert:
doInsert(exchange);
break;
case Update:
doUpdate(exchange);
break;
case Delete:
doDelete(exchange);
break;
case Default:
doDefault(exchange);
break;
default :
doDefault(exchange);
break;
}
}
