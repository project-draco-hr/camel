{
  File file=new File(name);
  if (!file.exists()) {
    return false;
  }
  boolean deleted=false;
  int count=0;
  while (!deleted && count < 3) {
    deleted=file.delete();
    if (count > 0) {
      try {
        Thread.sleep(1000);
      }
 catch (      InterruptedException e) {
      }
    }
    count++;
  }
  return deleted;
}
