{
  File file=new File(from);
  File target=new File(to);
  if (!file.exists()) {
    return false;
  }
  boolean renamed=false;
  int count=0;
  while (!renamed && count < 3) {
    renamed=file.renameTo(target);
    if (count > 0) {
      try {
        Thread.sleep(1000);
      }
 catch (      InterruptedException e) {
      }
    }
    count++;
  }
  return renamed;
}
