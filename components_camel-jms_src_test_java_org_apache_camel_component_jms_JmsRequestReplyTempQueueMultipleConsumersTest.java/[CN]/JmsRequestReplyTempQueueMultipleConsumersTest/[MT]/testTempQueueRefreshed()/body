{
  executorService=context.getExecutorServiceManager().newFixedThreadPool(this,"test",5);
  doSendMessages(100);
  connectionFactory.clear();
  Thread.sleep(1000);
  doSendMessages(100);
  connectionFactory.clear();
  Thread.sleep(1000);
  doSendMessages(100);
  context.getExecutorServiceManager().shutdown(executorService);
}
