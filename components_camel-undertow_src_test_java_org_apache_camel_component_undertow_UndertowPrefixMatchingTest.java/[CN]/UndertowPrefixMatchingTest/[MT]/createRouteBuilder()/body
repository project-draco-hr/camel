{
  return new RouteBuilder(){
    public void configure(){
      from("undertow:http://localhost:8888/myapp/suffix?matchOnUriPrefix=false").transform(bodyAs(String.class).append(" Must match exact path")).to("mock:myapp");
      from("undertow:http://localhost:8888/bar").transform(bodyAs(String.class).append(" Matching prefix")).to("mock:bar");
    }
  }
;
}
